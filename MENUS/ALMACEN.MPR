LPARAMETERS oFormRef, getMenuName, lUniquePopups, parm4, parm5, parm6, parm7, parm8, parm9
LOCAL cMenuName, nTotPops, a_menupops, cTypeParm2, cSaveFormName
IF TYPE("m.oFormRef") # "O" OR ;
  LOWER(m.oFormRef.BaseClass) # 'form' OR ;
  m.oFormRef.ShowWindow # 2
	MESSAGEBOX([This menu can only be called from a Top-Level form. Ensure that your form's ShowWindow property is set to 2. Read the header section of the menu's MPR file for more details.])
	RETURN
ENDIF
m.cTypeParm2 = TYPE("m.getMenuName")
m.cMenuName = SYS(2015)
m.cSaveFormName = m.oFormRef.Name
IF m.cTypeParm2 = "C" OR (m.cTypeParm2 = "L" AND m.getMenuName)
	m.oFormRef.Name = m.cMenuName
ENDIF
IF m.cTypeParm2 = "C" AND !EMPTY(m.getMenuName)
	m.cMenuName = m.getMenuName
ENDIF
DIMENSION a_menupops[6]
IF TYPE("m.lUniquePopups")="L" AND m.lUniquePopups
	FOR nTotPops = 1 TO ALEN(a_menupops)
		a_menupops[m.nTotPops]= SYS(2015)
	ENDFOR
ELSE
	a_menupops[1]="adminuistr"
	a_menupops[2]="movimiento"
	a_menupops[3]="reportes"
	a_menupops[4]="internos"
	a_menupops[5]="almacenes"
	a_menupops[6]="utilidades"
ENDIF


*       *********************************************************
*       *                                                         
*       *                      Menu Definition                    
*       *                                                         
*       *********************************************************
*

DEFINE MENU (m.cMenuName) IN (m.oFormRef.Name) BAR

DEFINE PAD _6o30wylyu OF (m.cMenuName) PROMPT "Administración" COLOR SCHEME 3 ;
	KEY ALT+A, ""
DEFINE PAD _6o30wylyv OF (m.cMenuName) PROMPT "Movimientos" COLOR SCHEME 3 ;
	KEY ALT+M, ""
DEFINE PAD _6o30wylyw OF (m.cMenuName) PROMPT "Reportes" COLOR SCHEME 3 ;
	KEY ALT+R, ""
DEFINE PAD _6o30wylyx OF (m.cMenuName) PROMPT "Utilidades" COLOR SCHEME 3 ;
	KEY ALT+U, ""
ON PAD _6o30wylyu OF (m.cMenuName) ACTIVATE POPUP (a_menupops[1])
ON PAD _6o30wylyv OF (m.cMenuName) ACTIVATE POPUP (a_menupops[2])
ON PAD _6o30wylyw OF (m.cMenuName) ACTIVATE POPUP (a_menupops[3])
ON PAD _6o30wylyx OF (m.cMenuName) ACTIVATE POPUP (a_menupops[6])

DEFINE POPUP (a_menupops[1]) MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF (a_menupops[1]) PROMPT "P\<roveedores" ;
	PICTURE "..\graphics\emply.ico"
ON SELECTION BAR 1 OF (a_menupops[1]) goapp.form("ar_proveedores")

DEFINE POPUP (a_menupops[2]) MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF (a_menupops[2]) PROMPT "Traspasos Entre Almacenes Salidas.." ;
	PICTURE "..\graphics\ventas.ico"
ON SELECTION BAR 1 OF (a_menupops[2]) goapp.form("ka_traspasos","V")

DEFINE POPUP (a_menupops[3]) MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF (a_menupops[3]) PROMPT "Internos"
DEFINE BAR 2 OF (a_menupops[3]) PROMPT "\-"
DEFINE BAR 3 OF (a_menupops[3]) PROMPT "Almacenes"
ON BAR 1 OF (a_menupops[3]) ACTIVATE POPUP (a_menupops[4])
ON BAR 3 OF (a_menupops[3]) ACTIVATE POPUP (a_menupops[5])

DEFINE POPUP (a_menupops[4]) MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF (a_menupops[4]) PROMPT "Lista de Precios" ;
	KEY F12, "F12" ;
	SKIP FOR wexist("forlistaprecios")
ON SELECTION BAR 1 OF (a_menupops[4]) goapp.form("ka_lprecios")

DEFINE POPUP (a_menupops[5]) MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF (a_menupops[5]) PROMPT "Inventario Por Almacen" ;
	SKIP FOR wexist("forinventarios")
DEFINE BAR 2 OF (a_menupops[5]) PROMPT "Kardex Por Almacen" ;
	SKIP FOR wexist("forcalmacen")
DEFINE BAR 3 OF (a_menupops[5]) PROMPT "Kardex Resumido" ;
	SKIP FOR wexist("forresumen")
DEFINE BAR 4 OF (a_menupops[5]) PROMPT "\-"
DEFINE BAR 5 OF (a_menupops[5]) PROMPT "Traspasos Entre Almacenes"
ON SELECTION BAR 1 OF (a_menupops[5]) goapp.form("ka_cinventarios")
ON SELECTION BAR 2 OF (a_menupops[5]) goapp.form("ka_calmacen",0)
ON SELECTION BAR 3 OF (a_menupops[5]) goapp.form("ka_resumido")
ON SELECTION BAR 5 OF (a_menupops[5]) goapp.form("ka_rretraspasos")

DEFINE POPUP (a_menupops[6]) MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF (a_menupops[6]) PROMPT "Calcular Stock" ;
	PICTURE "..\graphics\anular.ico"
ON SELECTION BAR 1 OF (a_menupops[6]) ;
	DO _6o30wylyy ;
	IN LOCFILE("\XSYSG\MENUS\ALMACEN" ,"MPX;MPR|FXP;PRG" ,"WHERE is ALMACEN?")

ACTIVATE MENU (m.cMenuName) NOWAIT

IF m.cTypeParm2 = "C"
	m.getMenuName = m.cMenuName
	m.oFormRef.Name = m.cSaveFormName 
ENDIF


