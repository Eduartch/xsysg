	PROCEDURE addchartproperties
		Lparameters lcProperties, lcObjectName
		
		Local laProperties(1), lcProperty
		Alines (laProperties, lcProperties, 5, ',')
		
		For Each lcProperty In laProperties
		
			If Empty (lcObjectName)
				loObject = Thisform.FoxCharts1
				lcName = lcProperty
			Else
				loObject = Thisform.FoxCharts1.&lcObjectName	
				lcName = lcObjectName + "." + lcProperty
			Endif
		
			If Pemstatus (loObject, lcProperty, 5)
				lxValue = loObject.&lcProperty
				lcType	= Vartype (lxValue)
		
				Do Case
		
					Case lcType = 'C'
						Insert Into PropertiesList (Name, Type, CStart) Values (lcName, lcType, lxValue)
		
					Case lcType = 'N'
						Insert Into PropertiesList (Name, Type, NStart) Values (lcName, lcType, lxValue)
		
					Case lcType = 'L'
						Insert Into PropertiesList (Name, Type, LStart) Values (lcName, lcType, lxValue)
		
				Endcase
		
			Endif
		
		Endfor
		
	ENDPROC

	PROCEDURE drawchart
		Debugout Datetime()
		
		
		If This.DrawChartEnabled
			Thisform.FoxCharts1.DrawChart()
		Endif
		
	ENDPROC

	PROCEDURE updatelegends
		With This.P2 As Page
		
			Local loObject As Object
			loObject = .SelObject
		
			.TxtCaption.Value 			= loObject.Caption
			.TxtFormat.Value 			= loObject.Format
		
			.CmdFont.Caption 			= loObject.FontName
			.CmdFont.FontName 		= loObject.FontName
			.CmdFont.FontSize 			= loObject.FontSize
			.CmdFont.FontBold 			= loObject.FontBold
			.CmdFont.FontItalic 			= loObject.FontItalic
			.CmdFont.FontUnderline 		= loObject.FontUnderline
			
			If Pemstatus(loObject,[FontCharSet], 5)
				.CmdFont.FontCharSet 	= loObject.FontCharSet
			Endif
		
			.SpnSize.Value 				= loObject.FontSize
		
			.Shape1.BackColor 			= loObject.ForeColor
			.SpnForeColorAlpha.Value 	= loObject.ForeColorAlpha
		
			.Shape2.BackColor 			= loObject.BackColor
			.SpnBackColorAlpha.Value 	= loObject.BackColorAlpha
		
			.chkBold.Value 				= loObject.FontBold
			.chkItalic.Value 				= loObject.FontItalic
			.chkUnderline.Value 			= loObject.FontUnderline
		
			.CmbAlignment.Value 		= loObject.Alignment + 1
		
			.SpnRotation.Value = loObject.Rotation
		
		Endwith
		
	ENDPROC

	PROCEDURE updateselectedlegend
		Local lnValue, loObject As Label
		
		With This.P2
		
			lnValue = .Optiongroup1.Value
		
			Do Case
				Case lnValue = 1
					loObject = Thisform.Foxcharts1.Title
		
				Case lnValue = 2
					loObject = Thisform.Foxcharts1.SubTitle
		
				Case lnValue = 3
					loObject = Thisform.Foxcharts1.XAxis
		
				Case lnValue = 4
					loObject = Thisform.Foxcharts1.AxisLegend2
		
				Case lnValue = 5
					loObject = Thisform.Foxcharts1.YAxis
		
				Case lnValue = 6
					loObject = Thisform.Foxcharts1.ShapeLegend
		
				Case lnValue = 7
					loObject = Thisform.Foxcharts1.ScaleLegend
		
				Case lnValue = 8
					loObject = Thisform.Foxcharts1.SideLegend
		
			Endcase
		
			.AddProperty ("SelObject", loObject)
		
		Endwith
		
	ENDPROC

