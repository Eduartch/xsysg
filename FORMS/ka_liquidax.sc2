		TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
		 lcaj_dcto AS dcto,lcaj_deud AS Importe,lcaj_btar AS banco,lcaj_ttar AS tipo,lcaj_rtar AS referencia,u.nomb AS Cajero
		 FROM fe_lcaja AS l INNER JOIN fe_usua AS u ON u.idusua=lcaj_idus
		 WHERE lcaj_fech='<<df>>' AND lcaj_form='T' AND lcaj_acti='A' AND lcaj_idau>0 ORDER BY u.nomb,lcaj_dcto
		ENDTEXT
		If Ejecutaconsulta(lc,'ta')<=0 Then
			Return
		Endif
		Report Form infvtastarjetag To Printer Prompt Noconsole
		
	ENDPROC

	PROCEDURE Cmdimprimir.Click
