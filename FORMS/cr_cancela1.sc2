					.vendedor=apagar.idven
					.txtnrounico.Value=apagar.nrou
					.nauto=apagar.rcre_idau
					.txtdscto.Value=apagar.rcre_idrc
					.txtidval.Value=0
					.txtidalma.Value="Tienda"
					.cmbforma.Enabled= .T.
					ct="Tienda"
					nidauto=apagar.rcre_idau
					If nidauto>0 Then
						TEXT TO lc NOSHOW
		                   SELECT a.nomb,b.tdoc FROM fe_rcom as b JOIN fe_tdoc as a ON (a.tdoc=b.tdoc) WHERE idauto=?nidauto
						ENDTEXT
						IF verificaconexion()=0 then
		RETURN 0
		endif
						If SQLExec(goapp.bdconn,lc,"ld")<1
							errorbd(lc)
							Return
						Endif
						.txttdcto.Value=Alltrim(ld.tdoc)+''+Alltrim(ld.nomb)
					Endif
				Endif
				.letras="S"
				.cmbforma.SetFocus
			Else
				.letras="N"
				Select ndoc,fech,fevto,dias,rcre_impc,saldo,pagos,nomv,ncontrol,situa,rcre_idau,tipo,docd,dscto,mone,rcre_idrc,tdoc From tmp Where pagos>0 Into Cursor txp
