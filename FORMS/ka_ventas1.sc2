	PROCEDURE Cmdaceptar.Click
		Local rpta,cdocumento,cdcto1
		Thisform.grivta.suma()
		If !Thisform.validar() Then
			Messagebox(Thisform.mensaje,32,MSGTITULO)
			Return
		Endif
		Thisform.nacta=0
		If Thisform.cmbforma.ListIndex=1
			Do Form ka_efectivo With Thisform.tdoc,Thisform.txtserie.Value+Thisform.txtnumero.Value,Thisform.txttotal.Value
		Endif
		cdcto1=Alltrim(Thisform.cmbdcto.Value)+' '+Thisform.txtserie.Value+Thisform.txtnumero.Value
		Thisform.Cajas=''
		With Thisform
			rpta=Messagebox("¿Los Datos de la Venta Son Correctos <Si/No>? "+cdcto1,48+3+0,MSGTITULO)
			Do Case
			Case rpta=7
				.nuevo()
				Return
			Case rpta=2
				.grivta.SetFocus
				Return
			Endcase
			swcr=0
			rpta=0
			rpta=Messagebox("¿Desea Imprimir Documento <Si/No>? "+cdcto1,48+3,MSGTITULO)
			If rpta=2
				.grivta.SetFocus
				Return
			Endif
			Set Procedure To capadatos,rngrifo Additive
			If IniciaTransaccion()=0 Then
				Return
			Endif
			.nauto=IngresaResumenDcto(.tdoc,Left(.cmbforma.Value,1),;
				.txtserie.Value+.txtnumero.Value,.txtfecha.Value,.txtfecha.Value,"",.txtvalor.Value,;
				.txtigv.Value,.txttotal.Value,.txtguia.Value,Left(.cmbmoneda.Value,1),;
				.txtdolar.Value,config.igv,'k',.txtcodigo.Value,'V',goapp.nidusua,1,goapp.tienda,fe_gene.idctav,fe_gene1.idctai,fe_gene.idcta,.txtitem.Value,0)
			If .nauto=0
				DeshacerCambios()
				Return
			Endif
			nidcon=RetConcepto(.tdoc+Left(.cmbforma.Value,1),'I')
			If nidcon>0 Then
				If IngresaCaja(.nauto,.txtfecha.Value,.txttotal.Value,'I',Left(.cmbforma.Value,1),Left(.cmbmoneda.Value,1),;
						.txtserie.Value+.txtnumero.Value,nidcon,goapp.nidusua,"Vta:"+.txtrazon.Value,'CK',.txttotal.Value,'S',2.85,goapp.tienda,.Cajas,0,0)<=0 Then
					DeshacerCambios()
					Return
				Endif
			Endif
			If .cmbforma.ListIndex=2 Or .cmbforma.ListIndex=3
				.ingresacreditos()
			Endif
			If .swcreditos=0
				DeshacerCambios()
				Return
			Endif
			na=.nauto
			If .cmbforma.ListIndex=1 Then
				If IngresaRvendedores(na,.txtcodigo.Value,.cvendedor,Left(.cmbforma.Value,1))=0 Then
					DeshacerCambios()
					Return
				Endif
			Endif
			sw=1
			Select tmpv
			Set Filter To coda<>0
			Go Top
			Do While !Eof()
				If MuestraStocksN(tmpv.coda,'ds')=0 Then
					sw=0
					Exit
				Endif
				If fe_gene.alma_nega=0 Then
					If tmpv.cant>ds.saldo Then
						sw=0
						Exit
					Endif
				Endif
				If .grabar(na)=0
					sw=0
					Exit
				Endif
				Select tmpv
				Skip
			Enddo
			If sw=0 Then
				DeshacerCambios()
				Messagebox("El Item:"+Alltrim(tmpv.Desc)+" No Tiene Stock Disponible Para Venta",16,MSGTITULO)
				Return
			Endif
			If .swv=1 And .swc=1 And .sws=1 And .swk=1 And .swcreditos=1 And Thisform.generanumero()=1
				GrabarCambios()
				If rpta=6
					.importe=Diletras(.txttotal.Value,'S')
					.mes=RetornaMes(.txtfecha.Value)
					Select Count(*) As ti From tmpv Into Cursor items
					ni=items.ti
					Select tmpv
					Set Filter To
					Replace All ndoc With .txtserie.Value+.txtnumero.Value,cletras With .importe
					Go Top
					For x=1 To .Item-.txtitem.Value
						ni=ni+1
						Insert Into tmpv(ndoc,nitem,cletras)Values(.txtserie.Value+.txtnumero.Value,ni,.importe)
					Next
					Go Top In tmpv
					Report Form (goapp.reporte) To Printer Prompt Noconsole
				Endif
				If Isdigit(.txtguia.Value)
					If Messagebox("¿Desea Hacer Guia de Remisión Para Este Documento?",48+4+0,MSGTITULO)=6
						Do Form ka_guiasventas With .nauto,.txtguia.Value
					Endif
				Endif
			Endif
			.nuevo()
		Endwith
		
	ENDPROC

	PROCEDURE Cmdanular.Click
		Local rpta,ccodigo
		With Thisform
			cdocumento=.txtserie.Value+.txtnumero.Value
			If Empty(cdocumento) Or Len(Alltrim(cdocumento))<10
				Messagebox("Se debe Ingresar Serie Y Número Para Poder Anularlo",48,MSGTITULO)
				.txtserie.SetFocus
				Return
			Endif
			Do Form v_verifica With "A" To verdad
			If !verdad
				.txtnumero.SetFocus
				Return
			Endif
			rpta=Messagebox("¿Esta Seguro de Anular este Documento? "+Rtrim(.cmbdcto.Value)+cdocumento,48+3+0,MSGTITULO)
			Do Case
			Case rpta=2
				.txtserie.SetFocus
				Return
			Case rpta=6
				ctipo="V"
				csw=""
				If Thisform.txtencontrado.Value='V' Then
					nauto=Thisform.cdcto
					cdcto=""
					ctdoc=""
		    		cu=thisform.idusuav
				Else
					nauto=0
					ctdoc=.tdoc
					cdcto=cdocumento
		     		cu=goapp.nidusua
				Endif
				Set Procedure To capadatos,rngrifo ADDITIVE 
				If PermiteAnularVentaN(nauto,Thisform.txtfecha.Value,thisform.tdoc)=0  Or PermiteIngresox(thisform.txtfecha.Value)=0 Then
					Return
				Endif
				If Messagebox("¿Desea Guardar Como Anulada El Documento?",48+4+0,MSGTITULO)=6
					csw="S"
				Endif
				dfecha=Thisform.txtfecha.Value
				ctipo='V'
				cu1=goapp.uauto
				If AnulaTransaccionN(ctdoc,cdocumento,ctipo,nauto,cu,csw,dfecha,goapp.uauto,0)=0 Then
			 	    RETURN 
				Endif
		    	goapp.uauto=0
				.nuevo
			Case rpta=7
				.nuevo
				Return
			Endcase
		Endwith
		
	ENDPROC

