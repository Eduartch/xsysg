	PROCEDURE Cmdconsulta.Click
		Local dfecha,cmone
		With Thisform
			cmone=Left(.cmbmoneda.Value,1)
			.txtpagoss.Value=0
			.txttotals.Value=0
			.txtingresoscs.Value=0
			.txtingresoss.Value=0
			.txtegresoss.Value=0
			.txtsaldos.Value=0
			.txtpagosd.Value=0
			.txttoingresosd.Value=0
			.txttoingresoss.Value=0
			.txttotald.Value=0
			.txtttarjeta.Value=0
			.txtingresoscd.Value=0
			.txttvbrutas.Value=0
			.txtingresosd.Value=0
			.txtegresosd.Value=0
			.txtsaldod.Value=0
			.txtsincajas.Value=0
			.txtsaldoa.Value=0
			.Ingresos=0
			.txttdsctos.Value=0
			.egresos=0
			.grimodelos1.RecordSource=""
			nidusuario=lu.idusua
			dfi=this.Parent.txtfechainicial.Value 
			dff=this.Parent.txtfechafinal.Value 
			cusuario=.cusuario
			nisla=This.Parent.cmbisla.ListIndex
			nturno=This.Parent.cmbturno.ListIndex
			If This.Parent.chkisla.Value=0 Then
				TEXT TO lc noshow
			        SELECT ndoc,TRIM(c.razo) AS cliente,TRIM(r.deta) AS deta,fech AS fechadcto,lcaj_fech AS fechaturno,
			        lcaj_form,lcaj_deud as Importe,lcaj_dsct as dscto,u.nomb AS usuario,lcaj_fope,lcaj_idus,lcaj_idtu,lcaj_form as form
					FROM fe_lcaja AS l INNER JOIN fe_rcom AS r ON r.idauto=l.lcaj_idau
					INNER JOIN fe_clie AS c ON c.idclie=r.idcliente
					INNER JOIN fe_usua AS u ON u.`idusua`=l.lcaj_idus  WHERE lcaj_fech between ?dfi and ?dff AND lcaj_acti='A' AND r.acti='A' ORDER BY lcaj_ndoc,lcaj_fech
				ENDTEXT
			Else
		
				TEXT TO lc noshow
			        SELECT ndoc,TRIM(c.razo) AS cliente,TRIM(r.deta) AS deta,fech AS fechadcto,lcaj_fech AS fechaturno,
			        lcaj_form,lcaj_deud as importe,lcaj_dsct as dscto,u.nomb AS usuario,lcaj_fope,lcaj_idus,lcaj_idtu,lcaj_form as form
					FROM fe_lcaja AS l INNER JOIN fe_rcom AS r ON r.idauto=l.lcaj_idau
					INNER JOIN fe_clie AS c ON c.idclie=r.idcliente
					INNER JOIN fe_usua AS u ON u.`idusua`=l.lcaj_idus  WHERE lcaj_fech between ?dfi and ?dff AND lcaj_acti='A' AND r.acti='A' and lcaj_codt=?nisla ORDER BY lcaj_ndoc,lcaj_fech
				ENDTEXT
		
			ENDIF
				IF verificaconexion()=0 then
		RETURN 0
		endif
			ncon=Abreconexion()
			If SQLExec(ncon,lc,"icaja")<1
				errorbd(lc)
				Return
			Endif
			CierraConexion(ncon)
			If This.Parent.chkturno.Value=1 Then
				Select * From  icaja Where lcaj_idtu=nturno Into Cursor icaja
			Endif
			If This.Parent.chkcajero.Value=1
				Select * From  icaja Where lcaj_idus=nidusuario Into Cursor icaja
			Endif
			If This.Parent.check1.Value=1
				Select * From  icaja Where dscto>0 Into Cursor icaja
			Endif
			Thisform.isla=nisla
			Thisform.fecha=dfecha
			Select Sum(importe) As tventas From icaja Into Cursor tt
			This.Parent.txttotals.Value=tt.tventas
		
			Select Sum(dscto) As tventas From icaja Into Cursor tt
			This.Parent.txttdsctos.Value=tt.tventas
			This.Parent.txttvbrutas.Value=Round(This.Parent.txttotals.Value+This.Parent.txttdsctos.Value,2)
		
		
			Select Sum(importe) As tventas From icaja Where Form='T'  Into Cursor tt
			This.Parent.txtttarjeta.Value=tt.tventas
		
			Thisform.turno=nturno
			Thisform.cajero=This.Parent.cmbalmacen.Value
			Thisform.idusuario=nidusuario
		
			r=0
		
		******************
			Select icaja
			Go Top
			.grimodelos1.RecordSource="icaja"
			.grimodelos1.SetFocus
		Endwith
		
	ENDPROC

