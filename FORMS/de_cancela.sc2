	PROCEDURE validar
		With Thisform
			Do Case
			Case !Empty(.txtdocumento.Value)
		* IF .txtpago.value>.txtimporte.value
		*     thisform.mensaje="El Pago Es Mayor Al Importe del Documento A Cancelar"
		*     RETURN .f.
		* ENDIF
			Case Thisform.cmbforma.ListIndex=2 And Thisform.txtamortiza.Value=0
				Thisform.mensaje="Ingrese Monto a Amortizar"
				Return .F.
			Case .txtpago.Value=0
				Thisform.mensaje="Importes No Válidos"
				Return .F.
			Case Thisform.txtdolar.Value=0
				Thisform.mensaje="Ingrese El Tipo de Cambio"
				Return .F.
			Otherwise
				Return .T.
			Endcase
		Endwith
		
	ENDPROC

	PROCEDURE cmbforma.InteractiveChange
		if this.listindex=1
		   thisform.txtamortiza.readonly=.t.
		  else
		   thisform.txtamortiza.readonly=.f.
		endif   
		   
	ENDPROC

	PROCEDURE cmbforma.LostFocus
		With Thisform
			If This.ListIndex=1
				.txtamortiza.ReadOnly=.T.
				.txtdias.ReadOnly=.T.
				.txtfechavt.ReadOnly=.T.
				.txtdias.Value=0
				.txtamortiza.Value=0
				.txtnsaldo.Value=0
			Else
				.txtamortiza.ReadOnly=.F.
				.txtamortiza.SetFocus
				.txtdias.ReadOnly=.F.
				.txtfechavt.ReadOnly=.F.
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE Cmdaceptar.Click
		Local ximporte,xfevto,xestado,rpta,ri
		With Thisform
			If !Thisform.validar()
				Messagebox(Thisform.mensaje,16,MSGTITULO)
				Return
			Endif
			rpta=preguntaGuardar()
			Do Case
			Case rpta=2
				.txtpago.SetFocus
				Return
			Case rpta=6
				If Thisform.viene='S'
					Thisform.cmdcerraro.Click()
					Return
				Endif
				nidclie=.proveedor
				cndoc=.txtnumero.Value
				dfech=.txtfecha.Value
				ndola=.txtdolar.Value
				sw=1
				cforma=Left(.cmbcaja.Value,1)
				nidrd=0
				If IniciaTRansacCion()=0 Then
					DeshacerCambios()
					Return
				Endif
				If .txtpago.ReadOnly= .T.
					Select tmp
					sw=1
					Scan For pagos>0
						nidrd=CancelaDeudas(dfech,tmp.fevto,tmp.pagos,cndoc,'P',tmp.moneda,.txtdetalle.Value,tmp.tipo,tmp.idrd,goapp.nidusua,tmp.ncontrol,'',Id(),ndola)
						If nidrd=0 Then
							sw=0
							Exit
						Endif
						nmp=Iif(tmp.moneda='D',Round(tmp.pagos*ndola,2),tmp.pagos)
						If cforma='E'  AND goapp.xopcion=2 Then
							If IngresaDatosLcajaEe(dfech,"","Pago "+.txtcliente.Value,fe_gene.gene_idpge,;
									0,nmp,'S',fe_gene.dola,goapp.nidusua,nidrd)=0 Then
								sw=0
								Exit
							Endif
						Endif
					Endscan
					If sw=0
						DeshacerCambios()
						Return
					Endif
				Else
					dfevto=Iif(.cmbforma.ListIndex=1,.txtfechavt.Value,.txtfechavto.Value)
					nmontop=.txtpago.Value-.txtinteres.Value
					nidrd=CancelaDeudasCi(dfech,dfevto,nmontop,cndoc,'P',tmp.moneda,.txtdetalle.Value,tmp.tipo,tmp.idrd,;
						goapp.nidusua,.txtcontrol.Value,.txtnrounico.Value,Id(),ndola,.txtinteres.Value)
					If nidrd=0 Then
						sw=0
						Exit
					Endif
					nmp=Iif(tmp.moneda='D',Round(.txtpago.Value*ndola,2),.txtpago.Value)
					If cforma='E'  Then
						If IngresaDatosLcajaEe(dfech,"","Pago "+.txtcliente.Value,fe_gene.gene_idpge,;
								0,nmp,'S',fe_gene.dola,goapp.nidusua,nidrd)=0 Then
							sw=0
							Exit
						Endif
					Endif
				Endif
				If sw=0 Then
					DeshacerCambios()
					return
				Else
					GrabarCambios()
				Endif
				.cmdcancelar.Click()
			Case rpta=7
				.cmdcancelar.Click()
			Endcase
		Endwith
		
	ENDPROC

