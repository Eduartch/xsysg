	PROCEDURE Pgf1.Page2.Cmdgenerar1.Click
		Thisform.pgf1.page2.grimodelos1.RecordSource=""
		df=Thisform.pgf1.page2.txtfecha.Value
		nidt=Thisform.pgf1.page2.cmbturno.ListIndex
		nisla=Thisform.pgf1.page2.cmbisla.ListIndex
		Thisform.consultar('listacf',nisla,nidt)
		Do Case
		Case nisla=1
			TEXT TO lc NOSHOW
			SELECT lect_fope AS fechadespacho,lect_inic AS lectura_galon,descri AS producto,lect_mang AS manguera,lect_idco AS surtidor,
			lect_idar AS codigo FROM fe_lecturas AS l INNER JOIN fe_art AS a ON a.idart=l.lect_idar WHERE lect_acti='A' and lect_idco in(1,2)
			ENDTEXT
		Case nisla=2
			TEXT TO lc NOSHOW
			SELECT lect_fope AS fechadespacho,lect_inic AS lectura_galon,descri AS producto,lect_mang AS manguera,lect_idco AS surtidor,
			lect_idar AS codigo FROM fe_lecturas AS l INNER JOIN fe_art AS a ON a.idart=l.lect_idar WHERE lect_acti='A' and lect_idco in(3,4)
			ENDTEXT
		Case nisla=3
			TEXT TO lc NOSHOW
			SELECT lect_fope AS fechadespacho,lect_inic AS lectura_galon,descri AS producto,lect_mang AS manguera,lect_idco AS surtidor,
			lect_idar AS codigo FROM fe_lecturas AS l INNER JOIN fe_art AS a ON a.idart=l.lect_idar WHERE lect_acti='A' and lect_idco in(5,6)
			ENDTEXT
		Case nisla=4
			TEXT TO lc NOSHOW
			SELECT lect_fope AS fechadespacho,lect_inic AS lectura_galon,descri AS producto,lect_mang AS manguera,lect_idco AS surtidor,
			lect_idar AS codigo FROM fe_lecturas AS l INNER JOIN fe_art AS a ON a.idart=l.lect_idar WHERE lect_acti='A' and lect_idco in(7,8)
			ENDTEXT
		Endcase
			IF verificaconexion()=0 then
		RETURN 0
		endif
		If SQLExec(goapp.bdconn,lc,'iniciales')<0 Then
			errorbd(lc)
			Return
		Endif
		*SELECT fecha,SUM(final),SUM(inicial),SUM(final)-SUM(inicial) as ventas,producto,manguera,surtidor,codigo from;
		(Select fechadespacho As fecha,lectura_galon As inicial,000000000000.00 As Final,producto,manguera,surtidor,codigo From iniciales;
		UNION All;
		SELECT fechadespacho As fecha,000000000000.00 As inicial,lectura_galon As Final,producto,manguera,surtidor,codigo;
		from listacf) as lista GROUP BY codigo,surtidor,manguera INTO CURSOR liq
		*Select listacf
		*SCAN all
		
		* nmonto=monedadecimal(listacf.lectura_galon)
		* REPLACE
		
		*ENDSCAN
		Select fechadespacho,Mton(lectura_galon) As Final,producto,manguera,surtidor,codigo From listacf Into Cursor listacf
		*Replace All lectura_galon With Mton(lectura_galon)
		*SELECT listacf
		*Go Top
		*Wait Window Vartype(listacf.final)
		*Select Sum(Final),Sum(inicial),Sum(Final)-Sum(inicial) As ventas,producto,manguera,surtidor,codigo From;
		(Select fechadespacho As fecha,lectura_galon As inicial,000000000000.00 As Final,producto,manguera,surtidor,codigo From iniciales;
		UNION All;
		SELECT fechadespacho As fecha,000000000000.00 As inicial,lectura_galon As Final,producto,manguera,surtidor,codigo;
		from listacf) As lista Group By codigo,surtidor,manguera Into Cursor liq
		
		
		SELECT fecha,SUM(final) as final,SUM(inicial) as inicial,SUM(final)-SUM(inicial) as ventas,producto,manguera,surtidor,codigo from;
		(Select fechadespacho As fecha,lectura_galon As inicial,000000000000.00 As Final,producto,manguera,surtidor,codigo From iniciales;
		UNION All;
		SELECT fechadespacho As fecha,000000000000.00 As inicial,Final,producto,manguera,surtidor,codigo;
		from listacf) as lista GROUP BY codigo,surtidor,manguera INTO CURSOR liq ORDER BY  surtidor,manguera
		
		
		
		
		
		Thisform.pgf1.page2.grimodelos1.RecordSource="liq"
		Thisform.pgf1.page2.grimodelos1.ocutarcolumnas()
		Thisform.pgf1.page2.grimodelos1.colocartituloscolumnas()
		
