	PROCEDURE Pgf1.Page1.Cmdaceptar1.Click
		
		Set Procedure To capadatos,rngrifo,ple5 Additive
		If !Thisform.validar() Then
			Messagebox(Thisform.mensaje,16,MSGTITULO)
			Return
		Endif
		If preguntaGuardar()=6 Then
			q=1
			If IniciaTransaccion()=0 Then
				DeshacerCambios()
				Return
			Endif
			Select listaci
			Scan All
				If IngresalecturasContometros20(listaci.surtidor,fe_gene.alma_sepa,listaci.lectura_galon,0,;
						fe_gene.fech,goapp.nidusua,listaci.codigo,listaci.manguera,listaci.precio)=0 Then
					q=0
					Exit
		     	Endif
			Endscan
			If q=1 Then
				GrabarCambios()
				mensaje("Grabado OK")
				Thisform.limpiar()
			Else
				DeshacerCambios()
				Return
			Endif
		Endif
		
	ENDPROC

