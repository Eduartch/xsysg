				this.cmdconsulta.Click()
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE mycolor
		Lparameters tienda
		*lnColor = 0xFFFFFF && Blanco por default
		Do Case
		Case tienda="UNO"
			lnColor = Rgb(0,255,0)
		Case tienda="DOS"
			lnColor = Rgb(255,255,0)
		Case tienda="TRE"
			lnColor = Rgb(255,100,0)
		Endcase
		Return lnColor
		
	ENDPROC

