	PROCEDURE validar
		Do Case
		Case Len(Alltrim(Thisform.pagef1.page2.txtdescripcion.Value))=0
			Thisform.mensaje="Ingrese Un Nombre para esta Promoción"
			Return .F.
		Case Thisform.pagef1.page2.txtcantidad.Value=0
			Thisform.mensaje="Ingrese Una Cantidad  Válida para esta Promoción"
			Return .F.
		Case !esfechaValida(Thisform.pagef1.page2.txtfechainicial.Value) Or  !esfechaValida(Thisform.pagef1.page2.txtfechafinal.Value)
			Thisform.mensaje="Las fechas de la promoción no son Válidas"
			Return .F.
		Case verificaAlias("xmm1")=0
			Thisform.mensaje="Seleccione Los Productos que incluye la Promoción"
			Return .F.
		Case cpr.cant<=0
			Thisform.mensaje="Seleccione Los Items a Ofertar"
			Return .F.
		Otherwise
			Return .T.
		Endcase
		
	ENDPROC

	PROCEDURE validar1
		Do Case
		Case Len(Alltrim(Thisform.pagef1.page3.txtdescripcion.Value))=0
			Thisform.mensaje="Ingrese Un Nombre para esta Promoción"
			Return .F.
		Case Thisform.pagef1.page3.txtmonto.Value=0
			Thisform.mensaje="Ingrese Un Monto Válido para esta Promoción"
			Return .F.
		Case !esfechaValida(Thisform.pagef1.page3.txtfechainicial.Value) Or  !esfechaValida(Thisform.pagef1.page3.txtfechafinal.Value)
			Thisform.mensaje="Las fechas no son Válidas"
			Return .F.
		Case verificaAlias("xmm")=0
			Thisform.mensaje="Seleccione Los Productos que incluye la Promoción"
			Return .F.
		Case cpr2.cant<=0
			Thisform.mensaje="Seleccione Los Items a Ofertar"
			Return .F.
		Otherwise
			Return .T.
		Endcase
		
	ENDPROC

