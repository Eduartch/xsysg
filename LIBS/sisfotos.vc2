	PROCEDURE AfterRowColChange
		Lparameters nColIndex
		Thisform.gridstocks.RecordSource=""
		Set Procedure To capadatos,rngrifo Additive
		If MuestraStocksN(lproductos.idart,'lc')=0 Then
			Return
		Endif
		Zap In lstock
		Insert Into lstock(calma,stock)Values("Contable",lc.saldo)
		Insert Into lstock(calma,stock)Values("Interno",lproductos.uno+lproductos.Dos+lproductos.tre+lproductos.cua)
		Go Top In lstock
		Thisform.gridstocks.RecordSource="lstock"
		
	ENDPROC

