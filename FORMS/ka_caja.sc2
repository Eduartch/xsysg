	PROCEDURE validar		&& validar los datos a grabar
		npagina=Thisform.pgfmvtos.ActivePage
		Do Case
		Case npagina=1
			df=Thisform.pgfmvtos.pgingresos.txtfecha.Value
		Case npagina=2
			df=Thisform.pgfmvtos.pggastos1.txtfecha.Value
		Case npagina=3
			df=Thisform.pgfmvtos.pgt.txtfecha.Value
		Endcase
		x=validacaja1(df,goapp.nidusua)
		If x="C"
			Messagebox("La caja de Esta Fecha Esta Cerrada",16,MSGTITULO)
			Return .F.
		Endif
		Do Case
		Case npagina=1
			If Thisform.pgfmvtos.pgingresos.txtimporte.Value=0 OR EMPTY(this.pgfmvtos.pgingresos.txtdetalle.Value)
				Messagebox("Datos No Válidos",48,MSGTITULO)
				Return .F.
			Endif
		Case npagina=2
			*If Thisform.nidauto=0 Then
			*	Messagebox("Tiene Que Seleccionar un Documento Pendiente de Pago  para Poder Registrarlo",48,MSGTITULO)
			*	Return .F.
			*Endif
			If Thisform.pgfmvtos.pggastos1.txtimporte.Value=0 OR EMPTY(this.pgfmvtos.pggastos1.txtdetalle.Value)
				Messagebox("Datos No Válidos",48,MSGTITULO)
				Return .F.
			Endif
		Case npagina=3
			Do Case
			*Case Thisform.pgfmvtos.pgt.txttpedidos.Value>0 And Thisform.pgfmvtos.pgt.txtimporte.Value<Thisform.pgfmvtos.pgt.txtsaldo.Value
			*	Messagebox("El saldo A Transferir debe de Ser el Total porque hay Ventas Con Pedidos de Venta",48,MSGTITULO)
			*	Return .F.
			Case  Thisform.pgfmvtos.pgt.txtimporte.Value=0;
					OR Thisform.pgfmvtos.pgt.txtimporte.Value>Thisform.pgfmvtos.pgt.txtsaldo.Value Or;
					thisform.pgfmvtos.pgt.txtsaldo.Value=0
				Messagebox("Datos No Válidos Los Importes no Pueden ser 0 o el importe a transferir es mayor al saldo de Caja",48,MSGTITULO)
				Return .F.
			Case goapp.nidusua=lusuat.idusua
				Messagebox("No se Puede Transferir al Mismo Cajero",48,MSGTITULO)
				Return .F.
		    CASE YEAR(this.pgfmvtos.pgt.txtfecha.Value)<>VAL(goapp.año) OR ;
		         !esfechavalida(this.pgfmvtos.pgt.txtfecha.Value)
		         	Messagebox("Ingrese Fecha Válida",48,MSGTITULO)
				Return .F.
			Endcase
		Endcase
		
	ENDPROC

