	PROCEDURE txtfecha.GotFocus
		If Thisform.txtnumerog.Value="0000000" Or Len(Alltrim(Thisform.txtnumerog.Value))<7 Then
			Thisform.txtnumerog.SetFocus
			Return
		Endif
		
	ENDPROC

	PROCEDURE txtnumerog.GotFocus
		If Thisform.txtserieg.Value="000" Or Empty(Thisform.txtserieg.Value) Then
			Thisform.txtserieg.SetFocus
			Return
		Endif
		
	ENDPROC

	PROCEDURE txtnumerog.LostFocus
		This.Value=Right("0000000"+Alltrim(This.Value),7)
		If VerificaSiguiaVtaEstaIngresada(Thisform.txtserieg.Value+Thisform.txtnumerog.Value)=0 Then
			Messagebox("Este No de Guia ya Esta Registrada",16,MSGTITULO)
			This.Value=""
			Thisform.txtserieg.SetFocus
			Return
		Endif
		
	ENDPROC

	PROCEDURE txtplaca.LostFocus
		With Thisform
			If !Empty(This.Value)
				cplaca=.txtplaca.Value
				
					IF verificaconexion()=0 then
		RETURN 0
		endif
				If SQLExec(goapp.bdconn,"CALL PROMUESTRATRANSPORTISTA(?cplaca,0)","fe_tra")<1
					errorbd(ERRORPROC)
					Return
				ENDIF
		   		If REGDVTO("fe_tra")>0 Then
					.verdad=.T.
					.ad(0)
					.idtran=fe_tra.idtra
					.txtruct.Value=fe_tra.ructr
					.txtrazont.Value=fe_tra.razon
					.txtdirecciont.Value=fe_tra.dirtr
					.txtconductor.Value=fe_tra.nombr
					.txtbrevete.Value=fe_tra.breve
					.txtconstancia.Value=fe_tra.Cons
					.txtmarca.Value=fe_tra.marca
					.txtplaca1.Value=fe_tra.placa1
		     		.txtrazont.SetFocus
				Else
					.verdad=.F.
					If Messagebox("Placa de Transportista NO Registrada ¿Desea Adicionarlo?",48+4+0,MSGTITULO)=7
						.txtplaca.Value=""
					Else
						.idtran=0
						.txtrazont.Value=""
						.txtdirecciont.Value=""
						.txtconductor.Value=""
						.txtbrevete.Value=""
						.txtconstancia.Value=""
						.txtruct.Value=""
						.txtplaca1.Value=""
						.txtmarca.Value=""
						.ad(1)
						.txtmarca.SetFocus
					Endif
				Endif
			Endif
		Endwith
	ENDPROC

	PROCEDURE txtserie.LostFocus
		this.value=right("000"+alltrim(this.value),3)
	ENDPROC

	PROCEDURE txtserieg.LostFocus
		This.BackColor=Rgb(255,255,255)
		This.Value=Right("000"+Alltrim(This.Value),3)
		If BuscarSeries(Val(This.Value),'09')=0 Then
		    this.Value=""
		    thisform.txtnumerog.Value=""
			Return 
		  Else
		  	thisform.txtnumerog.Value=Alltrim(Str(series.nume))
			thisform.Item=series.items
			thisform.nsgte=series.nume
		    thisform.idserie=series.idserie
		EndIf
	ENDPROC

