	PROCEDURE cmdver.Click
		If Thisform.proveedor=0 Then
			Return
		Endif
		With Thisform
			.txtpago.Value=0
			.txtimporte.Value=0
			.txtdias.Value=0
			.txtamortiza.Value=0
			.txtdocumento.Value=""
			.txtdetalle.Value=""
			.txtnumero.Value=""
			.txtnumero.Visible=.T.
			.grimodelos.Recordsource=""
			Do Form cr_vtosdocumentos With .proveedor,"C" To niden
			If xt.tipo="L"
				If !Empty(niden)
					nclie=Thisform.proveedor
					Select ndoc,fech,fevto,dias,rcre_impc,saldo,pagos,nomv,ncontrol,situa,rcre_idau,tipo,docd,dscto,mone,nrou,rcre_idrc,tdoc;
						FROM tmp Where pagos>0 Into Cursor txp
					Select * From tmp Where ncontrol=niden Into Cursor apagar
					.grimodelos.RecordSource="apagar"
					nidauto=apagar.rcre_idau
					.txtnumero.Value=apagar.ndoc
					.txtcontrol.Value=apagar.ncontrol
					.txtdocumento.Value=apagar.docd
					.txttipo.Value=apagar.tipo
					.txtfechae.Value=apagar.fech
					.txtfechavto.Value=apagar.fevto
					.txtimporte.Value=apagar.saldo
					.txtimporteo.Value=apagar.rcre_impc
					.txtsituacion.Value=apagar.situa
