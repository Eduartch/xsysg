	PROCEDURE Chkeditar1.Click
		If This.Value=1 Then
			If Left(goapp.tipousuario,1)<>'A' And Left(goapp.tipousuario,1)<>'G' Then
				Do Form v_verifica With "A" To verdad
				If !verdad
					Thisform.chkeditar1.Value=0
					Return
				Endif
			Endif
			Thisform.frame.Enabled= .T.
			Thisform.chkeditar1.Value=1
		Else
			Thisform.chkeditar1.Value=0
			Thisform.frame.Enabled= .F.
		Endif
