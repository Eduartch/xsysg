	PROCEDURE Init
		Settear()
		If datosGlobales()=0 Then
			Return .F.
		Endif
		With Thisform
			.cmbforma.ListIndex=1
			.txtdolar.Value=fe_gene.dola
			.grimodelos.RecordSource=""
		Endwith
		
	ENDPROC

	PROCEDURE recibo
		Local cnuc,npos
		Thisform.importe=N2L(Thisform.txtpago.Value)
		cnuc=Alltrim(Str(Thisform.txtpago.Value,10,2))
		npos=At(".",cnuc)
		If Thisform.cmbMONEDA1.ListIndex=1
			cm= "NUEVOS SOLES"
		Else
			cm= "DÖLARES AMERICANOS"
		Endif
		Do Case
		Case npos=0
			ccadena='00'+'/100 '+cm
		Case Len(Substr(cnuc,npos+1))=1
			ccadena=Substr(cnuc,npos+1,1)+'0'+'/100 ' +cm
		Otherwise
			ccadena=Substr(cnuc,npos+1,2)+'/100 '+cm
		Endcase
		Thisform.importe=Thisform.importe+' '+ccadena
		Report Form infrecibos To Printer Prompt Noconsole
		
	ENDPROC

	PROCEDURE Unload
		RETURN thisform.letras 
	ENDPROC

	PROCEDURE validar
		If Thisform.CMBcaja.ListIndex=1
			x=validacaja(Thisform.txtfecha.Value)
			If x="C"
				Messagebox("La caja de Esta Fecha Esta Cerrada",16,MSGTITULO)
				Return .F.
			Endif
		Endif
		np=Thisform.txtpago.Value
		With Thisform
			Do Case
			Case Empty(Thisform.txtnumero.Value)
				Thisform.mensaje="Ingrese Documento de Pago"
				Return .F.
			Case !esfechavalida(Thisform.txtfecha.Value)
				Thisform.mensaje="Fecha No Válida"
				Return .F.
			Case Thisform.cmbforma.ListIndex=2 And Thisform.txtamortiza.Value=0
				Thisform.mensaje="Ingrese Monto a Amortizar"
				Return .F.
			Case Thisform.proveedor=0
				Thisform.mensaje="Seleccione un Cliente"
				Return .F.
			Case np=0
				Thisform.mensaje="Importes No Válidos"
				Return .F.
			Otherwise
				Return .T.
			Endcase
		Endwith
		
	ENDPROC

