	PROCEDURE Init
		Lparameters cndoc,ctdoc,ccodp
		Settear()
		If MuestraAlmacenes()=0 Then
			Return .F.
		Endif
		If MuestraDctos('')=0 Then
			Return .F.
		Endif
		If MuestraPlanCuentas('')=0 Then
			Return .F.
		Endif
		If DatosGlobales()=0 Then
			Return .F.
		Endif
		With Thisform
			.cargarctas()
			Select * From Almacenes Into Cursor lalmacenesc
			.cmbalmacen.RowSource="lalmacenesc.nomb"
			Select nomb,TDOC,IDTDOC From dctosv Into Cursor ldctosc Order By TDOC
			.cmbdcto.RowSource="ldctosc.nomb"
			.cmbalmacen.ListIndex=1
			this.cmbtipo.ListIndex=5
			If Empty(cndoc)
				.txtdolar.Value=fe_gene.dola
				.cmbdcto.ListIndex=1
				.cmbmoneda.ListIndex=1
				.cmbforma.ListIndex=1
				This.WindowType= 0
			Else
				This.WindowType= 1
				.txtserie.Value=Left(cndoc,4)
				.txtnumero.Value=Substr(cndoc,5)
				.txtcodigo.Value=ccodp
				Select ldctosc
				Locate For TDOC=ctdoc
				.cmbdcto.Value=ldctosc.nomb
				.TDOC=ctdoc
				If MuestraProveedoresx("",2,ccodp,'proveedores')=0 Then
					Return .F.
				Endif
				.txtruc.Value=proveedores.nruc
				.txtrazon.Value=proveedores.razo
				.txtdireccion.Value=proveedores.Dire
				.txtciudad.Value=proveedores.ciud
				.buscar()
			Endif
		Endwith
		
	ENDPROC

