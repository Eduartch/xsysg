	PROCEDURE Pgf1.Page2.Click
		This.grimodelos1.RecordSource=""
		If verificaAlias("lcl")=1 Then
			Set Procedure To capadatos,ple5,precios Additive
			obj=Createobject("Precios")
			obj.CodCliente=lcl.idclie
			obj.ListarPreciosclientes(lcl.idclie,"Mvcl")
			Select Descri,Prel,PrecioCliente,prec_idpr,prec_idar,prec_idcl From Mvcl Into Cursor Mvcl Readwrite
			This.grimodelos1.RecordSource="Mvcl"
			This.grimodelos1.Refresh
			This.grimodelos1.colUMN1.SetFocus
		Endif
		
	ENDPROC

	PROCEDURE Pgf1.Page2.Cmdaceptar1.Click
		If PreguntaGuardar()=6 Then
			Set Procedure To capadatos,clientes,precios Additive
			obj=Createobject("precios")
			Select mvcl
			Scan All
				If mvcl.prec_idpr=0 Then
					obj.CodProducto=mvcl.prec_idar
					obj.CodCliente=mvcl.prec_idcl
					obj.nprecio=mvcl.preciocliente
					obj.codigo=mvcl.prec_idpr
					obj.RegistraPreciosXCliente()
				Endif
			Endscan
			This.Parent.Parent.page2.Click()
		Endif
		
	ENDPROC

	PROCEDURE Pgf1.Page2.Cmdproductos1.Click
		If verificaAlias("lcl")=1 Then
			If lcl.idclie>0 Then
				Do Form ka_muestraproductos With 'C' To nidart
				If nidart>0 Then
					Select mvcl
					Locate For prec_idar=nidart
					If !Found()
						Insert Into mvcl(Descri,prel,prec_idar,prec_idcl)Values(lproductos.Descri,lproductos.pre1,lproductos.idart,lcl.idclie)
					Endif
					This.Parent.Parent.page2.grimodelos1.Refresh
					This.Parent.Parent.page2.grimodelos1.colUMN3.SetFocus
				Endif
			Endif
		Endif
		
	ENDPROC

	PROCEDURE Pgf1.Page2.grimodelos1.COLUMN3.Cmdborraritem1.Click
