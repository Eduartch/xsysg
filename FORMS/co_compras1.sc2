	PROCEDURE Cmdaceptar.Click
		Local rpta,swc,swe
		If !Thisform.Valida() Then
			Messagebox(Thisform.mensaje,48,MSGTITULO)
			Return
		Endif
		If Thisform.txtencontrado.Value="V"
			rpta=PreguntaGuardar("¿Desea Guardar Los Cambios en este Documento <Si/No>? "+Alltrim(Thisform.cmbdcto.Value)+' '+Thisform.TXTSErie.Value+Thisform.TXTNUmero.Value)
		Else
			rpta=PreguntaGuardar()
		Endif
		Do Case
			Case rpta=7
				Thisform.nuevo()
				Return
			Case rpta=2
				Thisform.txttotal.SetFocus
				Return
		Endcase
		With Thisform
			nidcta1=ctasv1.idcta
			nidcta2=ctasv2.idcta
			nidcta3=ctasv3.idcta
			nidcta4=ctasv4.idcta
			nidctai=ctasi.idcta
			nidctae=ctase.idcta
			nidcta7=ctaso.idcta
			nidctat=ctast.idcta
			nimpo1=.txtvalor1.Value
			nimpo2=.txtvalor2.Value
			nimpo3=.txtvalor3.Value
			nimpo4=.txtvalor4.Value
			nimpo5=.txtigv.Value
			nimpo6=.txtexonerado.Value
			nimpo7=.txtotros.Value
			nimpo8=.txttotal.Value
			ct1=Left(.cmbtipo1.Value,1)
			ct2=Left(.cmbtipo2.Value,1)
			ct3=Left(.cmbtipo3.Value,1)
			ct4=Left(.cmbtipo4.Value,1)
			ct5=Left(.cmbtipo5.Value,1)
			ct6=Left(.cmbtipo6.Value,1)
			ct7=Left(.cmbtipo7.Value,1)
			ct8=Left(.cmbtipo8.Value,1)
			nt1=Iif(.tdoc='07',-nimpo1,nimpo1)
			nt2=Iif(.tdoc='07',-nimpo2,nimpo2)
			nt3=Iif(.tdoc='07',-nimpo3,nimpo3)
			nt4=Iif(.tdoc='07',-nimpo4,nimpo4)
			nt5=Iif(.tdoc='07',-nimpo5,nimpo5)
			nt6=Iif(.tdoc='07',-nimpo6,nimpo6)
			nt7=Iif(.tdoc='07',-nimpo7,nimpo7)
			nt8=Iif(.tdoc='07',-nimpo8,nimpo8)
			cm=Left(.cmbmoneda.Value,1)
			vdvto=0
			pasaCtasPagar=0
			If .cmbforma.ListIndex=2 And .tienepagos=0 And .tdoc<>'07' Then
				Do Form de_registra With .txtcodigo.Value,Left(.cmbmoneda.Value,1),.TXTSErie.Value+.TXTNUmero.Value,;
					.txttotal.Value,.txtfecha.Value,.txtdolar.Value,.tdoc,0,ctast.idcta To vdvto
				If vdvto=0 Then
					Messagebox("No Se Grabo Como Crédito",16,MSGTITULO)
					Return
				Endif
				pasaCtasPagar=1
				vdvto=1
			Else
				If .cmbforma.ListIndex=2 And .tdoc='07' Then
					Do Form cr_vtosdocumentos With .txtcodigo.Value,"P" To niden
					If niden<>Thisform.txttotal.Value  Then
						Messagebox("NO Ingreso como Descuento en el Estado de Cuenta del Proveedor Porque Los Importes no Coinciden",48,MSGTITULO)
						Return
					Endif
					vdvto=2
					pasaCtasPagar=2
				Else
					vdvto=1
				Endif
			Endif
			If IniciaTransaccion()=0 Then
				DeshacerCambios()
				Return
			Endif
			If .txtencontrado.Value<>'V' Then
				.nauto=IngresaResumenDctoC(.tdoc,Left(.cmbforma.Value,1),;
					.TXTSErie.Value+.TXTNUmero.Value,.txtfecha.Value,.txtfechar.Value,.txtdetalle.Value,nt1+nt2+nt3+nt4,nt5,nt8,'',Left(.cmbmoneda.Value,1),;
					.txtdolar.Value,fe_gene.igv,Left(.cmbtipo.Value,1),.txtcodigo.Value,Left(.cmbtipo.Value,1),goapp.nidusua,0,goapp.tienda,0,0,0,0,0,nt6,nt8)
				If .nauto=0 Then
					DeshacerCambios()
					Return
				Endif
				If IngresaValoresCtasC1(nt1,nt2,nt3,nt4,nt5,nt6,nt7,nt8,nidcta1,nidcta2,nidcta3,nidcta4,nidctai,nidctae,nidcta7,nidctat,ct1,ct2,ct3,ct4,ct5,ct6,ct7,ct8,.nauto)=0 Then
					DeshacerCambios()
					Return
				Endif
				If IngresaDatosLCajaEFectivo11(.txtfechar.Value,"",.txtrazon.Value,nidctat,0,.txttotal.Value,;
						cm,fe_gene.dola,0,.txtcodigo.Value,.nauto,Left(.cmbforma.Value,1),.TXTSErie.Value+.TXTNUmero.Value,.tdoc)=0 Then
					DeshacerCambios()
					Return
				Endif
				If .cmbforma.ListIndex=2 Then
					If .registrarcuentasporpagar()=0 Then
						DeshacerCambios()
						Return
					Endif
				Endif
			Else
				.nauto=Thisform.nreg
				If ActualizaResumenDctoC(.tdoc,Left(.cmbforma.Value,1),;
						.TXTSErie.Value+.TXTNUmero.Value,.txtfecha.Value,.txtfechar.Value,.txtdetalle.Value,nt1+nt2+nt3+nt4,nt5,nt8,'',Left(.cmbmoneda.Value,1),;
						.txtdolar.Value,fe_gene.igv,Left(.cmbtipo.Value,1),.txtcodigo.Value,Left(.cmbtipo.Value,1),goapp.nidusua,0,goapp.tienda,0,0,0,0,0,.nreg,nt6,nt8)=0 Then
					DeshacerCambios()
					Return
				Endif
				If .formaregistrada='C' And .cmbforma.ListIndex=1 Then
					If ActualizaDeudas(.nreg,goapp.nidusua)=0
						Return 0
					Endif
				Endif
				If IngresaDatosLCajaEFectivo11(.txtfechar.Value,"",.txtrazon.Value,nidctat,0,.txttotal.Value,;
						cm,fe_gene.dola,0,.txtcodigo.Value,.nauto,Left(.cmbforma.Value,1),.TXTSErie.Value+.TXTNUmero.Value,.tdoc)=0 Then
					DeshacerCambios()
					Return
				Endif
				id1=.idcta1
				id2=.idcta2
				id3=.idcta3
				id4=.idcta4
				id5=.idcta5
				id6=.idcta6
				id7=.idcta7
				id8=.idcta8
				IF verificaconexion()=0 then
		        RETURN 0
		        endif
				If SQLExec(goapp.bdconn,"Call ProActualizactasc(?nt1,?nt2,?nt3,?nt4,?nt5,?nt6,?nt7,?nt8,?nidcta1,?nidcta2,?nidcta3,?nidcta4,?nidctai,?nidctae,?nidcta7,?nidctat,?id1,?id2,?id3,?id4,?id5,?id6,?id7,?id8,?ct1,?ct2,?ct3,?ct4,?ct5,?ct6,?ct7,?ct8)")<0
					errorbd(ERRORPROC)
					DeshacerCambios()
					Return
				Endif
				If .nreg>0 Then
					If ActualizaDeudas(.nreg,goapp.nidusua)=0
						DeshacerCambios()
						Return
					Endif
				Endif
				If Thisform.cmbforma.ListIndex=2 Then
					If Thisform.registrarcuentasporpagar()=0 Then
						DeshacerCambios()
						Return
					Endif
				Endif
			Endif
			GrabarCambios()
			.nuevo()
		Endwith
		
		
	ENDPROC

