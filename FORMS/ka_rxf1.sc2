		thisform.nalmacen=ltdas.idalma
		
	ENDPROC

	PROCEDURE Cmdaexcel1.Click
		If verificaAlias("rmvtos")=1 Then
			This.titulo1="Reporte de Transacciones"
			This.calias="rmvtos"
			This.cgriddata="thisform.grimodelos"
			DoDefault()
		Endif
		
	ENDPROC

	PROCEDURE cmdconsulta.Click
		With Thisform
			.grimodelos.RecordSource=""
			dfi=.txtfechainicial.Value
			dff=.txtfechafinal.Value
			If .chktienda.Value=0 Then
				TEXT TO lc NOSHOW
				    SELECT ndoc as dcto,a.fech,b.nruc,b.razo,a.valor,a.rcom_exon,CAST(0 as decimal(12,2)) as inafecto,
				    a.igv,a.impo,u.nomb as usuario,fusua,rcom_hash,rcom_mens,mone,a.tdoc,a.ndoc,idauto,rcom_arch,b.clie_corr,tcom,a.idusua,a.deta
				    FROM fe_rcom as a JOIN fe_clie as b ON (a.idcliente=b.idclie) join fe_usua as u on u.idusua=a.idusua
				    where a.fech between ?dfi and ?dff  and  a.acti<>'I'  order by fech,ndoc
				ENDTEXT
			Else
				nidcodt=.nalmacen
				TEXT TO lc NOSHOW
				    SELECT a.ndoc as dcto,a.fech,b.nruc,b.razo,a.valor,a.rcom_exon,CAST(0 as decimal(12,2)) as inafecto,
				    a.igv,a.impo,u.nomb as usuario,fusua,rcom_hash,rcom_mens,mone,a.tdoc,a.ndoc,idauto,rcom_arch,b.clie_corr,tcom,a.idusua,a.deta
				    FROM fe_rcom as a JOIN fe_clie as b ON (a.idcliente=b.idclie) join fe_usua as u on u.idusua=a.idusua
				    where a.fech between ?dfi and ?dff  and a.acti<>'I'  and a.codt=?nidcodt  order by fech,ndoc
				ENDTEXT
			ENDIF
				IF verificaconexion()=0 then
		RETURN 0
		endif
			ncon=AbreConexion()
			If SQLExec(ncon,lc,"rmvtos")<0
				errorbd(lc)
				Return
			Else
				CierraConexion(ncon)
				Select dcto,fech,nruc,razo,valor,rcom_exon,inafecto,igv,Impo,usuario,fusua As fechaoperacion,;
				rcom_hash,rcom_mens,mone,tdoc,ndoc,idauto,rcom_arch,clie_corr,tcom,idusua,deta;
					From rmvtos Into Cursor rmvtos
				If .chkdcto.Value=1 Then
					Select * From rmvtos Where tdoc=ldctos.tdoc Into Cursor rmvtos
				Endif
				If !Empty(.txtserie.Value) Then
					cserie=.txtserie.Value
					Select * From rmvtos Where Left(ndoc,4)=cserie Into Cursor rmvtos
				Endif
				If This.Parent.chkusuario.Value=1 Then
					Select * From rmvtos Where idusua=lu.idusua Into Cursor rmvtos
				Endif
				Select * From rmvtos Into Cursor lll
				.grimodelos.RecordSource="rmvtos"
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE Cmdenviocorreo1.Click
		If verificaAlias("rmvtos")=1 Then
			Set Procedure To capadatos,enviarcorreo,ple5,imprimir Additive
			Select rmvtos
			Go Top
			Do While !Eof()
				If Empty(rmvtos.clie_corr) Or Alltrim(rmvtos.clie_corr)="@" Or Len(Alltrim(rmvtos.clie_corr))<=1 Then
					Messagebox("Correo Electrónico No Válido",16,MSGTITULO)
					Select rmvtos
					Skip
					Loop
				Endif
				EnviaCorreoBloque()
				Select rmvtos
				Skip
			Enddo
		Endif
		
	ENDPROC

