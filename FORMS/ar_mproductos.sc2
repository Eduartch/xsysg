			.frame.txtcodigof.ControlSource=""
			.frame.txtcodm.ControlSource=""
			.frame.txtcodg.ControlSource=""
			.frame.txtflete.ControlSource=""
		Endwith
		
	ENDPROC

	PROCEDURE validar
		Do Case
		Case  Empty(thisform.frame.txtdescripcion.Value) Or Empty(thisform.frame.txtunidad.Value)
			Thisform.mensaje="Ingrese Descripcion y Unidad del Producto"
			Return .F.
		Case Thisform.encontrado='S'
			Thisform.mensaje="Nombre de Producto Ya Esta Registrado"
			Return .F.
		Case Thisform.codencontrado='S'
			Thisform.mensaje="Código Ya Esta Registrado"
			Return .F.	
		Otherwise
			Return .T.
		Endcase
		
	ENDPROC

	PROCEDURE cmdaceptar.Click
		Local rpta
		If Thisform.chkeditar1.Value=0 And Thisform.modo="M"
			Return
		Endif
		If !Thisform.validar()
			Messagebox(Thisform.mensaje,16,MSGTITULO)
			Return
		Endif
		rpta=PregunTaGuardar()
		With Thisform.frame
			Do Case
			Case rpta=2
				.txtdescripcion.SetFocus
				Return
			Case rpta=6
				cdesc=.txtdescripcion.Value
				cunid=.txtunidad.Value
				nprec=.txtprecio.Value
				ncosto=.txtcosto.Value
				np1=.txtpreciomayor.Value
				np2=0
				np3=0
				npeso=.txtpeso.Value
		
				ccat=1
				cmar=1
		
				ctipro=Left(.cmbtipo.Value,1)
		
				nflete=1
		
				cm=Left(.cmbmoneda.Value ,1)
				ce=Left(.cmbestado.Value,1)
				nprecio=.txtprecio.Value
				nidgrupo=1
				ncosto=.txtcosto.Value
				cidpc=Id()
				dfecha=Datetime()
				nidusua=goapp.nidusua
				nutil1=(.txtutilidad1.Value/100)+1
				nutil2=0
				nutil3=0
				ncome=0
				ncomc=0
				nsmax=.txtstmaximo.Value
				nsmin=.txtstminimo.Value
				nidalma=almacenes.idalma
				ccodigo1=This.Parent.frame.txtcodigo1.Value
				ndolar=.txtdolar.Value
				If Thisform.modo="N"
					If CreaProductos(cdesc,cunid,nprec,ncosto,np1,np2,np3,npeso,ccat,cmar,ctipro,nflete,cm,cidpc,ncome,ncomc,nutil1,nutil2,nutil3,nidusua,nsmax,nsmin,ccodigo1,ndolar)=0 Then
						Return
					Else
						mensaje("Creado Correctamente")
						Thisform.limpiar()
						This.Parent.frame.txtdescripcion.SetFocus
					Endif
				Else
					ncoda=.txtcodigo.Value
					If ModificaProductos(cdesc,cunid,ncosto,np1,np2,np3,npeso,ccat,cmar,ctipro,nflete,cm,nprecio,ccodigo1,nutil1,nutil2,nutil3,ncome,ncomc,nidusua,ncoda,nsmax,nsmin,1,ndolar,'A')=0 Then
						Messagebox(" NO Editado Correctamente",64,MSGTITULO)
					Else
						mensaje("Editado Correctamente")
						Thisform.cmdcerraro.Click()
					Endif
				Endif
			Case rpta=7
				If Thisform.modo='N' Then
					Thisform.limpiar()
					Return
				Endif
			Endcase
		Endwith
		
	ENDPROC

