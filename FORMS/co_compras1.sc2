	PROCEDURE ingresadeudas
		If Thisform.registrarctasporpagar1.registra("tmpd",thisform.nauto,Thisform.txtcodigo.Value,Left(Thisform.cmbmoneda.Value,1),Thisform.txtfecha.Value,;
				Thisform.txttotal.Value,ctast.idcta,Thisform.txtdolar.Value)=0 Then
			Return 0
