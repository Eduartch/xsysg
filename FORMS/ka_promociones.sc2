	PROCEDURE consultarpromociones
		Thisform.pagef1.page1.grimodelos1.RecordSource=""
		TEXT TO lc noshow
			select Ifnull(a.descri,f.rpro_nomb) as descri,f.rpro_cant,rpro_unid,rpro_impo,rpro_feci,rpro_fecf,rpro_idrp,rpro_cond,rpro_tipo from
			 fe_rpro as f  left join fe_art as a  on a.idart=f.rpro_idar where rpro_acti='A' order by rpro_feci desc
		ENDTEXT
		If SQLExec(goapp.bdconn,lc,'rpromo')<1 Then
			errorbd(lc)
			Return
		Endif
		Thisform.pagef1.page1.grimodelos1.RecordSource="rpromo"
		Thisform.pagef1.page1.grimodelos1.column1.ControlSource="rpromo.descri"
		Thisform.pagef1.page1.grimodelos1.column2.ControlSource="rpromo.rpro_feci"
		Thisform.pagef1.page1.grimodelos1.column3.ControlSource="rpromo.rpro_fecf"
		
		Thisform.pagef1.page1.grimodelos1.column6.ControlSource="rpromo.rpro_unid"
		Thisform.pagef1.page1.grimodelos1.column7.ControlSource="rpromo.rpro_cant"
		
		Thisform.pagef1.page1.grimodelos1.column8.ControlSource="rpromo.rpro_impo"
		*Thisform.pagef1.page1.grimodelos1.column7.ControlSource="rpromo.descri"
	ENDPROC

