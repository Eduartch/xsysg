	PROCEDURE cmdaceptar.Click
		Local npor,rpta,xprec,xtdoc,x,vdev,vcoda
		vdev="X"
		Store 0 To xdev,vcantidad,vprec
		With Thisform
			If !Thisform.validar() Then
				Messagebox(.mensaje,48,MSGTITULO)
				Return
			Endif
			cdcto=Alltrim(.cmbdcto.Value)+' '+.txtserie.Value+.txtnumero.Value
			rpta=PreguntaGuardar()
			Do Case
			Case rpta=7
				.nuevo()
				Return
			Case rpta=2
				.txtvalor.SetFocus
				Return
			Endcase
			rpta=0
			xtdoc=Left(.cmbdcto.Value,2)
			If .cmbforma.ListIndex=2
				If xtdoc="07"
					Do Form cr_cancela With .txtcodigo.Value,.txttotal.Value,1 To ctipo
					If ctipo="S" And !Used("apagar")
						Messagebox("No Ingreso como Pago-Letra",48,MSGTITULO)
						Return
					Endif
					If ctipo="N" And !Used("tmp")
						Messagebox("No Ingreso como Pago-Factura",48,MSGTITULO)
						Return
					Endif
					.tipod=ctipo
				Endif
			Endif
			cndcto=.txtserie.Value+.txtnumero.Value
			ctdoc=Left(.cmbdcto.Value,2)
			ntotal=.txttotal.Value
			nv=Iif(xtdoc='07',-.txtvalor.Value,.txtvalor.Value)
			nigv=Iif(xtdoc='07',-.txtigv.Value,.txtigv.Value)
			nt=Iif(xtdoc='07',-.txttotal.Value,.txttotal.Value)
			dfvta=This.Parent.txtfecha.Value
			sw=1
			nidpagos=0
			ncomision=(nt*.Tcomisiones)/.txtfimporte.Value
			oturnos=Createobject("turno")
			oturnos.ListarTurnos("lturnos")
			Go Top In lturnos
			ogrifero.Asigna(goapp.nidusua,lturnos.turn_inic,lturnos.turn_fina,fe_gene.alma_Sepa,lturnos.turn_refe,goapp.usuario)
			Select Sum(Round(devo*costo,2)) As tt From tmpn Where Prec=0 Into Cursor sstg
			Create Cursor tmpv(coda N(10),Desc c(100),unid c(4),cant N(12,7),Prec N(12,7),ndoc c(12),cletras c(180),;
				nitem N(5),hash c(30),fech d,codc N(5),guia c(10),direccion c(120),dni c(8),Forma c(30),fono c(15),tdoc c(2),;
				vendedor c(60),dias N(3),razon c(120),nruc c(11),mone c(1) Default 'S',fech1 d,tdoc1 c(2),dcto c(12),referencia c(60),archivo c(120),tigv N(6,3))
			Go Top In tmpn
			If IniciaTransaccion()=0 Then
				DeshacerCambios()
				Return
			Endif
			If .ctipoventa='N' Then
				ntigv=fe_gene.igv
				.nauto=IngresaDocumentoElectronico(ctdoc,Left(.cmbforma.Value,1),cndcto,.txtfecha.Value,.txtmodifica.Value,nv,nigv,nt,"",Left(.cmbmoneda.Value,1),;
					.txtdolar.Value,fe_gene.igv,tmpn.tcom,.txtcodigo.Value,fe_gene.alma_Sepa,goapp.nidusua,goapp.tienda,fe_gene.idctav,fe_gene.idctai,fe_gene.idctat,sstg.tt,0,0,0)
			Else
				ntigv=1
				.nauto=IngresaDocumentoElectronico(ctdoc,Left(.cmbforma.Value,1),cndcto,.txtfecha.Value,.txtmodifica.Value,0,0,nt,"",Left(.cmbmoneda.Value,1),;
					.txtdolar.Value,1,tmpn.tcom,.txtcodigo.Value,fe_gene.alma_Sepa,goapp.nidusua,goapp.tienda,fe_gene.idctav,fe_gene.idctai,fe_gene.idctat,sstg.tt,0,nv,0)
			Endif
			If .nauto=0
				DeshacerCambios()
				Return
			Endif
			If IngresaDatosLCajaEFectivoCturnos(.txtfecha.Value,"",.lblRAZON.Value,fe_gene.idctat,nt,0,;
					'S',fe_gene.dola,goapp.nidusua,.txtcodigo.Value,.nauto,Left(.cmbforma.Value,1),.txtserie.Value+.txtnumero.Value,ctdoc,goapp.isla,fe_gene.alma_Sepa)=0 Then
				DeshacerCambios()
				Return
			Endif
			If .cmbforma.ListIndex=1
				If IngresaRvendedores(.nauto,.txtcodigo.Value,.cvendedor,"E")=0 Then
					DeshacerCambios()
					Return
				Endif
			Else
				If xtdoc='07' Then
					If IngresaRvendedores(.nauto,.txtcodigo.Value,.cvendedor,"E")=0 Then
						DeshacerCambios()
						Return
					Endif
				Endif
			Endif
			If .cmbforma.ListIndex=2 Then
				If xtdoc='08' Then
					If .ingresarcreditos()=0 Then
						DeshacerCambios()
						Return
					Endif
				Else
					nidpagos=.ingresapagos()
					If nidpagos=0 Then
						DeshacerCambios()
						Return
					Endif
				Endif
			Endif
			If .optdetalles.optagrupada.Value=1
				Go Top In tmpn
				If xtdoc="07"
					Insert Into tmpv(Desc,cant,Prec,ndoc,tigv)Values("Descuento Adicional",1,1*.txttotal.Value,cndcto,ntigv)
				Else
					Insert Into tmpv(Desc,cant,Prec,ndoc,tigv)Values(Alltrim(.txtmodifica.Value),1,1*.txttotal.Value,cndcto,ntigv)
				Endif
		*	IngresaKardexGrifo(na,tmpv.coda,'V',tmpv.Prec,tmpv.cant,'I','K',.codv,tmpv.isla,tmpv.nidcontometro,0,tmpv.pre1)
				If INGRESAKARDEXGrifo(.nauto,tmpn.coda,'V',0,0,'I','K',lv.idven,goapp.tienda,0,ncomision,0)=0 Then
					DeshacerCambios()
					Return
				Endif
			Else
				Select tmpn
				Go Top
				.comprobante1.tipoventanotacredito=tmpn.tcom
				p=0
				Do While !Eof()
					p=p+1
					If tmpn.devo>0 Then
						Insert Into tmpv(coda,Desc,unid,cant,Prec,ndoc,tigv)Values(tmpn.coda,tmpn.Desc,tmpn.unid,-tmpn.devo,tmpn.dsct,cndcto,ntigv)
						If tmpn.tcom='K' Then
							If INGRESAKARDEXGrifo(.nauto,tmpn.coda,'V',tmpn.Prec,-tmpn.devo,'I','K',lv.idven,tmpn.alma,0,tmpn.costo,0)=0 Then
								sw=0
								Exit
							Endif
							If ActualizaStock(tmpn.coda,tmpn.alma,tmpn.devo,'C')=0 Then
								sw=0
								Exit
							Endif
						Else
							If IngresaDetalleVTa(tmpn.Desc,p,p,p,.nauto,tmpn.Prec,tmpn.cant)=0 Then
								sw=0
								Exit
							Endif
						Endif
					Else
						If tmpn.dsct>0 Then
							If INGRESAKARDEXGrifo(.nauto,tmpn.coda,'V',tmpn.dsct,0,'I','K',lv.idven,tmpn.alma,0,0,0)=0 Then
								sw=0
								Exit
							Endif
							Insert Into tmpv(coda,Desc,unid,cant,Prec,ndoc,tigv)Values(tmpn.coda,tmpn.Desc,tmpn.unid,1,tmpn.dsct,cndcto,ntigv)
						Endif
					Endif
					Select tmpn
					Skip
				Enddo
			Endif
			If sw=0 Then
				DeshacerCambios()
				Return
			Endif
			Select idauto From tmpn Where idauto>0 Into Cursor xt Group By idauto
			Select xt
			Scan All
				If IngresarNotasCreditoVentas1(.nauto,xt.idauto,nidpagos)=0 Then
					sw=0
					Exit
				Endif
			Endscan
			If sw=1 And PermiteIngresoVentas1(cndcto,ctdoc,.nauto,dfvta)>=1 And .generanumero()=1 Then
				GrabarCambios()
				This.Parent.comprobante1.Version='2.1'
				If fe_gene.gene_exon='N' Then
					If xtdoc='07' Then
						If .tdoc1='01' Then
							vdcto=.comprobante1.obtenerdatosnotascredito(.nauto,'E',Iif(fe_gene.gene_cpea='N','SF',.F.))
						Else
							vdvto=.comprobante1.obtenerdatosnotascredito(.nauto,"",'SF')
						Endif
					Else
						If .tdoc1='01' Then
							vdvto=.comprobante1.obtenerdatosnotasDebito(.nauto,'E',Iif(fe_gene.gene_cpea='N','SF',.F.))
						Else
							vdvto=.comprobante1.obtenerdatosnotasDebito(.nauto,"",'SF')
						Endif
					Endif
				Else
					If xtdoc='07' Then
						If .tdoc1='01' Then
							vdvto=.comprobante1.obtenerdatosnotecreditoexonerada(.nauto,'E',Iif(fe_gene.gene_cpea='N','SF',.F.))
						Else
							vdvto=.comprobante1.obtenerdatosnotecreditoexonerada(.nauto,"",'SF')
						Endif
					Else
						If .tdoc1='01' Then
							vdvto=.comprobante1.obtenernotasdebitoexonerada(.nauto,'E',Iif(fe_gene.gene_cpea='N','SF',.F.))
						Else
							vdvto=.comprobante1.obtenernotasdebitoexonerada(.nauto,"",'SF')
						Endif
					Endif
				Endif
				.importe=Diletras(.txttotal.Value,'S')
				Set Procedure To capadatos,foxbarcodeqr,imprimir Additive
				m.oFbc = Createobject("FoxBarcodeQR")
				Select tmpv
				Select Count(*) As ti From tmpv Into Cursor items
				**For x=1 To .Item-items.ti
				*	Insert Into tmpv(ndoc)Values(cndcto)
				*Endfor
				Replace All ndoc With cndcto,cletras With .importe,;
					nruc  With .txtruc.Value,razon With .lblRAZON.Value,Forma With .cmbforma.Value,fech With .txtfecha.Value,;
					referencia With .txtmodifica.Value,vendedor With .cmbvendedor.Value,tdoc With xtdoc,;
					hash With .comprobante1.hash,mone With .cmbmoneda.Value,direccion With Alltrim(.txtdireccion.Value),;
					dni With .txtdni.Value,tdoc1 With .tdoc1,dcto With .cdctos,referencia With .txtmodifica.Value,fech1 With .fecha,tigv With ntigv In tmpv
				Select tmpv
				Go Top
				obji=Createobject("Imprimir")
				obji.Tdoc=xtdoc
				obji.ArchivoPdf=.comprobante1.ArchivoPdf
				obji.ElijeFormato()
				obji.ImprimeComprobanteComoticket('S')
			*	obji.ImprimeComprobante('S')
				obji.GeneraPDF("")
				If !Empty(.comprobante1.correo) Then
		*	.comprobante1.enviarcorreocliente(.comprobante1.correo)
				Endif
				.nuevo()
			Else
				DeshacerCambios()
				Return
			Endif
		Endwith
		*******
		
	ENDPROC

