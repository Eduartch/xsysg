	PROCEDURE Cmdaceptar.Click
		Local ximporte,xfevto,xestado,rpta,ri
		If !Thisform.validar()
			Messagebox(Thisform.mensaje,16,MSGTITULO)
			Return
		Endif
		With Thisform
			rpta=Messagebox(MSGGUARDA,32+3+0,MSGTITULO)
			Do Case
			Case rpta=2
				.txtpago.SetFocus
				Return
			Case rpta=6
				Cajas=''
				If PermiteIngresoaCaja(.txtfecha.Value)=0 Then
					Messagebox("Ya se La Liquido Caja en esta Fecha",16,MSGTITULO)
					Do Form v_verifica With 'A' To verdad
					If !verdad Then
						Return
					Endif
					Cajas='A'
				Endif
				nimp1=Iif(.cmbforma.ListIndex=1,.txtpago.Value,0)
				cm2="S"
				nt1=.txtpago.Value
				cm1="S"
				tcvta=.txtdolar.Value
				cfo=Left(.cmbcaja.Value,1)
				*ri=Messagebox("¿Emitir Recibo de Ingresos S/N?",48+3+0,MSGTITULO)
				cdetalle=""
				corigen=""
				If .cmbcaja.ListIndex=1
					cdetalle="Pago:"+Rtrim(.txtcliente.Value)
				Else
					cdetalle=Alltrim(.txtdetalle.Value)+' Cliente: '+ Rtrim(.txtcliente.Value)
				Endif
				nidclie=.proveedor
				cndoc=.txtnumero.Value
				cesta="P"
				dfech=.txtfecha.Value
				ndola=.txtdolar.Value
				nidus=goapp.nidusua
				cfo=Left(.cmbcaja.Value,1)
				nidtda=goapp.tienda
				cpc=Id()
				cbanco="Pago:"+Alltrim(.txtcliente.Value)+Alltrim(.txtdetalle.Value)
				cb1=.txtdetalle.Value
				swca=1
				swcr=1
				con=""
				Do Case
				Case .cmbcaja.ListIndex=1 Or .cmbcaja.ListIndex=3
					nidcon=RetConcepto('PCE','I')
					If .cmbcaja.ListIndex=1 Then
						con='E'
					Endif
				Case .cmbcaja.ListIndex=2
					nidcon=RetConcepto('PCD','I')
				Case .cmbcaja.ListIndex=4
					nidcon=RetConcepto('PCN','I')
				Endcase
				If IniciaTransaccion()=0 Then
					DeshacerCambios()
					Return
				Endif
				If .letras="N"
					Select tmp
					sw=1
					Scan For pagos>0
						cdocp=tmp.docd
						nacta=tmp.pagos
						cmone=tmp.mone
						cbco=tmp.banco
						dfevto=tmp.fevto
						ctipo=tmp.tipo
						csitua=tmp.situa
						nctrl=tmp.ncontrol
						cnrou=""
						nidrc=tmp.rcre_idrc
						If CancelaCreditos(cndoc,nacta,cesta,cmone,cb1,dfech,dfevto,ctipo,nctrl,cnrou,nidrc,cpc,nidus)=0 Then
							swcr=0
							Exit
						Endif
						If con="E" And goapp.xopcion=2 AND (tmp.tdoc='01' OR tmp.tdoc='03' OR tmp.tdoc='07' OR tmp.tdoc='08') Then
							If IngresaDatosLcajaEe(dfech,"","Cliente:"+.txtcliente.Value,fe_gene.gene_idcre,;
									tmp.pagos,0,'S',fe_gene.dola,goapp.nidusua,NIDCREDITOS.NIDC)=0 Then
								swca=0
								Exit
							Endif
						Endif
						If IngresaCaja(tmp.rcre_idau,.txtfecha.Value,tmp.pagos,'I',Left(.cmbcaja.Value,1),'S',tmp.ndoc,nidcon,goapp.nidusua,"Pago:"+.txtcliente.Value,'CC',tmp.pagos,'S',2.85,goapp.tienda,Cajas,NIDCREDITOS.NIDC,0)<=0 Then
							swca=0
							Exit
						Endif
					Endscan
				Else
					cdocp=.txtdocumento.Value
					nacta=.txtpago.Value
					cmone=.txtmoneda.Value
					cbanco="Cliente:"+Alltrim(.txtcliente.Value)+Alltrim(.txtdetalle.Value)
					cbco=.txtbanco.Value
					dfevto=Iif(.cmbforma.ListIndex=1,.txtfechavto.Value,.txtfechavt.Value)
					ctipo=.txttipo.Value
					csitua=.txtsituacion.Value
					nimpc=.txtimporteo.Value
					nctrl=.txtcontrol.Value
					nidven=.vendedor
					cnrou=.txtnrounico.Value
					nauto=.nauto
					nidaval=0
					ndscto=.txtdscto.Value
					nidrc=.txtdscto.Value
					If CancelaCreditos(cndoc,nacta,cesta,cmone,cb1,dfech,dfevto,ctipo,nctrl,cnrou,nidrc,cpc,nidus)=0 Then
						swcr=0
					Else
						If IngresaCaja(nauto,dfech,nacta,'I',Left(.cmbcaja.Value,1),'S',cndoc,nidcon,goapp.nidusua,"Pago:"+.txtcliente.Value,'CC',nacta,'S',2.85,goapp.tienda,Cajas,NIDCREDITOS.NIDC,0)<=0 Then
							swca=0
						Else
							If con="E"  And goapp.xopcion=2  Then
								If IngresaDatosLcajaEe(dfech,"","Cliente:"+.txtcliente.Value,fe_gene.gene_idcre,;
										nacta,0,'S',fe_gene.dola,goapp.nidusua,NIDCREDITOS.NIDC)=0 Then
									swca=0
									Exit
								Endif
							Endif
						Endif
					Endif
				Endif
				If swcr=1 And swca=1
					GrabarCambios()
				Else
					DeshacerCambios()
				Endif
				.retorno=1
				.cmdcancelar.Click()
			Case rpta=7
				.cmdcancelar.Click()
			Endcase
		Endwith
		
	ENDPROC

