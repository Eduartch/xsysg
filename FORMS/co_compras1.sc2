		Local nt
		nt=Thisform.txtvalor1.Value+Thisform.txtvalor2.Value+Thisform.txtvalor3.Value+Thisform.txtvalor4.Value
		Return nt
		
	ENDPROC

	PROCEDURE valida
		x=''
		If Thisform.cmbforma.ListIndex=1 Then
			x=validacaja(Thisform.txtfechar.Value)
		Endif
		nr=Iif(Thisform.txtencontrado.Value="V",Thisform.nreg,0)
		Do Case
		CASE PermiteIngresoCompras(Thisform.TXTSErie.Value+Thisform.TXTNUmero.Value,Thisform.TDOC,Thisform.txtcodigo.Value,nr,Thisform.txtfecha.Value)=0 
			Thisform.mensaje="NUMERO de Documento de Compra Ya Registrado "
			Return .F.
		Case  permiteIngresox(Thisform.txtfechar.Value)=0
			Thisform.mensaje="No Es posible Registrar en esta Fecha estan bloqueados Los Ingresos"
			Return .F.
		Case Empty(Thisform.TXTSErie.Value) Or Empty(Thisform.TXTNUmero.Value)
			Thisform.mensaje="Ingrese un Nº de Documento Válido"
			Return .F.
		Case Year(Thisform.txtfechar.Value)<>Val(goapp.año) or !esfechaValida(Thisform.txtfecha.Value) Or !esfechaValida(Thisform.txtfechar.Value) OR thisform.txtfecha.Value>fe_gene.fech 
			Thisform.mensaje="Fecha No permitida por el Sistema"
			Return .F.
		Case Thisform.txtcodigo.Value=0
			Thisform.mensaje="Seleccione Un Proveedor"
			Return .F.
		Case (Thisform.MontoR<>thisform.txttotal.Value  Or Left(Thisform.monedaregistrada,1)<>Left(Thisform.cmbmoneda.Value,1)) And Thisform.tienepagos=1 AND thisform.txtencontrado.Value='V'
			Thisform.mensaje="El Nuevo Monto Ingresado o el Tipo de Moneda es Diferente al Registrado como Deuda(Tiene Que ser los mismos Datos) "
			Return .F.
		Case Thisform.formaregistrada <> Left(Thisform.cmbforma.Value,1) And Thisform.tienepagos=1 AND thisform.txtencontrado.Value='V'
			Thisform.mensaje="Este Documento Tiene Pagos Aplicados y no es Posible Cambiar la forma de pago"
			Return .F.
		Case x='C' 
			thisform.mensaje="Ya se La Liquido Caja en esta Fecha"
			Return .F.
		Otherwise
			Return .T.
		Endcase
		
	ENDPROC

	PROCEDURE validar1
		x=''
		nr=Thisform.nreg
		Do Case
		Case Thisform.txtencontrado.Value<>'V'
			Thisform.mensaje="Ingrese Un Documento Válido"
			Return .F.
		Case HayDctoenCompras(Thisform.TXTSErie.Value+Thisform.TXTNUmero.Value,Thisform.TDOC,Thisform.txtcodigo.Value,nr)=0
			Thisform.mensaje="NUMERO de Documento de Compra Ya Registrado "
			Return .F.
		Case Len(Alltrim(Thisform.TXTSErie.Value))<4 Or Len(Alltrim(Thisform.TXTNUmero.Value))<8
			Thisform.mensaje="Ingrese un Nº de Documento Válido"
			Return .F.
		Case Year(Thisform.txtfechar.Value)<>Val(goapp.año) Or !esfechaValida(Thisform.txtfecha.Value) Or !esfechaValida(Thisform.txtfechar.Value) Or Thisform.txtfecha.Value>fe_gene.fech
			Thisform.mensaje="Fecha No permitida por el Sistema"
			Return .F.
		Case Thisform.txtcodigo.Value=0
			Thisform.mensaje="Seleccione Un Proveedor"
			Return .F.
		Case (Thisform.MontoR<>Thisform.txttotal.Value  Or Left(Thisform.monedaregistrada,1)<>Left(Thisform.cmbmoneda.Value,1)) And Thisform.tienepagos=1 And Thisform.txtencontrado.Value='V'
			Thisform.mensaje="El Nuevo Monto Ingresado o el Tipo de Moneda es Diferente al Registrado como Deuda(Tiene Que ser los mismos Datos) "
			Return .F.
		Case Thisform.formaregistrada <> Left(Thisform.cmbforma.Value,1) And Thisform.tienepagos=1 And Thisform.txtencontrado.Value='V'
			Thisform.mensaje="Este Documento Tiene Pagos Aplicados y no es Posible Cambiar la forma de pago"
			Return .F.
		Otherwise
			Return .T.
		Endcase
		
	ENDPROC

