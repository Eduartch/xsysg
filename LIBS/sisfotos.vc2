		LPARAMETERS nColIndex
		WITH thisform
		.txtruc.refresh
		.txtrazon.refresh
		.txtfono.refresh
		.txtfax.refresh
		.txtdireccion.refresh
		.txtciudad.refresh
		ENDWITH 
	ENDPROC

	PROCEDURE busca
		LOCAL ln,lp
		thisform.griclientes.retorna()
		thisform.cdcto=""
		WITH thisform
		if thisform.txtencontrado.value<>"V"
		   store 0 to r
		   x=0
		   ln=thisform.txtserie.value+thisform.txtnumero.value 
		   lp=thisform.txtcodigo.value  
		   ctdoc=thisform.tdoc 
		   TEXT TO lcConsulta NOSHOW 
		     SELECT a.idauto,a.alma,a.idkar,b.descri,b.unid,b.tipro,a.idauto,a.idart,a.cant,a.prec,a.incl,a.fech,c.fecr,c.form,c.exon,c.ndo2,a.igv,a.idprov,a.tipo,a.tdoc,a.ndoc,a.dola,a.mone FROM
		     fe_kar as a JOIN fe_rcom as c using(idauto) JOIN fe_art as b using(idart) WHERE a.ndoc=?ln AND a.tdoc=?ctdoc AND a.idprov=?lp
		   ENDTEXT
		   IF SQLEXEC(goapp.bdConn, lcconsulta, "kardex") < 1
		      errorbd(lcconsulta)
		     RETURN 
		   ENDIF 
		   SELECT kardex
		   GO TOP 
		   DO WHILE !EOF()
		        IF x=0
		           DELETE FROM tmpc WHERE coda=0
		           IF month(kardex.fecr)<>goapp.mes or year(kardex.fecr)<>val(goapp.año)
		              DO FORM v_verifica with "V" to verdad         
		              IF !verdad
		                .txtencontrado.value="X"
		                EXIT 
		               ELSE 
		                goapp.mes=month(kardex.fecr) 
		              ENDIF      
		          
		             *SELECT fe_sucu
		             *LOCATE FOR codt=kardex.alma
		             *.cmbalmacen.value=nomb
			       ENDIF 
		          .txtencontrado.value="V"
		          .txtfecha.value=kardex.fech
		          .txtfechar.value=kardex.fecr
		          .txtguia.value=kardex.ndo2
		          .txtdolar.value=kardex.dola
		          .optigv.optnoincluir.value=1
		          .optigv.optincluido.value=0
		          .txtencontrado.value="V"
		          .cmbmoneda.value=iif(kardex.mone="S","Soles","Dólares")
		          IF kardex.exon="E"
		             .optexoneraciones.optexonerada.value=1
		             .optexoneraciones.optnoexonerada.value=0
		            ELSE
		             .optexoneraciones.optexonerada.value=0
		             .optexoneraciones.optnoexonerada.value=1
		          ENDIF    
				  IF kardex.incl="I"
		             .optigv.optnoincluir.value=0
		             .optigv.optincluido.value=1
		           ELSE 
		             .optigv.optnoincluir.value=1
		             .optigv.optincluido.value=0
		          ENDIF     
		*		  .txtporcentaje.value=kardex.npor
		          .cmbforma.listindex=IIF(kardex.form="C",2,1)
			      .calmacen=kardex.alma
		          thisform.cdcto=kardex.idauto
		         ENDIF 
		         SELECT kardex
			      x=x+1
		         .txtitem.value=.txtitem.value+1
		*        .txtpercepcion.value=kardex.npper
			*    IF kardex.npper>0
		 *		     thisform.chkpercepcion.value=1
		 *		    ELSE
		*             thisform.chkpercepcion.value=0
		*		  ENDIF 
		    	  INSERT INTO tmpc(coda,desc,unid,cant,prec,nreg,caan,prea,dsnc,dsnd,gast,nitem,tipro)values;
		    	  (kardex.idart,kardex.descri,kardex.unid,kardex.cant,IIF(kardex.incl="I",kardex.prec*kardex.igv,kardex.prec),kardex.idkar,kardex.cant,;
		    	  IIF(kardex.incl="I",kardex.prec*kardex.igv,kardex.prec),1,1,1,.txtitem.value,kardex.tipro)
			      SELECT  kardex
			      SKIP 
			  ENDDO   
		ENDIF  
		SELECT tmpc
		IF .txtencontrado.value="V"
		   .grivta.refresh
		   .grivta.suma()
		   .grivta.column1.text1.setfocus
		ENDIF 	       
		RETURN .txtencontrado.value
		ENDWITH 		
	ENDPROC

	PROCEDURE continua
		IF !EMPTY(thisform.txtserie.value) or !EMPTY(thisform.txtnumero.value) or !EMPTY(thisform.txtruc.value)
		    if thisform.tdoc="01" or thisform.tdoc="09"
		       if len(trim(thisform.txtruc.value))<11
		          thisform.txtruc.setfocus
		          RETURN 
		        ENDIF 
		     ENDIF     
		     GO TOP 
		     IF BOF() and EOF()
		         APPEND BLANK 
		         thisform.nreg=recno()
		         thisform.txtitem.value=1
		      ENDIF    
		      thisform.grivta.column1.text1.setfocus
		    ELSE 
		      thisform.txtserie.setfocus  
		ENDIF  
	ENDPROC

	PROCEDURE retorna
		WITH thisform
		.txtruc.controlsource=""
		.txtrazon.controlsource=""
		.txtdireccion.controlsource=""
		.txtciudad.controlsource=""
		.txtfono.controlsource=""
		.txtfax.controlsource=""
		.griclientes.recordsource=""
		.griclientes.visible=.f.
		.txtbuscar.value=""
		.txtbuscar.visible=.f.
		ENDWITH 
		
	ENDPROC

	PROCEDURE Column1.Text1.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		
	ENDPROC

ENDDEFINE

