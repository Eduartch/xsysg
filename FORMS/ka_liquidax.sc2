			.grimodelos1.RecordSource=""
			This.Parent.grid2.RecordSource=""
			This.Parent.grid3.RecordSource=""
			nidusuario=lu.idusua
			dfecha1=Ctod("01/10/2018")
			dfecha2=dfecha-1
			f1=dfecha1
			f2=dfecha2 
			TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
			    SUM(if(a.lcaj_deud<>0,lcaj_deud,0)) as ingresoss,SUM(if(a.lcaj_acre<>0,lcaj_acre,0)) as egresoss
			    FROM fe_lcaja  as a WHERE  a.lcaj_fech between '<<f1>>' and '<<f2>>'  and a.lcaj_acti='A' and a.lcaj_form='E' and lcaj_idus=<<nidusuario>> group by lcaj_idus
