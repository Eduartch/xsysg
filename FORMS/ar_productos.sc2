	PROCEDURE Pgf1.Page2.Cmdanterior1.Click
		This.ctabla="lpro"
		Local calias
		calias=This.ctabla
		If Used((calias)) Then
			Select (calias)
			If Bof() Then
				Go Bottom
			Else
				If !Bof() Then
					Skip -1
					Thisform.codigo=lpro.idart
					thisform.pgf1.page2.txt1.Value=lpro.descri
					Thisform.pgf1.page2.cmdejecutar1.Click()
				Endif
			Endif
		Endif
		
	ENDPROC

	PROCEDURE Pgf1.Page2.Cmdejecutar1.Click
		Thisform.pgf1.page2.grimodelos.RecordSource=""
		ccoda=Thisform.codigo
		Thisform.pgf1.page2.txt2.Value=0
		If Thisform.pgf1.page2.chkmoneda.Value=0 Then
			na=Val(Thisform.pgf1.page2.cmbaño.Value)
			TEXT TO lc NOSHOW
				    SELECT b.razo,c.fech,cant,ROUND(prec*c.vigv,2) as prec,ifnull(y.mone,c.mone) as mone,ifnull(y.tdoc,c.tdoc) as tdoc,ifnull(y.ndoc,c.ndoc) as ndoc,
					MONTH(c.fech) as mes,x.guic_idac FROM fe_kar as a INNER JOIN fe_rcom  as c
					ON(c.idauto=a.idauto) inner join fe_prov as b ON (b.idprov=c.idprov)
					left join vguiascompras as x on x.guic_idau=c.idauto left join vregcompras as y on y.idauto=x.guic_idac WHERE idart=?ccoda
					AND a.tipo='C' and c.acti<>'I' and a.acti='A' and YEAR(c.fech)=?na order by c.fech desc;
			ENDTEXT
		Else
			dfi=Thisform.pgf1.page2.txtfechainicial.Value
			dff=Thisform.pgf1.page2.txtfechafinal.Value
			TEXT TO lc NOSHOW
				    SELECT b.razo,c.fech,cant,ROUND(prec*c.vigv,2) as prec,ifnull(y.mone,c.mone) as mone,ifnull(y.tdoc,c.tdoc) as tdoc,
				    ifnull(y.ndoc,c.ndoc) as ndoc,MONTH(c.fech) as mes,x.guic_idac
					FROM fe_kar as a INNER JOIN fe_rcom  as c
					ON(c.idauto=a.idauto) inner join fe_prov as b ON (b.idprov=c.idprov)
					left join vguiascompras as x on x.guic_idau=c.idauto left join vregcompras as y on y.idauto=x.guic_idac
		            WHERE idart=?ccoda AND a.tipo='C' and c.acti<>'I' and a.acti='A'
				    and c.fech between ?dfi and ?dff order by c.fech desc;
			ENDTEXT
		ENDIF
		IF verificaconexion()=0 then
		RETURN 0
		endif
		If SQLExec(goapp.bdconn,lc,"uc")<1
			errorbd(lc)
			Return
		Endif
		Select razo,fech,cant,Prec,mone,Iif(mone='D',Round(Prec*fe_gene.dola,2),Prec) As precios,tdoc,ndoc,mes,guic_idac From uc Into Cursor uc
		Select Avg(precios) As promedio,SUM(cant) as tcant From uc Into Cursor el
		With Thisform.pgf1.page2
			.txt2.Value=el.promedio
			.txt3.Value=el.tcant
			.grimodelos.RecordSource="uc"
			.grimodelos.SetAll("dynamicbackcolor","iif(MOD(RECNO(),2)=0,RGB(233,233,233),RGB(247,247,240))","Column")
			.grimodelos.SetFocus
		Endwith
		
	ENDPROC

