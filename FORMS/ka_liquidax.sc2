			.txtingresoscd.Value=0
			.txttvtas.Value=0
			.txtingresosd.Value=0
			.txtegresosd.Value=0
			.txtsaldod.Value=0
			.txtsincajas.Value=0
			.txtsaldoa.Value=0
			.Ingresos=0
