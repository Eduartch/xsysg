	PROCEDURE Unload
		If Thisform.Foxy=1 Then
			Do foxypreviewer.App
		Endif
		
	ENDPROC

	PROCEDURE validar
		With Thisform
			Do Case
			Case  .txtserieg.Value="000"  Or .txtnumerog.Value="0000000" Or Len(Alltrim(.txtserieg.Value))<3  Or Len(Alltrim(.txtnumerog.Value))<7
				.mensaje="Ingrese el Un Nº de la Guia Valido"
				Return .F.
			Case .txtfechat.Value<.txtfecha.Value
				.mensaje="La Fecha de Traslado No Puede Ser Antes que la Fecha de Emisión"
				Return .F.
			Case Thisform.txtitem.Value<=0
				.mensaje="No Existen Items Para este Documento"
				Return .F.
			Case Empty(.txtrazont.Value)
				.mensaje="Faltan Datos Transporte"
				Return .T.
		   Otherwise
				Return .T.
			Endcase
		Endwith
		
	ENDPROC

	PROCEDURE Cmbalmacen.LostFocus
		thisform.calmacen=almacenes.codt
	ENDPROC

	PROCEDURE cmdaceptar.Click
		Local rpta,crp,nit
		Thisform.mensaje=""
		With Thisform
			If !Thisform.validar() Then
				Messagebox(Thisform.mensaje,16,MSGTITULO)
				Return
			Else
				If !Empty(Thisform.mensaje)
					Messagebox(Thisform.mensaje,16,MSGTITULO)
				Endif
			Endif
			rpta=Messagebox("¿Son Correctos Los Datos [Guia de Remisión Nº] <SI/NO>? "+.txtserieg.Value+.txtnumerog.Value,32+3+0,MSGTITULO)
			Do Case
			Case rpta=6
				rpta=Messagebox("¿Imprimir Guia de Remisión <SI/NO>? "+.txtserieg.Value+.txtnumerog.Value,32+3+0,MSGTITULO)
				If rpta=2
					Thisform.grivtag.SetFocus
					Return
				Endif
				If !Empty(.txtplaca.Value) And !Empty(.txtrazont.Value)
					nidtr=ProcesaTransportista(.txtruct.Value,.txtrazont.Value,.txtdirecciont.Value,;
						.txtbrevete.Value,.txtconstancia.Value,.txtmarca.Value,.txtplaca.Value,;
						.idtran,Iif(.verdad,1,0),.txtconductor.Value,goapp.nidusua,.txtplaca1.Value)
					If nidtr=0 Then
						Return
					Endif
				Else
					nidtr=0
				Endif
				If IniciaTransaccion()=0 Then
					Return
				Endif
				nidg=IngresaGuias(.txtfecha.Value,.txtpartida.Value,.txtllegada.Value,.Auto,.txtfechat.Value,;
					goapp.nidusua,.txtdetalle.Value,nidtr,.txtserieg.Value+.txtnumerog.Value)
				If nidg=0 Then
					DeshacerCambios()
					Return
				Endif
				Select tmpvg
				Go Top
				s=1
				Do While !Eof()
					If GrabaDetalleGuias(tmpvg.nidkar,tmpvg.cant,nidg)=0
						s=0
						Exit
					Endif
					Select tmpvg
					Skip
				Enddo
				If .generanumero()=0 OR s=0 Then
					DeshacerCambios()
					Return
				Else
					GRabarCambios()
					If rpta=6
						Select Count(*) As ti From tmpvg Into Cursor xitems
						.txtitem.Value=xitems.ti
						Select tmpvg
						Replace All ndoc With .txtserieg.Value+.txtnumerog.Value
						For x=1 To .Item-.txtitem.Value
							Insert Into tmpvg(ndoc,nitem)Values(.txtserieg.Value+.txtnumerog.Value,x)
						Next
						Select tmpvg
						Go Top
						Report Form (goapp.reporte) To Printer Prompt Noconsole
					Endif
				Endif
			Case rpta=2
				.grivtag.SetFocus
				Return
			Endcase
			.limpiar()
		Endwith
		
	ENDPROC

	PROCEDURE Cmdcerraro.Click
		Go Top In tmpvg
		If Thisform.txtpesototal.Value>0 And !Empty(Thisform.txtserieg.Value) And !Empty(Thisform.txtnumerog.Value)
			Thisform.cmdaceptar.Click
		Endif
		DoDefault()
		
	ENDPROC

