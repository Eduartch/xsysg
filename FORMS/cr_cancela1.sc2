				.grimodelos.RecordSource="txp"
				.cmbforma.Enabled= .F.
				.txtpago.ReadOnly= .T.
				.txtpago.Value=niden
				.txtnumero.SetFocus
			Endif
			.grimodelos.SetAll("dynamicbackcolor","iif(MOD(RECNO(),2)=0,RGB(233,233,233),RGB(247,247,242))","Column")
			.cmbmoneda1.ListIndex=Iif(.txtmoneda.Value="S",1,2)
		Endwith
		
	ENDPROC

	PROCEDURE Command2.Click
		If Left(goapp.tipousuario,1)<>'A' Then
			Do Form v_verifica With "A" To verdad
			If !verdad
				Thisform.txtfecha.ReadOnly= .T.
			Else
				Thisform.txtfecha.ReadOnly= .F.
				Thisform.txtfecha.SetFocus
			EndIf
		EndIf 	
		
