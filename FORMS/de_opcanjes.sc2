	ENDPROC

	PROCEDURE Pageframe1.Page2.grideudas.Column3.Text1.LostFocus
		REPLACE dias WITH this.value-thisform.PAgeframe1.PAge2.txtfecha.value IN tmpd
		
	ENDPROC

	PROCEDURE Pageframe1.Page2.grideudas.Column6.Text1.LostFocus
		Local r
		Select tmpd
		r=Recno()
		Sum Impo To Thisform.PAgeframe1.PAge2.txttotal.Value
		If r>0
			Go r
		Endif
		
	ENDPROC

