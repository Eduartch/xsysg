	PROCEDURE buscar
		With Thisform
			If .txtencontrado.Value<>"V"
				cndoc=.txtserie.Value+.txtnumero.Value
				ct=.tdoc
				nidprov=.txtcodigo.Value
				TEXT TO lc NOSHOW
			        SELECT a.dolar,a.fech,a.fecr,a.mone,a.idauto,a.vigv,a.valor,a.igv,a.impo,a.ndoc,a.deta,a.tcom,
			        b.impo as impo1,c.nomb,b.nitem,c.ncta,b.ecta_tipo,a.form,
			        b.idectas,ifnull(a.idcaja,0) as idcaja from vrcompras as a inner join 
			        fe_ectasc as b on b.idrcon=a.idauto inner join fe_plan as c on c.idcta=b.idcta
			        where a.ndoc=?cndoc and a.tdoc=?ct and a.idprov=?nidprov
				ENDTEXT
				IF verificaconexion()=0 then
		RETURN 0
		endif
				If SQLExec(goapp.bdconn,lc,"rc")<1
					errorbd(lc)
				Else
					If regdvto('rc')>0
						.txtencontrado.Value="V"
						.txtdolar.Value=rc.dolar
						.txtfecha.Value=rc.fech
						.txtfechar.Value=rc.fecr
						.txtfechavto.Value=rc.fecr
						.cmbMONEDA.ListIndex=Iif(rc.mone="S",1,2)
						.cmbforma.ListIndex=Iif(rc.Form='C',2,1)
						.formaregistrada=rc.Form
						.MonedaRegistrada=rc.mone
						cm=rc.mone
						Do Case
						Case rc.tcom='1' 
							.cmbtipo.ListIndex=1
						Case rc.tcom='2'
							.cmbtipo.ListIndex=2
						Case rc.tcom='3' 
							.cmbtipo.ListIndex=3
						Case rc.tcom='4'
							.cmbtipo.ListIndex=4
						Case rc.tcom='5'
							.cmbtipo.ListIndex=5
						Endcase
						.txtcorrelativo.Value=rc.idauto
						.nreg=rc.idauto
						If Validadeuda(rc.idauto)=0 Then
							.tienepagos=1
						Else
							.tienepagos=0
						Endif
						.idcaja=Iif(Vartype(rc.idcaja)='C',Val(rc.idcaja),rc.idcaja)
						x=0
						Select rc
						Do While !Eof()
							x=x+1
							If rc.Impo1<>0
								Do Case
								Case rc.nitem=1
									.txtvalor1.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.cmbctav1.Value=rc.ncta
									.txtctav1.Value=rc.nomb
									.cmbtipo1.ListIndex=Iif(rc.ecta_tipo='D',1,2)
									.idcta1=rc.idectas
								Case rc.nitem=2
									.txtvalor2.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.cmbctav2.Value=rc.ncta
									.txtctav2.Value=rc.nomb
									.cmbtipo2.ListIndex=Iif(rc.ecta_tipo='D',1,2)
									.idcta2=rc.idectas
								Case rc.nitem=3
									.txtvalor3.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.cmbctav3.Value=rc.ncta
									.txtctav3.Value=rc.nomb
									.cmbtipo3.ListIndex=Iif(rc.ecta_tipo='D',1,2)
									.idcta3=rc.idectas
								Case rc.nitem=4
									.txtvalor4.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.cmbctav4.Value=rc.ncta
									.txtctav4.Value=rc.nomb
									.cmbtipo4.ListIndex=Iif(rc.ecta_tipo='D',1,2)
									.idcta4=rc.idectas
								Case rc.nitem=5
									.txtigv.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.idcta5=rc.idectas
									.txtctai.Value=rc.nomb
									.cmbctai.Value=rc.ncta
									.cmbtipo5.ListIndex=Iif(rc.ecta_tipo='D',1,2)
								Case rc.nitem=6
									.txtexonerado.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.txtctae.Value=rc.nomb
									.cmbctae.Value=rc.ncta
									.idcta6=rc.idectas
									.cmbtipo6.ListIndex=Iif(rc.ecta_tipo='D',1,2)
								Case rc.nitem=7
									.txtotros.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.cmbctao.Value=rc.ncta
									.txtctao.Value=rc.nomb
									.idcta7=rc.idectas
									.cmbtipo7.ListIndex=Iif(rc.ecta_tipo='D',1,2)
								Case rc.nitem=8
									.txttotal.Value=Iif(rc.Impo1<0,Abs(rc.Impo1),rc.Impo1)
									.cmbctat.Value=rc.ncta
									.txtctat.Value=rc.nomb
									.idcta8=rc.idectas
									.cmbtipo8.ListIndex=Iif(rc.ecta_tipo='D',1,2)
									.MontoR=rc.Impo1
								Endcase
							Endif
							Select rc
							Skip
						Enddo
						.txtdetalle.Value=rc.Deta
					Endif
				Endif
			Endif
			Do Case
			Case Empty(.txtencontrado.Value)
				.txtvalor1.SetFocus
			Case .txtencontrado.Value="X"
				.txtencontrado.Value=""
				.cmbdcto.SetFocus
				Return
			Otherwise
				.txtvalor1.SetFocus
			Endcase
		Endwith
		
	ENDPROC

	PROCEDURE cargarctas
		If goapp.autorizactas<>'S' Then
			Select ncta,idcta,nomb From lctas Where Left(ncta,2)='63' Or Left(ncta,2)='65' OR Left(ncta,2)='60'  Into Cursor ctasv1
			Select ncta,idcta,nomb From lctas Where Left(ncta,2)='63' Or Left(ncta,2)='65'  Into Cursor ctasv2
			Select ncta,idcta,nomb From lctas Where Left(ncta,2)='63' Or Left(ncta,2)='65'  Into Cursor ctasv3
			Select ncta,idcta,nomb From lctas Where Left(ncta,2)='63' Or Left(ncta,2)='65'  Into Cursor ctasv4
			Select ncta,idcta,nomb From lctas Where Left(ncta,5)='40.11' Into Cursor ctasi
			Select ncta,idcta,nomb From lctas Where Left(ncta,2)='63' Into Cursor ctase
			Select ncta,idcta,nomb From lctas Where Left(ncta,2)='00' Into Cursor ctaso
			Select ncta,idcta,nomb From lctas Where Left(ncta,2)='42' Or Left(ncta,2)='46' Into Cursor ctast
		Else
			Select ncta,idcta,nomb From lctas Into Cursor ctasv1
			Select ncta,idcta,nomb From lctas Into Cursor ctasv2
			Select ncta,idcta,nomb From lctas Into Cursor ctasv3
			Select ncta,idcta,nomb From lctas Into Cursor ctasv4
			Select ncta,idcta,nomb From lctas Into Cursor ctasi
			Select ncta,idcta,nomb From lctas Into Cursor ctase
			Select ncta,idcta,nomb From lctas Into Cursor ctaso
			Select ncta,idcta,nomb From lctas Into Cursor ctast
		Endif
		With Thisform
			.cmbctav1.RowSource="ctasv1.ncta"
			.cmbctav2.RowSource="ctasv2.ncta"
			.cmbctav3.RowSource="ctasv3.ncta"
			.cmbctav4.RowSource="ctasv4.ncta"
			.cmbctae.RowSource="ctase.ncta"
			.cmbctai.RowSource="ctasi.ncta"
			.cmbctao.RowSource="ctaso.ncta"
			.cmbctat.RowSource="ctast.ncta"
			Select ctasv1
			Locate For idcta=fe_gene.idctacv
			If Found()
				.cmbctav1.Value=ctasv1.ncta
				.txtctav1.Value=ctasv1.nomb
			Else
				.cmbctav1.ListIndex=1
			Endif
			.idcta1=ctasv1.idcta
			.cmbtipo1.ListIndex=1
			Select ctasi
			Locate For idcta=fe_gene.idctaci
			If Found()
				.cmbctai.Value=ctasi.ncta
				.txtctai.Value=ctasi.nomb
			Else
				.cmbctai.ListIndex=1
			Endif
			.idcta6=ctasi.idcta
			.cmbtipo6.ListIndex=1
			Select ctast
			Locate For idcta=fe_gene.idctact
			If Found()
				.cmbctat.Value=ctast.ncta
				.txtctat.Value=ctast.nomb
			Else
				.cmbctat.ListIndex=1
			Endif
			.idcta8=ctast.idcta
			.cmbtipo8.ListIndex=2
			.cmbtipo1.ListIndex=1
			.cmbctav2.ListIndex=1
			.cmbctav3.ListIndex=1
			.cmbctav4.ListIndex=1
			.cmbctao.ListIndex=1
			.cmbctae.ListIndex=1
			.idcta1=ctasv1.idcta
			.idcta2=ctasv2.idcta
			.idcta3=ctasv3.idcta
			.idcta4=ctasv4.idcta
			.idcta5=ctase.idcta
			.idcta6=ctasi.idcta
			.idcta7=ctaso.idcta
			.idcta8=ctast.idcta
			.cmbtipo2.ListIndex=1
			.cmbtipo3.ListIndex=1
			.cmbtipo4.ListIndex=1
			.cmbtipo5.ListIndex=1
			.cmbtipo7.ListIndex=1
			.txtctav1.ControlSource="ctasv1.nomb"
			.txtctav2.ControlSource="ctasv2.nomb"
			.txtctav3.ControlSource="ctasv3.nomb"
			.txtctav4.ControlSource="ctasv4.nomb"
			.txtctaE.ControlSource="ctase.nomb"
