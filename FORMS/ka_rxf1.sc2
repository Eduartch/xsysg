	PROCEDURE Init
		Settear()
		If datosGlobales()=0 Then
			Return .F.
		Endif
		This.GRimodelos.RecordSource=""
		If MuestraDctos("")=0 Then
			Return .F.
		Endif
		If MuestraUsuarios1("",0,0,'lu')=0 Then
			Return .F.
		Endif
		Select * From dctosv Into Cursor ldctos
		With This
			.cmbdcto.RowSource="ldctos.nomb"
			.cmbdcto.ListIndex=1
			.cmbmoneda.ListIndex=1
			.cmbalmacen.RowSource="lu.nomb"
			.cmbalmacen.ListIndex=1
			.GRimodelos.RecordSource=""
		Endwith
		
	ENDPROC

	PROCEDURE opciones
		Lparameters opt
		Set Procedure To capadatos,enviarcorreo,ple5,imprimir,rngrifo Additive
		If opt=1 Then
			ReimprimirStandargrifo(rmvtos.idauto,rmvtos.tdoc,rmvtos.tcom)
			Do Form ka_ldctos1 To verdad
			obji=Createobject("Imprimir")
			obji.tdoc=rmvtos.tdoc
			Select tmpv
			Set Filter To !Empty(Desc)
			Go Top
			obji.ImprimeComprobanteComoTicket('S')
		Else
			ReimprimirStandargrifo(rmvtos.idauto,rmvtos.tdoc,rmvtos.tcom)
			car=Justfname(rmvtos.rcom_arch)
			npos=At(".",car)
			carpdf=Left(car,npos-1)+'.Pdf'
			obji=Createobject("Imprimir")
			obji.tdoc=rmvtos.tdoc
			obji.ImprimeComprobante('N')
			obji.ArchivoPdf=carpdf
			obji.GeneraPDF('S')
		Endif
		
	ENDPROC

