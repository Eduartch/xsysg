	PROCEDURE grabar
		Local npagina,dfecha,nimporte,cdetalle,ctipo,cforma,cmone,Cem,nimportep
		Cajas=''
		nimportep=0
		With This
			npagina=.pgfmvtos.ActivePage
			cidpc=Id()
			Do Case
			Case npagina=1
				cndoc=.pgfmvtos.pgingresos.txtdcto.Value
				dfecha=.pgfmvtos.pgingresos.txtfecha.Value
				nimporte=.pgfmvtos.pgingresos.txtimporte.Value
				cdetalle=.pgfmvtos.pgingresos.txtdetalle.Value
				cforma='E'
				cmone=Left(.pgfmvtos.pgingresos.cmbmoneda.Value,1)
				ctipo="I"
				ne=Thisform.codproyectoi
				ndebe=.pgfmvtos.pgingresos.txtimporte.Value
				nhaber=0
				Cem=''
				nidcajero=goapp.idcajero
				Thisform.nidauto=0
			Case npagina=2
				cndoc=.pgfmvtos.pggastos1.txtdcto.Value
				dfecha=.pgfmvtos.pggastos1.txtfecha.Value
				nimporte=.pgfmvtos.pggastos1.txtimporte.Value
				cdetalle=.pgfmvtos.pggastos1.txtdetalle.Value
				cforma='E'
				cmone=Left(.pgfmvtos.pggastos1.cmbmoneda.Value,1)
				nhaber=.pgfmvtos.pggastos1.txtimporte.Value
				ndebe=0
				ctipo="S"
				ne=Thisform.codproyectoe
				nidcajero=lus.idusua
				Cem=''
			Case npagina=3
				cndoc="TRANSFER."
				dfecha=.pgfmvtos.pgt.txtfecha.Value
				nimporte=.pgfmvtos.pgt.txtimporte.Value
				nidcajero1=lusuat.idusua
				cforma=Left(.pgfmvtos.pgt.cmbforma1.Value,1)
				cmone="S"
				ctipo="I"
				ne=0
				.nidauto=0
				Cem='S'
				nimportep=.pgfmvtos.pgt.txttpedidos.Value
				nhaber=0
				ndebe=.pgfmvtos.pgt.txtimporte.Value
			Endcase
			Set Procedure To capadatos,rngrifo Additive
			If Iniciatransaccion()=0 Then
				DeshacerCambios()
				Return
			Endif
			If npagina<>3 Then
				nidx=IngresaDatosLCajaEFectivoCturnosDvto(dfecha,"",cdetalle,0,ndebe,nhaber,;
					'S',fe_gene.dola,nidcajero,0,0,'E',cndoc,"",goapp.isla,fe_gene.alma_sepa)
				If nidx=0 Then
					DeshacerCambios()
					Return
				Else
					If npagina=2 And This.pgfmvtos.pggastos1.chkcargo.Value=1 Then
						If IngresaSueldosYPagos(0,nhaber,dfecha,'P',goapp.nidusua,nidx,nidcajero,Alltrim(cndoc)+' '+Alltrim(cdetalle) )=0 Then
							DeshacerCambios()
							Return
						Endif
					Endif
				Endif
			Else
				cdetalle="Tr.de "+Alltrim(goapp.usuario)+ "-"+ Alltrim(.pgfmvtos.pgt.cmbcajero1.Value)+' '+Left(.pgfmvtos.pgt.txtdetalle.Value,50)
				nidx=IngresaDatosLCajaEFectivoCturnos20Transferencia(dfecha,"",cdetalle,0,0,ndebe,;
					'S',fe_gene.dola,goapp.idcajero,0,0,'E',cndoc,"",goapp.isla,fe_gene.alma_sepa,0,0)
				If nidx=0 Then
					DeshacerCambios()
					Return
				Endif
				If IngresaDatosLCajaEFectivoCturnos20Transferencia(dfecha,"",cdetalle,0,ndebe,0,;
						'S',fe_gene.dola,nidcajero1,0,0,'E',cndoc,"",goapp.isla,fe_gene.alma_sepa,0,nidx)=0 Then
					DeshacerCambios()
					Return
				Endif
				If ActualizaIdTransferenciaCaja(nidx)=0 Then
					DeshacerCambios()
					Return
				Endif
				If npagina=2 Then
					If Val(Substr(This.pgfmvtos.pggastos1.txtdcto.Value,5))>=This.nsgte
						If GeneraCorrelativo(Val(Substr(This.pgfmvtos.pggastos1.txtdcto.Value,5))+1,This.idserie)=0 Then
							Return
						Endif
					Endif
				Endif
			Endif
			Grabarcambios()
			If npagina=3 Then
				ImprimeTransferencia(nidx)
			Endif
		Endwith
		
	ENDPROC

