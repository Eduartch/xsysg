	PROCEDURE buscar
		Local ln,lp
		Thisform.cdcto=""
		With Thisform
			If .txtencontrado.Value<>"V"
				Store 0 To r
				x=0
				ln=.txtserie.Value+.txtnumero.Value
				lp=.txtcodigo.Value
				ctdoc=.tdoc
				TEXT TO lc NOSHOW
				     SELECT a.idauto,a.alma,a.idkar,a.descri,a.unid,a.tipro,a.idauto,a.idart,a.valor,a.igv,a.impo,a.incl,
				     a.cant,a.prec,a.fech,a.fecr,a.form,a.exon,a.ndo2,a.vigv as igv,a.idprov,a.tipo,a.tdoc,a.ndoc,cast(dolar as decimal(6,4)) as dolar,a.mone from vmuestracompras as a
				     WHERE a.ndoc=?ln AND a.tdoc=?ctdoc AND a.idprov=?lp
				ENDTEXT
					IF verificaconexion()=0 then
		RETURN 0
		endif
				ncon=AbreConexion()
				If SQLExec(ncon, Lc,"kardex") < 1
					errorbd(Lc)
					Return
				Endif
				CierraConexion(ncon)
				If regdvto("kardex")>=1 Then
					Messagebox("Documento de Compra Ya Registrado",16,MSGTITULO)
				Endif
				Select kardex
				Go Top
				Do While !Eof()
					If x=0
						Delete From tmpc Where coda=0
						.txtencontrado.Value="V"
						.txtfecha.Value=kardex.fech
						.txtfechar.Value=kardex.fecr
						.txtguia.Value=kardex.ndo2
						.fechaingreso=kardex.fech
						.txtdolar.Value=kardex.dolar
						.txtencontrado.Value="V"
						.cmbmoneda.Value=Iif(kardex.mone="S","Soles","Dólares")
						If kardex.exon="E"
							.optexoneraciones.optexonerada.Value=1
							.optexoneraciones.optnoexonerada.Value=0
						Else
							.optexoneraciones.optexonerada.Value=0
							.optexoneraciones.optnoexonerada.Value=1
						Endif
						If kardex.Incl="I"
							.optigv.optnoincluir.Value=0
							.optigv.optincluido.Value=1
						Else
							.optigv.optnoincluir.Value=1
							.optigv.optincluido.Value=0
						Endif
						.cmbforma.ListIndex=Iif(kardex.Form="C",2,1)
						.calmacen=kardex.alma
						.cdcto=kardex.idauto
						.txtvalor.Value=kardex.valor
						.txtigv.Value=kardex.igv
						.txttotal.Value=kardex.Impo
						Select calmacenes
						Locate For idalma=kardex.alma
						.cmbalmacen.Value=calmacenes.nomb
					Endif
					Select kardex
					x=x+1
					.txtitem.Value=.txtitem.Value+1
					Insert Into tmpc(coda,Desc,unid,cant,Prec,nreg,dsnc,dsnd,gast,nitem,tipro)Values;
						(kardex.idart,kardex.Descri,kardex.unid,kardex.cant,Iif(kardex.Incl="I",kardex.Prec*kardex.igv1,kardex.Prec),kardex.idkar,;
						1,1,1,.txtitem.Value,kardex.tipro)
					Select  kardex
					Skip
				Enddo
			Endif
			Select tmpc
			If .txtencontrado.Value="V"
				.grivta.SetFocus
			Else
				.cmdotro.Click()
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE calcularcostosactuales
		Local nd,np1
		nd=Thisform.txtdolar.Value
		If Thisform.optigv.optnoincluir.Value=1 Then
			ni=config.igv
		Else
			ni=1
		Endif
		Select tmpc
		Scan All
			np1=tmpc.Prec*((100-tmpc.d1)/100*(100-tmpc.d2)/100*(100-tmpc.d3)/100)*ni
			Replace tmpc.costoact With Round(Iif(Thisform.cmbmoneda.ListIndex=2,(np1*nd)+tmpc.gast,np1+tmpc.gast),2) In tmpc
		Endscan
		
		
	ENDPROC

