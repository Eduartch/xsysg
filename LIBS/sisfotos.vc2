	ENDPROC

	PROCEDURE Cmbmoneda.GotFocus
		if empty(thisform.txtruc.value)
		   thisform.txtruc.setfocus
		   return
		endif   
	ENDPROC

	PROCEDURE txtnumero.LostFocus
		local encontrado
		encontrado=0
		this.value=right("0000000"+alltrim(this.value),7)
		sele fe_kar
		SET ORDER TO kar_tdoc 
		seek thisform.txtserie.value+this.value
		do whil .t.
		    if fe_kar.ndoc<>thisform.txtserie.value+this.value and fe_kar.tdoc<>left(thisform.cmbdcto.value,2)
		       exit
		    endif  
		    if fe_kar.tipo="V" and fe_kar.ndoc=thisform.txtserie.value+this.value and fe_kar.tdoc=left(thisform.cmbdcto.value,2) and ttip="k"
		       thisform.txttotal1.value=thisform.txttotal1.value+round(cant*prec*npor,2)
		       thisform.txtvalor1.value=round(thisform.txttotal1.value/igv,2)
		       thisform.txtigv1.value=round(thisform.txttotal1.value-thisform.txtvalor1.value,2)
		    endif  
		    sele fe_kar
		    if eof()
		       exit
		      else
		       skip
		     endif    
		endd  
		
		
		
	ENDPROC

	PROCEDURE txtserie.LostFocus
		This.Value=Right("000"+Alltrim(This.Value),3)
		If Left(Thisform.cmbdcto.Value,2)='07' Then
			If BuscarSeries(Val(This.Value),'07')=0 Then
				This.Value=""
				Thisform.txtnumero.Value=""
				Thisform.cmbdcto.SetFocus
				Return
			Else
				Thisform.txtnumero.Value=Alltrim(Str(series.nume))
				Thisform.Item=series.items
				Thisform.nsgte=series.nume
				Thisform.idserie=series.idserie
			Endif
		Endif
		
	ENDPROC

	PROCEDURE txtvalor.InteractiveChange
		thisform.txtigv.value=round(this.value*(fe_gene.igv-1),2)
		thisform.txttotal.value=round(this.value+thisform.txtigv.value,2)
	ENDPROC

