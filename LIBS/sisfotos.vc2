DEFINE CLASS txtbuscar AS textbox 
 	*< CLASSDATA: Baseclass="textbox" Timestamp="" Scale="Pixels" Uniqueid="" />

	#INCLUDE "..\include\sisfotos.h"
	BackColor = 255,255,255
	BorderColor = 192,192,192
	FontBold = .T.
	FontName = "Tahoma"
	FontSize = 8
	Format = "!"
	Height = 21
	MaxLength = 40
	Name = "txtbuscar"
	SelectOnEntry = .T.
	SpecialEffect = 1
	StatusBarText = "Digite el Nombre Para Localizar"
	Visible = .F.
	Width = 416
	
	PROCEDURE KeyPress
		Lparameters nKeyCode, nShiftAltCtrl
		Local cvalor
		If nKeyCode=13 And !Empty(This.Value)
			Thisform.cbusca=This.Value
			cvalor='%'+Alltrim(This.Value)+'%'
			Thisform.griart.RecordSource=""
			nd=fe_gene.dola
			If MuestraProductos1(cvalor,nd,"lproductos")<0 Then
				Return
			Endif
			If Regdvto("lproductos")=0 Then
				Thisform.nohay=.T.
				Messagebox(NOHAYDATOS,16,MSGTITULO)
			Else
				With Thisform
					.nohay=.F.
					.griart.RecordSource="lproductos"
					.griart.column1.ControlSource="lproductos.descri"
					.griart.column2.ControlSource="lproductos.unid"
					.griart.column3.ControlSource="lproductos.pre1"
					.griart.column4.ControlSource="lproductos.pre2"
					.griart.column5.ControlSource="lproductos.pre3"
					.griart.column6.ControlSource="lproductos.uno"
					.griart.column7.ControlSource="lproductos.dos"
					.griart.column8.ControlSource="lproductos.tre"
					.griart.column9.ControlSource="lproductos.cua"
					.griart.SetAll("dynamicbackcolor","iif(MOD(RECNO(),2)=0,RGB(234,234,255),RGB(247,247,247))","Column")
				Endwith
			Endif
		Endif
		
	ENDPROC

ENDDEFINE

