	PROCEDURE cmbdcto.LostFocus
		This.BackColor=Rgb(255,255,255)
		Thisform.tdoc=dcc.tdoc
		If Thisform.tdoc='GI' Then
			Thisform.cmbalmacen.ListIndex=2
			thisform.calmacen=2
		Else
			Thisform.cmbalmacen.ListIndex=1
			thisform.calmacen=1
