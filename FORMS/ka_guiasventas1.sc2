	PROCEDURE ad
		Lparameters l
		LOCAL x as Logical 
		IF l=0 THEN 
		   x=.t.
		  ELSE
		   x=.f.
		ENDIF      
		With Thisform
			.txtrazont.ReadOnly=x
			.txtdirecciont.ReadOnly=x
			.txtconductor.ReadOnly=x
			.txtmarca.ReadOnly=x
			.txtruct.ReadOnly=x
			.txtplaca1.ReadOnly=x
			.txtconstancia.ReadOnly=x
			.txtbrevete.ReadOnly=x
		Endwith
		
	ENDPROC

	PROCEDURE generanumero
		IF VAL(thisform.txtnumerog.Value)>=thisform.nsgte 
		   If GeneraCorrelativo(VAL(thisform.txtnumerog.Value)+1,thisform.idserie)=0 then 
		      Return 0
		     Else 
		      Return 1
		   EndIf 
		  Else 
		   Return 1
		ENDIF
	ENDPROC

	PROCEDURE Init
		Lparameters nidauto,xguia
		Settear()
		If datosGlobales()=0 Then
			Return .F.
		Endif
		If MuestraAlmacenes()=0 Then
			Return .F.
		Endif
		Thisform.grivtag.RecordSource=""
		Create Cursor tmpvg(coda N(8),Descri c(40),unid c(4),cant N(10,2),Prec N(10,2),uno N(10,2),Dos N(10,2),;
			peso N(10,2),alma N(10,2),ndoc c(10),nreg N(10),codc c(5),tref c(2),Refe c(10),fecr d,Valida c,nitem N(3),saldo N(10,2),idin N(8),nidkar N(10))
		With Thisform
			nalma=goapp.tienda
			Select almacenes
			Locate For idalma=nalma
			.txtpartida.Value=Alltrim(almacenes.Dire)+' '+Alltrim(almacenes.ciud)
			.idtran=0
		Endwith
		
	ENDPROC

	PROCEDURE limpiar
		Zap In tmpvg
		With Thisform
			.entregas="F"
			.verdad=.F.
			.encontrado="F"
			.txtserie.Value=""
			.txtnumero.Value=""
			.txtfechaf.Value=""
			.txtruc.Value=""
			.txtpesototal.Value=0
			.encontrado=""
			.txtserieg.Value=""
			.txtnumerog.Value=""
			.txtrazon.Value=""
			.txtdireccion.Value=""
			.txtllegada.Value=""
			.txtplaca.Value=""
			.txtrazont.Value=""
			.txtdirecciont.Value=""
			.txtconductor.Value=""
			.txtmarca.Value=""
			.txtruct.Value=""
			.txtplaca1.Value=""
			.txtconstancia.Value=""
			.txtbrevete.Value=""
			.txtdetalle.Value=""
			.chktransporte.Value=0
			.txtbrevete.Value=""
			.cmdeliminar.Enabled=.F.
			.ad(1)
		Endwith
	ENDPROC

	PROCEDURE validar
		With Thisform
			Do Case
				Case .txtserieg.Value="000"  Or .txtnumerog.Value="0000000" OR LEN(ALLTRIM(.txtserieg.Value))<3  Or LEN(ALLTRIM(.txtnumerog.Value))<7
					.mensaje="Ingrese el Un Nº de la Guia Valido"
					Return .F.
				Case .txtfechat.Value<.txtfecha.Value
					.mensaje="La Fecha de Traslado No Puede Ser Antes que la Fecha de Emisión"
					Return .F.
				Case Empty(.txtrazont.Value)
					.mensaje="Faltan Datos Transporte"
					Return .T.
				Otherwise
					Return .T.
			Endcase
		Endwith
	ENDPROC

	PROCEDURE Cmbalmacen.LostFocus
		thisform.calmacen=almacenes.codt
	ENDPROC

	PROCEDURE cmdaceptar.Click
		Local rpta,crp,nit
		Thisform.mensaje=""
		With Thisform
			If !Thisform.validar()
				Messagebox(Thisform.mensaje,16,MSGTITULO)
				Return
			Else
				If !Empty(Thisform.mensaje)
					Messagebox(Thisform.mensaje,16,MSGTITULO)
				Endif
			Endif
			rpta=Messagebox("¿Son Correctos Los Datos [Guia de Remisión Nº]? "+.txtserieg.Value+.txtnumerog.Value,32+3+0,MSGTITULO)
			Do Case
			Case rpta=6
				rpta=Messagebox("¿Desea Imprimir Guia de Remisión? "+.txtserieg.Value+.txtnumerog.Value,32+3+0,MSGTITULO)
				If rpta=2
					Thisform.grivtag.SetFocus
					Return
				Endif
				If !Empty(.txtplaca.Value) And !Empty(.txtrazont.Value)
					nidtr=ProcesaTransportista(.txtruct.Value,.txtrazont.Value,.txtdirecciont.Value,;
						.txtbrevete.Value,.txtconstancia.Value,.txtmarca.Value,.txtplaca.Value,;
						.idtran,Iif(.verdad,1,0),.txtconductor.Value,goapp.nidusua,.txtplaca1.Value)
					If nidtr=0 Then
						Return
					Endif
				Else
					nidtr=0
				Endif
				If IniciaTransaccion()=0 Then
					Return
				Endif
				nidg=IngresaGuias(.txtfecha.Value,.txtpartida.Value,.txtllegada.Value,.Auto,.txtfechat.Value,;
					goapp.nidusua,.txtdetalle.Value,nidtr,.txtserieg.Value+.txtnumerog.Value)
				If nidg=0 Then
					DeshacerCambios()
					Return
				Endif
				Select tmpvg
				Go Top
				s=1
				Do While !Eof()
					If GrabaDetalleGuias(tmpvg.nidkar,tmpvg.cant,nidg)=0
						s=0
						Exit
					Endif
					If IngresaEntregas(tmpvg.cant,tmpvg.idin,nidg)=0 Then
						s=0
						Exit
					Endif
					Select tmpvg
					Skip
				Enddo
				If .generanumero()=0 Then
					s=0
				Endif
				If s=0 Then
					DeshacerCambios()
					Return
				Else
					GRabarCambios()
					If rpta=6
						.cmdeliminar.Enabled=.F.
						Select Count(*) As ti From tmpvg Into Cursor xitems
						.txtitem.Value=xitems.ti
						Select tmpvg
						Replace All ndoc With .txtserieg.Value+.txtnumerog.Value
						For x=1 To .Item-.txtitem.Value
							Insert Into tmpvg(ndoc,nitem)Values(.txtserieg.Value+.txtnumerog.Value,x)
						Next
						Select tmpvg
						Go Top
						Report Form (goapp.reporte) To Printer Noconsole
					Endif
				Endif
			Case rpta=2
				.grivtag.SetFocus
				Return
			Endcase
			.limpiar()
		Endwith
		
	ENDPROC

	PROCEDURE Cmdcerraro.Click
		Go Top In tmpvg
		If Thisform.txtpesototal.Value>0 And !Empty(Thisform.txtserieg.Value) And !Empty(Thisform.txtnumerog.Value)
			Thisform.cmdaceptar.Click
		Endif
		DoDefault()
		
	ENDPROC

