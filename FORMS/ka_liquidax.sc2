			Select ndoc,tipo,Impo,Forma,deta From icaja Into Cursor icajas Readwrite
			Select icajas
			Append From Dbf("ivtas")
			Go Top
			Report Form infliquidag To Printer Prompt Noconsole
		Endif
		
	ENDPROC

