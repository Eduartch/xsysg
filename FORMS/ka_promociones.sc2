	PROCEDURE Pagef1.Page1.Grimodelos1.Column5.Cmdconsulta1.Click
		Local r As Integer
		r=Messagebox("¿Anular la Promoción <Si/No>?",32+3+0,MSGTITULO)
		If r=6 Then
			If ActualizaPromocion(rpromo.rpro_idrp)=1 Then
				Messagebox("Desactivado Correctamente",64,MSGTITULO)
				thisform.ConsultarPromociones()
			Endif
		Endif
		
	ENDPROC

	PROCEDURE Pagef1.Page2.Cmdaceptar1.Click
		Local rpta As Integer
		Select Sum(cant*equi) As cant From u1 Into Cursor cpr
		If !Thisform.validar() Then
			Messagebox(Thisform.mensaje,16,MSGTITULO)
			Return
		Endif
		rpta=Messagebox(MSGGUARDA,32+3+0,MSGTITULO)
		If rpta=6 Then
			dfechai=Thisform.pagef1.page2.txtfechainicial.Value
			dfechaf=Thisform.pagef1.page2.txtfechafinal.Value
			nidart=Thisform.codigo
			ncant=Thisform.pagef1.page2.txtcantidad.Value
			cunid=Thisform.pagef1.page2.cmbunidad.Value
			nequi=1
			ncosto=0
			cnombre=Thisform.pagef1.page2.txtdescripcion.Value
			sw=1
			If IniciaTransaccion()=0 Then
				DeshacerCAMBIOS()
				Return
			Endif
			nid=registraCabeceraPromociones(dfechai,dfechaf,nidart,0,ncant,0,cunid,nequi,ncosto,'P',cnombre)
			If nid=0 Then
				DeshacerCAMBIOS()
				Return
			Endif
			Select xmm1
			Go Top
			Scan All
				If ActualizaPromocionProducto(xmm1.idart,nid,1)=0 Then
					sw1=0
					Exit
				Endif
			Endscan
			Select u1
			Scan All
				If IngresaDetallePromocion(nid,u1.coda,u1.cant,u1.unid,u1.equi,u1.idepta)=0 Then
					sw=0
					Exit
				Endif
			Endscan
			If sw=1 Then
				GrabarCambios()
				Zap In u1
				Zap In u2
				Thisform.Limpiar1()
			Else
				DeshacerCAMBIOS()
			Endif
		Endif
		
	ENDPROC

	PROCEDURE Pagef1.Page2.Cmdborrar1.Click
		Select u1
		nid=u1.coda
		Delete
		Select u2
		Delete From u2 Where ucoda=nid
		Thisform.pagef1.page2.grimodelos1.Refresh
		
	ENDPROC

	PROCEDURE Pagef1.Page2.Cmdotro1.Click
		Do Form ka_muestraproductos1 To nidcodigo
		If nidcodigo>0 Then
			Select u1
			Locate For coda=nidcodigo
			If !Found()
				Insert Into u1(Descri,cant,coda)Values(lproductos.Descri,0,lproductos.idart)
				x=0
				Select listapr
				Go Top
				Scan All
					x=x+1
					Insert Into u2(uequi,ucoda,uunid,uitem,uprecio,uidepta,ucosto,ucomi)Values(listapr.epta_cant,lproductos.idart,;
						listapr.pres_Desc,x,listapr.epta_Prec,listapr.epta_idep,listapr.costo,listapr.epta_comi)
				Endscan
				Thisform.pagef1.page2.grimodelos1.column2.cmbunidad.RowSource="u2.uunid"
				Thisform.pagef1.page2.grimodelos1.column2.cmbunidad.ListIndex=1
				Thisform.pagef1.page2.grimodelos1.Refresh
				Thisform.pagef1.page2.grimodelos1.SetFocus 
			Else
				Messagebox("Productos Ya Adicionado",64,MSGTITULO)
				Thisform.pagef1.page2.grimodelos1.SetFocus
			Endif
		Endif
		
	ENDPROC

