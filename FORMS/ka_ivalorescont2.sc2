			from dbo.Journal  as j inner join dbo.Producto as p on p.nId_Producto=j.nId_Producto
			where j.nId_Producto =2 and j.nLado_Surtidor in(2)  and j.nPosicion_Manguera=4 order by j.nId_Journal desc) as c
