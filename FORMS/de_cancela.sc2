	PROCEDURE Init
		Lparameters xcodp,nimporte,v
		Thisform.viene=Iif(v=1,"S","N")
		With Thisform
			If MuestraProveedoresX('',2,xcodp,"lproveedor")<1
				Return .F.
			Endif
			.txtcliente.Value=lproveedor.razo
			.proveedor=xcodp
			.txtdolar.Value=fe_gene.dola
			.cmbforma.ListIndex=1
			.txtpago.Value=nimporte
			.grimodelos.RecordSource=""
			.cmdver.Click()
		Endwith
		
	ENDPROC

