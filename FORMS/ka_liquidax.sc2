			cusuario=.cusuario
			nisla=This.Parent.cmbisla.ListIndex
			dfecha=.txtfecha.Value
			f=cfechas(dfecha)
			If This.Parent.chkisla.Value=0 Then
				TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
			        deta,ndoc,
					round(case forma when 'E' then if(tipo='I',impo,0) else 0 end,2) as efectivo,
					round(case forma when 'C' then if(tipo='I',impo,0) else 0 end,2) as credito,
					round(case forma when 'D' then if(tipo='I',impo,0) else 0 end,2) as deposito,
				    lcaj_dsct AS dscto,
					round(case forma when 'T' then if(tipo='I',impo,0) else 0 end,2) as tarjeta,
					round(case forma when 'A' then if(tipo='I',impo,0) else 0 end,2) as Centrega,
					round(case tipo when 'S' then if(forma='E',impo,0) else 0 end,2) as egresos,CAST(0 as decimal(12,2)) as saldo,
					usua,fechao,lcaj_idtu,lcaj_codt,lcaj_rtar as rtarjeta,usuavtas,forma,mone,tmon1,dola,nimpo,tipo,tdoc,idcredito,iddeudas,idauto,impo,
			        0 as cheque
					from(
					SELECT a.lcaj_tdoc as tdoc,a.lcaj_form as forma,if(lcaj_deud<>0,'I',if(lcaj_acre=0,'I','S')) as tipo,lcaj_dcto as ndoc,
					if(lcaj_deud<>0,lcaj_deud,if(lcaj_acre=0,lcaj_deud,lcaj_acre)) as impo,
		            lcaj_deta as deta,lcaj_mone as  mone,lcaj_idcr as idcredito,lcaj_idde as iddeudas,lcaj_idau as idauto,
					c.nomb as usua,a.lcaj_fope as fechao,'' as usuavtas,a.lcaj_mone as tmon1,lcaj_dola as dola,
					if(lcaj_deud<>0,lcaj_deud,if(lcaj_acre=0,lcaj_deud,lcaj_acre)) as nimpo,lcaj_idtu,lcaj_codt,lcaj_dsct,lcaj_rtar FROM
					fe_lcaja as a 
					inner join fe_usua as c on c.idusua=a.lcaj_idus WHERE lcaj_fech='<<f>>' and lcaj_acti<>'I' and lcaj_idau>0 AND lcaj_idus=<<nidusuario>>
					union all
					SELECT a.lcaj_tdoc,a.lcaj_form as forma,if(lcaj_deud<>0,'I','S') as tipo,a.lcaj_ndoc as ndoc,if(a.lcaj_deud<>0,lcaj_deud,lcaj_acre) as impo,
		            a.lcaj_deta as deta,a.lcaj_mone as mone,lcaj_idcr as idcredito,lcaj_idde as iddeudas,lcaj_idau as idauto,
					c.nomb as usua,a.lcaj_fope as fechao,'' as usuavtas,a.lcaj_mone as tmon1,a.lcaj_dola as dola,a.lcaj_deud as nimpo,lcaj_idtu,lcaj_codt,lcaj_dsct,lcaj_rtar FROM
					fe_lcaja as a 
					inner join fe_usua as c on c.idusua=a.lcaj_idus WHERE lcaj_fech='<<f>>' and lcaj_acti<>'I' and lcaj_idau=0 AND lcaj_idus=<<nidusuario>>)
					as b order by fechao,lcaj_codt,ndoc
				ENDTEXT
			Else
		
				TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
			        deta,ndoc,
					round(case forma when 'E' then if(tipo='I',impo,0) else 0 end,2) as efectivo,
					round(case forma when 'C' then if(tipo='I',impo,0) else 0 end,2) as credito,
					round(case forma when 'D' then if(tipo='I',impo,0) else 0 end,2) as deposito,
				    lcaj_dsct as dscto,
					round(case forma when 'T' then if(tipo='I',impo,0) else 0 end,2) as tarjeta,
					round(case forma when 'A' then if(tipo='I',impo,0) else 0 end,2) as Centrega,
					round(case tipo when 'S' then if(forma='E',impo,0) else 0 end,2) as egresos,CAST(0 as decimal(12,2)) as saldo,
					usua,fechao,lcaj_idtu,lcaj_codt,lcaj_rtar as rtarjeta,usuavtas,forma,mone,tmon1,dola,nimpo,tipo,tdoc,idcredito,iddeudas,idauto,impo,0 as cheque
					from(
					SELECT a.lcaj_tdoc as tdoc,a.lcaj_form as forma,if(lcaj_deud<>0,'I',if(lcaj_acre=0,'I','S')) as tipo,lcaj_dcto as ndoc,
					if(lcaj_deud<>0,lcaj_deud,if(lcaj_acre=0,lcaj_deud,lcaj_acre)) as impo,
		            lcaj_deta as deta,lcaj_mone as  mone,lcaj_idcr as idcredito,lcaj_idde as iddeudas,lcaj_idau as idauto,
					c.nomb as usua,a.lcaj_fope as fechao,'' as usuavtas,a.lcaj_mone as tmon1,lcaj_dola as dola,
					if(lcaj_deud<>0,lcaj_deud,if(lcaj_acre=0,lcaj_deud,lcaj_acre)) as nimpo,lcaj_idtu,lcaj_codt,lcaj_dsct,lcaj_rtar FROM
					fe_lcaja as a 
					inner join fe_usua as c on c.idusua=a.lcaj_idus WHERE lcaj_fech='<<f>>' and lcaj_acti<>'I' and lcaj_idau>0 AND lcaj_idus=<<nidusuario>> and lcaj_codt=<<nisla>>
					union all
					SELECT a.lcaj_tdoc,a.lcaj_form as forma,if(lcaj_deud<>0,'I','S') as tipo,a.lcaj_ndoc as ndoc,if(a.lcaj_deud<>0,lcaj_deud,lcaj_acre) as impo,
		            a.lcaj_deta as deta,a.lcaj_mone as mone,lcaj_idcr as idcredito,lcaj_idde as iddeudas,lcaj_idau as idauto,
					c.nomb as usua,a.lcaj_fope as fechao,'' as usuavtas,a.lcaj_mone as tmon1,a.lcaj_dola as dola,a.lcaj_deud as nimpo,lcaj_idtu,lcaj_codt,lcaj_dsct,lcaj_rtar 
					FROM  fe_lcaja as a 
					inner join fe_usua as c on c.idusua=a.lcaj_idus WHERE lcaj_fech='<<f1>>' and lcaj_acti<>'I' and lcaj_idau=0 AND lcaj_idus=<<nidusuario>>  and lcaj_codt=<<nisla>> )
					as b order by fechao,lcaj_codt,ndoc
				ENDTEXT
		
			Endif
		   	If Ejecutaconsulta(lc,"icaja")<1
				Return
			Endif
			nturno=This.Parent.cmbturno.ListIndex
			*****Para Calcular Ventas***
			Do Case
			Case  This.Parent.chkisla.Value=1  And This.Parent.chkturno.Value=1
				TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
		        descri as ndoc,SUM(ROUND(cant*k.prec,2)) AS Impo,SUM(ROUND(cant*k.kar_prel,2)) AS dscto FROM
		        (SELECT lcaj_idau,lcaj_dcto FROM fe_lcaja
		        WHERE   lcaj_fech='<<f>>' and lcaj_acti<>'I' and lcaj_idau>0 AND lcaj_idus=<<nidusuario>> and lcaj_codt=<<nisla>> and lcaj_idtu=<<nturno>> GROUP BY lcaj_dcto,lcaj_idau) AS l 
		        INNER JOIN fe_rcom AS r ON r.idauto=l.lcaj_idau 
		        INNER JOIN fe_kar AS k ON k.idauto=r.idauto 
		        INNER JOIN fe_art AS a ON a.idart=k.idart WHERE r.acti='A' AND k.acti='A' GROUP BY descri
				ENDTEXT
			Case  This.Parent.chkisla.Value=0  And This.Parent.chkturno.Value=1
				TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
		        descri as ndoc,SUM(ROUND(cant*k.prec,2)) AS Impo,SUM(ROUND(cant*k.kar_prel,2)) AS dscto FROM
		        (SELECT lcaj_idau,lcaj_fech,lcaj_dcto FROM fe_lcaja
		        WHERE   lcaj_fech='<<f>>' and lcaj_acti<>'I' and lcaj_idau>0 AND lcaj_idus=<<nidusuario>> and lcaj_idtu=<<nturno>> GROUP BY lcaj_dcto,lcaj_idau) AS l 
		        INNER JOIN fe_rcom AS r ON r.idauto=l.lcaj_idau 
		        INNER JOIN fe_kar AS k ON k.idauto=r.idauto 
		        INNER JOIN fe_art AS a ON a.idart=k.idart WHERE r.acti='A' AND k.acti='A' GROUP BY descri
				ENDTEXT
			Case  This.Parent.chkisla.Value=0  And This.Parent.chkturno.Value=0
				TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
		        descri as ndoc,SUM(ROUND(cant*k.prec,2)) AS Impo,SUM(ROUND(cant*k.kar_prel,2)) AS dscto
		        FROM (SELECT lcaj_idau,lcaj_dcto FROM fe_lcaja
		        WHERE   lcaj_fech='<<f>>' and lcaj_acti<>'I' and lcaj_idau>0 AND lcaj_idus=<<nidusuario>> GROUP BY lcaj_dcto,lcaj_idau) AS l
		        INNER JOIN fe_rcom AS r ON r.idauto=l.lcaj_idau 
		        INNER JOIN fe_kar AS k ON k.idauto=r.idauto 
		        INNER JOIN fe_art AS a ON a.idart=k.idart WHERE r.acti='A' AND k.acti='A' GROUP BY descri
				ENDTEXT
			Case This.Parent.chkisla.Value=1 And This.Parent.chkturno.Value=0
				TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
		        descri as ndoc,SUM(ROUND(cant*k.prec,2)) AS Impo,SUM(ROUND(cant*k.kar_prel,2)) AS dscto FROM
		        (SELECT lcaj_idau,lcaj_dcto FROM fe_lcaja
		        WHERE   lcaj_fech='<<f>>' and lcaj_acti<>'I' and lcaj_idau>0 AND lcaj_idus=<<nidusuario>> and lcaj_codt=<<nisla>>  GROUP BY lcaj_dcto,lcaj_idau) AS l
		        INNER JOIN fe_rcom AS r ON r.idauto=l.lcaj_idau 
		        INNER JOIN fe_kar AS k ON k.idauto=r.idauto 
		        INNER JOIN fe_art AS a ON a.idart=k.idart WHERE r.acti='A' AND k.acti='A' GROUP BY descri
				ENDTEXT
			Endcase
			If Ejecutaconsulta(lc,"ivtas")<1
				Return
			Endif
			If Left(goapp.tipousuario,1)='A' Then
				TEXT TO lc NOSHOW TEXTMERGE PRETEXT 7
			    Cajero,saldo FROM (SELECT u.nomb AS Cajero,SUM(IF(a.lcaj_deud<>0,lcaj_deud,0))-SUM(IF(a.lcaj_acre<>0,lcaj_acre,0)) AS saldo
			    FROM fe_lcaja  AS a INNER JOIN fe_usua AS u ON u.idusua=a.lcaj_idus
			    WHERE  a.lcaj_fech<='<<f>>' AND a.lcaj_acti='A' AND a.lcaj_form='E' GROUP BY lcaj_idus,cajero) AS l WHERE l.saldo<>0
				ENDTEXT
				If Ejecutaconsulta(lc,"saldosa")<1
					Return
				Endif
				This.Parent.grid3.RecordSource="saldosa"
			Else
				This.Parent.grid3.RecordSource=""
			Endif
			If This.Parent.chkturno.Value=1 Then
				Select * From  icaja Where lcaj_idtu=nturno Into Cursor icaja
			Endif
		
			Select * From icaja Into Cursor icaja Readwrite
			Select icaja
			Go Top
			nsaldo=Thisform.txtsaldoa.Value
			Scan All
				nsaldo=nsaldo+(efectivo-egresos)
				Replace saldo With nsaldo In icaja
			Endscan
			Select icaja
			Go Top
			This.Parent.grid2.RecordSource="ivtas"
			Select Sum(Impo) As Ti From ivtas Into Cursor ttt
			This.Parent.txttvtas.Value=ttt.Ti
		
			Thisform.isla=nisla
			Thisform.fecha=dfecha
		
			Thisform.turno=nturno
			Thisform.cajero=This.Parent.cmbalmacen.Value
			Thisform.idusuario=nidusuario
		*	.anteriores()
		*	Select * From icaja Into Cursor icaja Readwrite Order By idcon
		*.faltan('01')
		*.faltan('03')
		*.faltan('21')
		*If .chkmoneda1.Value=0
		*		.reportecaja("S")
		*   .reportecaja("D")
		*Else
		*	.reportecaja(cmone)
		*Endif
			r=0
		*	Select * From rcaja Into Cursor rcaja Order By orden
			Select Sum(tarjeta) As ttar From icaja Where Forma='T' And tipo='I'  Into Cursor tttt
			Select Sum(efectivo) As resing From icaja Where Forma='E' And tipo='I'  Into Cursor vr
			Thisform.Ingresos=vr.resing
			Select Sum(efectivo) As resegre From icaja Where Forma='E' And tipo='S' Into Cursor vr
			Thisform.egresos=vr.resegre
		**************
		*Total De Ventas
		**************
			Select Sum(Impo) As ts From icaja Where tipo="I" And idauto>0  Into Cursor xt
			This.Parent.txtttarjeta.Value=tttt.ttar
			.txttotals.Value=xt.ts
		*******************
		*Ventas Al Credito
		*******************
			Select Sum(credito) As ts From icaja Into Cursor xt
			.txtingresoscs.Value=xt.ts
		*origen="CK"
		*********************
		*Solo Ventas Al Contado
		*********************
			Select Sum(efectivo) As ts From icaja Where Forma="E" And tipo="I"  And idauto>0 Into Cursor xt
		*Select Sum(Impo) As tne From icaja  Where Forma="E" And origen="CK" And tipo="I"  And Impo<0 Into Cursor xnt
			.txtingresoss.Value=xt.ts
		*+xnt.tne
		********************
		*Otros Ingresos
		********************
			Select Sum(efectivo) As ts From icaja Where Forma="E" And tipo="I" And idauto=0 And idcredito=0 Into Cursor xt
			.txttoingresoss.Value=xt.ts
		********************
		*Egresos
		********************
			Select Sum(egresos) As ts From icaja Where tipo="S" And Forma="E" Into Cursor xt
			.txtegresoss.Value=xt.ts
		********************
		*Pagos de Clientes
		********************
			Select Sum(efectivo)  As ts From icaja Where idcredito>0  And Forma="E"  Into Cursor xt
			.txtpagoss.Value=xt.ts
		********************
		*Ingresos Que no Son en Efectivo
		********************
			Select Sum(credito+deposito)  As ts From icaja Where Forma<>"E"  Into Cursor xt
			.txtsincajas.Value=xt.ts
		*******************
			Select Sum(dscto)  As ts From icaja Into Cursor xt
			.txttdsctos.Value=xt.ts
			.txttvbrutas.Value=.txttdsctos.Value+.txttotals.Value
		*******************
			.txtsaldos.Value=(.txtingresoss.Value-.txtegresoss.Value)+.txtpagoss.Value+.txttoingresoss.Value+.txtsaldoa.Value
		******************
			Select icaja
			Go Top
			.grimodelos1.RecordSource="icaja"
			.grimodelos1.SetAll("dynamicforecolor","iif(icaja.tipo='S',RGB(255,0,0),RGB(0,0,0))","Column")
			.grimodelos1.SetFocus
		Endwith
		
	ENDPROC

