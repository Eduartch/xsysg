DEFINE CLASS txtfechar AS textbox 
 	*< CLASSDATA: Baseclass="textbox" Timestamp="" Scale="Pixels" Uniqueid="" />

	#INCLUDE "..\include\sisfotos.h"
	Alignment = 3
	BorderColor = 192,192,192
	FontName = "Tahoma"
	Height = 23
	Name = "txtfechar"
	SpecialEffect = 1
	Value = (date())
	Width = 99
	
	PROCEDURE GotFocus
		this.BackColor=RGB(255,255,128)
	ENDPROC

	PROCEDURE LostFocus
		this.BackColor=RGB(255,255,255)
		IF this.ReadOnly= .F. 
		  if year(this.value)<>val(goapp.año)
		     messagebox("Año no Configurado por el Sistema",48,MSGTITULO)
		     thisform.txtnumero.setfocus
		     RETURN 
		  ENDIF 
		  if month(this.value)<>goapp.mes or year(this.value)<>val(goapp.año)
		     DO FORM v_verifica to verdad WITH "V"
		     IF !verdad
		        thisform.txtnumero.setfocus
		        return
		      ELSE   
		        goapp.mes=month(this.value)
		      ENDIF    
		  ENDIF    
		ENDIF
	ENDPROC

ENDDEFINE

