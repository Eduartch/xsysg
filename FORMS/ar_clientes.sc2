	PROCEDURE Init
		settear()
		If datosglobales()=0 Then
			Return .F.
		Endif
		Thisform.AddProperty("Idvi",0)
		If Left(goapp.tipousuario,1)<>'A' Then
			This.pgf1.page2.Enabled=.F.
		Endif
		With Thisform.pgf1.page1
			Thisform.nombre="Clientes"
			.txtlocalizar.SetFocus
		Endwith
		
	ENDPROC

	PROCEDURE Muestra.ver
		With Thisform.pgf1.page1 
			.grimodelos.RecordSource=""
			lw = '%'+Alltrim(.txtlocalizar.Value)+'%'
			Do Case
				Case .optbusca.optdescripcion.Value=1
					o=0
					i=0
				Case .optbusca.optruc.Value=1
					o=1
					i=0
				Case .optbusca.optciudad.Value=1
					o=4
					i=4
				Case .optbusca.optcodigo.Value=1
					i=Val(.txtlocalizar.Value)
					o=3
			Endcase
			Set Procedure To capadatos,ple5,clientes Additive
			obj=Createobject("clientex")
			obj.ListarClientes(lw,o,i,'lcl')
			If REgdvto("lcl")=0 Then
				Messagebox(NOHAYDATOS,16,MSGTITULO)
				thisform.nohay=.T.
				.cmdnuevo.SetFocus
				Return
			Endif
			thisform.nohay=.F.
			.grimodelos.RecordSource="lcl"
			.grimodelos.colUMN1.ControlSource="lcl.nruc"
			.grimodelos.colUMN2.ControlSource="lcl.ndni"
			.grimodelos.colUMN3.ControlSource="lcl.razo"
			.grimodelos.colUMN4.ControlSource="lcl.dire"
			.grimodelos.colUMN5.ControlSource="lcl.ciud"
			.grimodelos.colUMN6.ControlSource="lcl.fono"
			.grimodelos.colUMN7.ControlSource="lcl.idclie"
			.grimodelos.colUMN8.ControlSource="lcl.clie_lcre"
			.grimodelos.SetAll("dynamicbackcolor","iif(MOD(RECNO(),2)=0,RGB(234,234,255),RGB(247,247,242))","Column")
		Endwith
		
	ENDPROC

