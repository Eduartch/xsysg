			from dbo.Journal  as j inner join dbo.Producto as p on p.nId_Producto=j.nId_Producto
			where j.nId_Producto =5 and j.nLado_Surtidor in(4) and j.nPosicion_Manguera=1 order by j.nId_Journal desc) as c
			ENDTEXT
