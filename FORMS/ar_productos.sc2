	PROCEDURE Pgf1.Page1.Grimodelos.controla
		Lparameters nkey
		With Thisform.pgf1.page1
			Do Case
			Case nkey=7
				.txtlocalizar.SetFocus
			Case nkey=13
				nidart=lpro.idart
				Do Form ar_mproductos With "M","X",0
				.cmdactualizar1.Click()
				If !Empty(This.Parent.grimodelos.RecordSource) Then
					Select (This.Parent.grimodelos.RecordSource)
					Locate For idart=nidart
					This.Parent.grimodelos.Refresh
					This.Parent.grimodelos.column1.SetFocus
				Endif
			Endcase
		Endwith
		
	ENDPROC

