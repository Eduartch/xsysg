	PROCEDURE Pgf1.Page1.Grimodelos.controla
		Lparameters nkey
		Local  cnombre
		With Thisform.pgf1.page1
			cnombre=thisform.nombre
			Do Case
				Case nkey=7
					.txtlocalizar.SetFocus
				Case nkey=13
					nidart=lcl.idclie
					Do Form ar_mclientes With "M",0,"",""
					thisform.muestra.ver()
					Select (.grimodelos.RecordSource)
					Locate For idclie=nidart
					.grimodelos.Refresh
					.grimodelos.column1.SetFocus
			Endcase
		Endwith
		
	ENDPROC

