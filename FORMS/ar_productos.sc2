	PROCEDURE Muestra.ver
		With Thisform.pgf1.page1
			.grimodelos.RecordSource=""
			lw = '%'+Alltrim(.txtlocalizar.Value)+'%'
			nid=Val(.txtlocalizar.Value)
			opt=Iif(.optbusca.optdescripcion.Value=1,'1','2')
			If MuestraProductosDescCod(lw,fe_gene.dola,'T',0,"lpro") < 1
				Thisform.nohay=.T.
				Return
			Endif
			If regdvto('lpro')=0
				Mensaje(NOHAYDATOS)
				Thisform.nohay=.T.
			Else
				Thisform.nohay=.F.
				.grimodelos.RecordSource="lpro"
				.grimodelos.coluMN1.ControlSource="lpro.idart"
				.grimodelos.coluMN2.ControlSource="lpro.descri"
				.grimodelos.coluMN3.ControlSource="lpro.unid"
				.grimodelos.coluMN4.ControlSource="lpro.uno"
				.grimodelos.coluMN5.ControlSource="lpro.pre1"
				.grimodelos.coluMN6.ControlSource="lpro.dcat"
				.grimodelos.coluMN7.ControlSource="lpro.prod_smin"
				.grimodelos.coluMN8.ControlSource="lpro.prod_smax"
				.grimodelos.SetAll("dynamicbackcolor","iif(MOD(RECNO(),2)=0,RGB(234,234,255),RGB(247,247,242))","Column")
				.grimodelos.Refresh
			*	.grimodelos.SetFocus
			ENDIF
			thisform.Text1.Value=thisform.nohay 
		Endwith
		
	ENDPROC

