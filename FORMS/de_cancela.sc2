	PROCEDURE cmdver.Click
		Thisform.grimodelos.RecordSource=""
		With Thisform
			.txtpago.Value=0
			.txtimporte.Value=0
			.txtdias.Value=0
			.txtamortiza.Value=0
			.txtdocumento.Value=""
			.txtdetalle.Value=""
			.txtnumero.Value=""
			.txtfechavto.Value=DATE()
			.txtnumero.Visible=.T.
			Do Form cr_vtosdocumentos With .proveedor,"P" To niden
			If xt.tipo="L"
				If !Empty(niden)
					nprov=Thisform.proveedor
					TEXT TO lc NOSHOW
		               SELECT b.ndoc as docd,a.fech,a.fevto,SUM(a.impo-a.acta) as importe,b.impo as impc,b.idauto,a.banco,a.tipo,a.ncontrol,c.rdeu_mone as mone,
		               a.nrou,a.ndoc FROM fe_deu as a inner join fe_rdeu as c on c.rdeu_idrd=a.deud_idrd left join fe_rcom as b
		               on b.idauto=c.rdeu_idau where ncontrol=?niden AND c.rdeu_idpr=?nprov AND a.acti='A'  GROUP BY ncontrol
					ENDTEXT
						IF verificaconexion()=0 then
		RETURN 0
		endif
					If SQLExec(goapp.bdconn,lc,"apagar")<1
						errorbd(lc)
						Return
					Endif
					.grimodelos.RecordSource="apagar"
					nidauto=apagar.idauto
					.txtnumero.Value=apagar.ndoc
					.txtcontrol.Value=apagar.ncontrol
					.txtdocumento.Value=apagar.docd
					.txttipo.Value=apagar.tipo
					.txtfechae.Value=apagar.fech
					.txtfechavto.Value=apagar.fevto
					.txtimporte.Value=apagar.importe
					.txtimporteo.Value=apagar.impc
					.txtbanco.Value=apagar.banco
					.txtmoneda.Value=apagar.mone
					.txtnrounico.Value=apagar.nrou
					.nauto=apagar.idauto
					TEXT TO lc NOSHOW
		                    SELECT a.nomb,b.tdoc FROM fe_rcom as b JOIN fe_tdoc as a ON (a.tdoc=b.tdoc) WHERE idauto=?nidauto
					ENDTEXT
						IF verificaconexion()=0 then
		RETURN 0
		endif
					If SQLExec(goapp.bdconn,lc,"ld")<1
						errorbd(lc)
						Return
					Endif
					.txttdcto.Value=ld.nomb
					Thisform.letras='S'
					Select ndoc,fech,fevto,fevto-fech As dias,impc,importe As saldo,ncontrol,idauto,tipo,docd From apagar Into Cursor txp
					.grimodelos.RecordSource="txp"
				Endif
				.cmbforma.SetFocus
			Else
				Select ndoc,fech,fevto,dias,importec As importe ,saldo,pagos,ncontrol,idauto,tipo,docd,moneda From tmp Where pagos>0 Into Cursor txp
				Go Top In txp
				.txtmoneda.Value=txp.moneda
				.txtpago.Value=niden
				.txtfechavto.Value=DATE()
				If Thisform.txtmoneda.Value<>'S' Then
					.txtmontoMN.Value=Round(.txtpago.Value*.txtdolar.Value,2)
				Else
					.txtmontoMN.Value=.Txtpago.Value
				Endif
				.letras='N'
				.grimodelos.RecordSource="txp"
				.txtpago.ReadOnly= .T.
				.txtnumero.SetFocus
			Endif
		Endwith
		
	ENDPROC

