	PROCEDURE pgfmvtos.Pgt.Command1.Click
		nidus=goapp.idcajero
		dfecha=fe_gene.fech
		nisla=goapp.isla
		dfecha1=Ctod("15/11/2018")
		If goapp.conectacontrolador='S' Then
			If Left(goapp.tipousuario,1)='A' Then
				TEXT TO lc NOSHOW
		        SELECT SUM(if(a.lcaj_deud<>0,if(lcaj_mone='S',lcaj_deud,lcaj_deud*lcaj_dola),if(lcaj_mone='S',-lcaj_acre,-lcaj_acre*lcaj_dola))) as saldo
		        FROM fe_lcaja  as a WHERE
		        a.lcaj_fech between ?dfecha1 and ?dfecha and a.lcaj_acti='A' and a.lcaj_form='E' and lcaj_idus=?nidus group by lcaj_idus
				ENDTEXT
			Else
				TEXT TO lc NOSHOW
		        SELECT SUM(if(a.lcaj_deud<>0,if(lcaj_mone='S',lcaj_deud,lcaj_deud*lcaj_dola),if(lcaj_mone='S',-lcaj_acre,-lcaj_acre*lcaj_dola))) as saldo
		        FROM fe_lcaja  as a WHERE
		        a.lcaj_fech=?dfecha and a.lcaj_acti='A' and a.lcaj_form='E' and lcaj_idus=?nidus and lcaj_codt=?nisla group by lcaj_idus
				ENDTEXT
			Endif
		Else
			If Left(goapp.tipousuario,1)='A' Then
				TEXT TO lc NOSHOW
		        SELECT SUM(if(a.lcaj_deud<>0,if(lcaj_mone='S',lcaj_deud,lcaj_deud*lcaj_dola),if(lcaj_mone='S',-lcaj_acre,-lcaj_acre*lcaj_dola))) as saldo
		        FROM fe_lcaja  as a WHERE
		        a.lcaj_fech between ?dfecha1 and ?dfecha and a.lcaj_acti='A' and a.lcaj_form='E' and lcaj_idus=?nidus group by lcaj_idus
				ENDTEXT
			Else
				TEXT TO lc NOSHOW
		        SELECT SUM(if(a.lcaj_deud<>0,if(lcaj_mone='S',lcaj_deud,lcaj_deud*lcaj_dola),if(lcaj_mone='S',-lcaj_acre,-lcaj_acre*lcaj_dola))) as saldo
		        FROM fe_lcaja  as a WHERE a.lcaj_fech 
		        between ?dfecha1 and ?dfecha and a.lcaj_acti='A' and a.lcaj_form='E' and lcaj_idus=?nidus group by lcaj_idus
				ENDTEXT
			Endif
		ENDIF
			IF verificaconexion()=0 then
		RETURN 0
		endif
		If SQLExec(goapp.bdconn,lc,'scaja')<1 Then
			errorbd(lc)
			Return
		ENDIF
		If Left(goapp.tipousuario,1)='V' Then
			Thisform.pgfmvtos.pgt.txtsaldo.Visible= .F.
		Else
			Thisform.pgfmvtos.pgt.txtsaldo.Visible= .T.
		Endif
		Thisform.pgfmvtos.pgt.txtsaldo.Value=Iif(!Isnull(scaja.saldo),scaja.saldo,0)
		
	ENDPROC

	PROCEDURE pgfmvtos.Pgt.Command2.Click
		Do Form v_verifica With "A" To verdad
		If !verdad
			Thisform.pgfmvtos.pgt.txtfecha.ReadOnly= .T.
		Else
			Thisform.pgfmvtos.pgt.txtfecha.ReadOnly= .F.
			Thisform.pgfmvtos.pgt.txtfecha.SetFocus
		Endif
		
	ENDPROC

	PROCEDURE pgfmvtos.Pgt.Command3.Click
		Do Form v_verifica With "A" To verdad
		If !verdad
			Thisform.pgfmvtos.pgt.cmbcajero1.Enabled=.f.
		Else
			Thisform.pgfmvtos.pgt.cmbcajero1.Enabled=.t.
			Thisform.pgfmvtos.pgt.cmbcajero1.SetFocus 
		Endif
		
	ENDPROC

	PROCEDURE pgfmvtos.Pgt.txtfecha.LostFocus
		If !this.ReadOnly 
		   this.ReadOnly=.t.
		EndIf 
	ENDPROC

ENDDEFINE
