		If Thisform.verdad
			thisform.ad(.f.)
		Endif
		
	ENDPROC

	PROCEDURE cmdeliminar.Click
		Local r
		Sele tmpvg
		r=Recno()
		If  Eof()
			Thisform.cmdeliminar.Enabled=.F.
			Return
		Endif
		Delete
		Thisform.txtitem.Value=Thisform.txtitem.Value-1
		Sum cant*peso To Thisform.txtpesototal.Value
		If r>0
			Go r
		Endif
		Thisform.grivtag.Refresh
		Thisform.grivtag.column1.text1.SetFocus
	ENDPROC

	PROCEDURE Cmdjalar1.Click
		Thisform.grivtag.RecordSource=""
		Zap In tmpvg
		Do Form ka_pentrega With 10,Thisform.txtitem.Value  To verdad
		If verdad
			Go Top In lista
			With Thisform
				.tdoc=lista.tdoc
				.txtrAZON.Value=lista.cliente
				.txtcodigo.Value=lista.idclie
				.txtllegada.Value=Rtrim(lista.Dire)+'-'+Rtrim(lista.ciud)
				.txtruC.Value=lista.nruc
				.Auto=lista.idauto
				.txtdIRECCION.Value=Rtrim(lista.Dire)+'-'+Rtrim(lista.ciud)
				.txtserie.Value=Iif(Len(Alltrim(lista.ndoc))<=10,Left(lista.ndoc,3),Left(lista.ndoc,4))
				.txtnumero.Value=Iif(Len(Alltrim(lista.ndoc))<=10,Substr(lista.ndoc,4,7),Substr(lista.ndoc,5))
				.txtfeCHAf.Value=lista.fech
				.grivtag.RecordSource="tmpvg"
				.grivtag.column1.ControlSource="tmpvg.descri"
				.grivtag.column2.ControlSource="tmpvg.unid"
				.grivtag.column3.ControlSource="tmpvg.cant"
				.grivtag.column4.ControlSource="tmpvg.peso"
				.grivtag.column5.ControlSource="round(tmpvg.cant*tmpvg.peso,2)"
				.grivtag.column6.ControlSource="tmpvg.prec"
				.grivtag.column7.ControlSource="round(tmpvg.cant*tmpvg.prec,2)"
				.cmdeliminar.Enabled= .T.
				Go Top In tmpvg
				.grivtag.Refresh
				.txtserieg.SetFocus
			Endwith
		Endif
		
	ENDPROC

