	PROCEDURE lproductos
		Lparameters nkey
		With Thisform
			Do Case
			Case nkey=7
				.txtbuscar.SetFocus
			Case nkey=13
				.cmdotro.Enabled=.T.
				If .tipo=0
					Select tmpc
					Go .nreg
					Replace Desc With lproductos.Descri,unid With lproductos.unid,coda With lproductos.idart,;
						ndoc With .txtserie.Value+.txtnumero.Value,alma With lproductos.uno,peso With lproductos.peso,;
						tipro With lproductos.tipro,Prec With lproductos.Prec,costosf With lproductos.costosf,gast With lproductos.flete,;
						costoant With lproductos.costo,swcosto With 1
				Else
					.grivta.column4.cmbprecios.RowSource=""
					Select tmpv
					If goapp.vitems=0 Then
						Locate For coda=lproductos.idart
						If !Found()
							Go .nreg
							Repla Desc With lproductos.Descri,unid With lproductos.unid,coda With lproductos.idart,Prec With lproductos.pre1,;
								ndoc With .txtserie.Value+.txtnumero.Value,pre1 With lproductos.pre1,pre2 With lproductos.pre2,pre3 With lproductos.pre3,;
								prem With lproductos.pre1,premax With lproductos.pre3,costo With lproductos.costo,nitem With .txtitem.Value,;
								uno With lproductos.uno,Dos With lproductos.Dos,tipro With lproductos.tipro,idcosto With Val(lproductos.cost_idco),;
								comi With lproductos.prod_come
							Do Case
							Case goapp.tienda=1
								Replace alma With lproductos.uno In tmpv
							Case goapp.tienda=2
								Replace alma With lproductos.Dos In tmpv
							Case goapp.tienda=3
								Replace alma With lproductos.tre In tmpv
							Case goapp.tienda=4
								Replace alma With lproductos.cua In tmpv
							Endcase
							Local nprecio
							nprecio=DultimoPrecio(Thisform.txtcodigo.Value,lproductos.idart)
							Insert Into precios(precio,coda,iden,nitem)Values(lproductos.pre1,lproductos.idart,1,.txtitem.Value)
							Insert Into precios(precio,coda,iden,nitem)Values(lproductos.pre2,lproductos.idart,2,.txtitem.Value)
							Insert Into precios(precio,coda,iden,nitem)Values(lproductos.pre3,lproductos.idart,3,.txtitem.Value)
							Insert Into precios(precio,coda,iden,nitem)Values(nprecio,lproductos.idart,4,.txtitem.Value)
							.grivta.column4.cmbprecios.RowSource="precios.precio"
							.grivta.column4.cmbprecios.Format="9,999.99"
							.grivta.column4.cmbprecios.InputMask="9,999.99"
							Do Case
							Case .cmbpreferencias.ListIndex=1
								.grivta.column4.cmbprecios.Value=lproductos.pre1
							Case .cmbpreferencias.ListIndex=2
								.grivta.column4.cmbprecios.Value=lproductos.pre2
							Case .cmbpreferencias.ListIndex=3
								.grivta.column4.cmbprecios.Value=lproductos.pre3
							Case .cmbpreferencias.ListIndex=4
								If nprecio>=0 Then
									.grivta.column4.cmbprecios.Value=nprecio
								Endif
							Endcase
							Replace pos With .cmbpreferencias.ListIndex In tmpv
						Else
							.txtitem.Value=.txtitem.Value-1
							Go .nreg
							Delete
						Endif
					Else
						Go .nreg
						Repla Desc With lproductos.Descri,unid With lproductos.unid,coda With lproductos.idart,Prec With lproductos.pre1,;
							ndoc With .txtserie.Value+.txtnumero.Value,pre1 With lproductos.pre1,pre2 With lproductos.pre2,pre3 With lproductos.pre3,;
							prem With lproductos.pre1,premax With lproductos.pre3,costo With lproductos.costo,nitem With .txtitem.Value,;
							uno With lproductos.uno,Dos With lproductos.Dos,tipro With lproductos.tipro,idcosto With Val(lproductos.cost_idco),;
							comi With Iif(.cmbforma.ListIndex=1,lproductos.prod_come,lproductos.prod_comc)
						Do Case
						Case goapp.tienda=1
							Replace alma With lproductos.uno In tmpv
						Case goapp.tienda=2
							Replace alma With lproductos.Dos In tmpv
						Case goapp.tienda=3
							Replace alma With lproductos.tre In tmpv
						Case goapp.tienda=4
							Replace alma With lproductos.cua In tmpv
						Endcase
						Local nprecio
						nprecio=DultimoPrecio(Thisform.txtcodigo.Value,lproductos.idart)
						Insert Into precios(precio,coda,iden,nitem)Values(lproductos.pre1,lproductos.idart,1,.txtitem.Value)
						Insert Into precios(precio,coda,iden,nitem)Values(lproductos.pre2,lproductos.idart,2,.txtitem.Value)
						Insert Into precios(precio,coda,iden,nitem)Values(lproductos.pre3,lproductos.idart,3,.txtitem.Value)
						Insert Into precios(precio,coda,iden,nitem)Values(nprecio,lproductos.idart,4,.txtitem.Value)
						.grivta.column4.cmbprecios.RowSource="precios.precio"
						.grivta.column4.cmbprecios.Format="9,999.99"
						.grivta.column4.cmbprecios.InputMask="9,999.99"
						Do Case
						Case .cmbpreferencias.ListIndex=1
							.grivta.column4.cmbprecios.Value=lproductos.pre1
						Case .cmbpreferencias.ListIndex=2
							.grivta.column4.cmbprecios.Value=lproductos.pre2
						Case .cmbpreferencias.ListIndex=3
							.grivta.column4.cmbprecios.Value=lproductos.pre3
						Case .cmbpreferencias.ListIndex=4
							If nprecio>=0 Then
								.grivta.column4.cmbprecios.Value=nprecio
							Endif
						Endcase
						Replace pos With .cmbpreferencias.ListIndex In tmpv
					Endif
				Endif
				.retornar()
				.grivta.column3.text1.SetFocus
			Case nkey=27
				.griart.Visible=.F.
				If .tipo=0
					Sele tmpc
				Else
					Sele tmpv
				Endif
				Go .nreg
				Delete
				.txtitem.Value=.txtitem.Value-1
				.retornar()
				.grivta.column1.text1.SetFocus
			Endcase
		Endwith
		
	ENDPROC

