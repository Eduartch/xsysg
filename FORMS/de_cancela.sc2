	PROCEDURE txtdetalle.InteractiveChange
		local lnSelStart
		IF '!' $ THIS.FORMAT
			lnSelStart = this.SelStart
			this.value = UPPER(this.value)
			this.SelStart = lnSelStart
		ENDIF
	ENDPROC

	PROCEDURE txtdias.InteractiveChange
		thisform.txtfechavt.value=thisform.txtfechavto.value+this.value
	ENDPROC

	PROCEDURE txtdolar.InteractiveChange
		With Thisform
			If .txtmoneda.Value<>'S' Then
				.txtmontoMN.Value=Round(.txtpago.Value*This.Value,2)
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE txtfecha.LostFocus
		thisform.txtdolar.Value=DtipoCambio(this.Value,'V')
	ENDPROC

	PROCEDURE Txtinteres.InteractiveChange
		thisform.txtnsaldo.value=thisform.txtimporte.value-thisform.txtamortiza.value 
		thisform.txtpago.Value=this.Value 
	ENDPROC

	PROCEDURE txtpago.InteractiveChange
		If Thisform.txtmoneda.Value<>'S' Then
			Thisform.txtmontoMN.Value=Round(This.Value*Thisform.txtdolar.Value,2)
		Else
			Thisform.txtmontoMN.Value=This.Value
		Endif
		If Thisform.txttipo.Value<>'F' Then
			If Thisform.cmbforma.ListIndex=1
				Thisform.txtinteres.Value=This.Value-Thisform.txtimporte.Value
			Else
				Thisform.txtinteres.Value=This.Value-Thisform.txtamortiza.Value
			Endif
		Endif
		
	ENDPROC

	PROCEDURE txtpago.LostFocus
		if this.value>0 and !empty(thisform.txtnumero.value) 
		   thisform.cmdaceptar.enabled=.t.
		endif   
	ENDPROC

