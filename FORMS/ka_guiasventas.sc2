			x=.F.
		Endif
		With Thisform
			.txtrazont.ReadOnly=x
			.txtdirecciont.ReadOnly=x
			.txtconductor.ReadOnly=x
			.txtmarca.ReadOnly=x
			.txtruct.ReadOnly=x
			.txtplaca1.ReadOnly=x
			.txtconstancia.ReadOnly=x
			.txtbrevete.ReadOnly=x
		Endwith
		
	ENDPROC

	PROCEDURE generanumero
		If Val(Thisform.txtnumerog.Value)>=Thisform.nsgte
			If GeneraCorrelativo(Val(Thisform.txtnumerog.Value)+1,Thisform.idserie)=0 Then
				Return 0
			Else
				Return 1
			Endif
		Else
			Return 1
		Endif
	ENDPROC

	PROCEDURE Init
		Lparameters nidauto,xguia
		If MuestraAlmacenes()=0 Then
			Return .F.
		Endif
		Thisform.grivtag.RecordSource=""
		If goapp.foxy=1 Then
			Do FOXYPREVIEWER.App With "Release"
			Thisform.foxy=1
		Endif
		Create Cursor tmpvg(coda N(8),Descri c(100),unid c(10),cant N(10,2),Prec N(10,2),;
			peso N(10,2),alma N(10,2),ndoc c(10),nreg N(10),tref c(2),Refe c(10),fecr d,;
			Valida c,nitem N(3),saldo N(10,2),idin N(8),nidkar N(10))
		With Thisform
			nalma=goapp.tienda
			Select almacenes
			Locate For idalma=nalma
			.txtpartida.Value=Alltrim(almacenes.Dire)+' '+Alltrim(almacenes.ciud)
			.idtran=0
			.Auto=0
			Select tmpvg
			Index On Descri Tag Descri
			Index On nitem Tag items
			If nidauto>0 Then
				TEXT TO lc NOSHOW
			      SELECT a.idauto,a.idkar,a.idart as coda,a.cant,a.prec,b.valor,b.igv, b.impo,b.fech,b.form,b.idcliente as idclie,
			      c.razo,c.nruc,c.dire,c.ciud,c.ndni,b.tdoc,b.ndoc,e.descri,e.unid,e.peso,a.cant as saldo
			      from fe_kar as a inner join fe_rcom as b on b.idauto=a.idauto inner join fe_clie as c
			      ON c.idclie=b.idcliente inner join fe_art as e on e.idart=a.idart  where a.idauto=?nidauto AND a.acti<>'I'
			      and b.acti<>'I' and b.tipom='V' order by a.idkar
				ENDTEXT
				This.WindowType= 1
			Else
				Do Form ka_lselecciona With 0,'V' To nids
				TEXT TO lc NOSHOW
				   SELECT a.idauto,a.idkar,a.idart as coda,a.saldo as cant,a.prec,a.fech,a.form,a.idcliente as idclie,
			       c.razo,c.nruc,c.dire,c.ciud,c.ndni,a.tdoc,a.ndoc,e.descri,e.unid,e.peso,a.saldo
			       from vpdtesx as a inner join fe_clie as c  ON c.idclie=a.idcliente inner join fe_art as e on e.idart=a.idart  where a.saldo>0 and a.idauto=?nids
			       group by a.idkar order by a.idkar     
			     ENDTEXT
				Thisform.WindowType=0
			ENDIF
				IF verificaconexion()=0 then
		RETURN 0
		endif
			ncon=Abreconexion()
			If SQLExec(ncon,lc,'lg')<0 Then
				errorbd(lc)
				Return .F.
			Endif
			CierraConexion(ncon)
			.Auto=lg.idauto
			.txtrAZON.Value=lg.razo
			.txtllegada.Value=Alltrim(lg.Dire)+' '+Alltrim(lg.ciud)
			.txtruC.Value=Iif(lg.tdoc='01',lg.nruc,lg.ndni)
			.txtdIRECCION.Value=Alltrim(lg.Dire)+' '+Alltrim(lg.ciud)
			.txtserie.Value=Iif(Len(Alltrim(lg.ndoc))<=10,Left(lg.ndoc,3),Left(lg.ndoc,4))
			.txtnumero.Value=Iif(Len(Alltrim(lg.ndoc))<=10,Substr(lg.ndoc,4,7),Substr(lg.ndoc,5))
			.txtfeCHA.Value=Date()
			.txtserieg.Value=Right("000"+Left(xguia,1),3)
			.txtnumerog.Value=Right("000000"+Substr(xguia,3,4),7)
			.txtfeCHAF.Value=lg.fech
			.tdoc=lg.tdoc
			.txtcodigo.Value=lg.idclie
			.entregas="F"
			r=0
			Select lg
			Scan All
				Insert Into tmpvg(coda,Descri,unid,cant,peso,Prec,nidkar,saldo);
					values(lg.coda,lg.Descri,lg.unid,lg.cant,lg.peso,lg.Prec,lg.idkar,lg.saldo)
			      r=r+1
			Endscan
			.txtitem.Value=r
			.grivtag.RecordSource="tmpvg"
			.grivtag.column1.ControlSource="tmpvg.descri"
			.grivtag.column2.ControlSource="tmpvg.unid"
			.grivtag.column3.ControlSource="tmpvg.cant"
			.grivtag.column4.ControlSource="tmpvg.peso"
			.grivtag.column5.ControlSource="round(tmpvg.cant*tmpvg.peso,2)"
			.grivtag.column6.ControlSource="tmpvg.prec"
			.grivtag.column7.ControlSource="round(tmpvg.cant*tmpvg.prec,2)"
			Go Top In tmpvg
			.grivtag.Refresh
		Endwith
		
	ENDPROC

