	PROCEDURE Init
		Lparameters cmodo,ctipo,reg
		*cmodo si es a Nuevo,Modificar
		*Ctipo Si es llamado de Compras O Ventas
		*reg El nº de registro
		Local npos
		Thisform.vaciarcontroles()
		Thisform.actualizo="N"
		Thisform.Alias=Alias()
		Thisform.tipo=ctipo
		Thisform.modo=cmodo
		Thisform.jgrupos()
		Thisform.jlineas()
		Thisform.jmarcas()
		Thisform.jfletes()
		thisform.AddProperty("CodEncontrado","")
		If MuestraAlmacenes()=0 Then
			Return .F.
		Endif
		Thisform.encontrado='N'
		thisform.frame.cmbalmacen.RowSource="almacenes.nomb"
		Select * From fe_gene Into Cursor config
		With Thisform.frame
			.txtdolar.Value=fe_gene.dola
			If cmodo="N"
				.Enabled= .T.
				Thisform.cmdborrar.Enabled=.F.
				Thisform.cmdcambiar.Enabled=.F.
				.cmbmarca.ListIndex=1
				.cmbtipo.ListIndex=1
				.cmbfletes.ListIndex=1
				.cmbestado.ListIndex=1
				.cmbgrupo.ListIndex=1
				.txtproveedor.Value=""
		        .cmbalmacen.ListIndex=1
		        this.frame.txtunidad.value='NIU'
				*Thisform.cmdanterior1.Enabled= .F.
				*Thisform.cmdsgte1.Enabled= .F.
				.cmbgrupo.SetFocus
			Else
				Thisform.chkeditar1.Enabled= .T.
				.Enabled= .F.
				Thisform.refrescar()
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE jfletes
		thisform.frame.cmbfletes.RowSource=""
		If MuestraFletesX('',"lfletes")< 1 Then
			Return
		Endif
		Thisform.frame.cmbfletes.RowSource="lfletes.desflete"
		If Thisform.modo="M"
			Thisform.frame.Txtcodigof.Value=lpro.idflete
			Select lfletes
			Locate For lfletes.idflete=lpro.idflete
			Thisform.frame.cmbfletes.Value=lfletes.desflete
			Thisform.frame.txtflete.Value=lfletes.Prec
		Else
			Thisform.frame.Txtcodigof.Value=lfletes.idflete
		Endif
		
	ENDPROC

