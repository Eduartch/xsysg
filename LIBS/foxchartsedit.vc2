	PROCEDURE P2.cmdFont.Click
		Local lcFont
		Local lcFontStyle, laFont[1]
		lcFontStyle = Iif(This.FontBold      ,[B],[])  +;
			IIF(This.FontItalic    ,[I],[])
		lcFontStyle = Iif(Empty(lcFontStyle), [N], m.lcFontStyle)
		
		lcFont = Getfont(This.Caption,This.FontSize,lcFontStyle,This.FontCharSet)
		If Empty(lcFont)
			Return
		Endif
		=Alines(laFont,lcFont,1,[,])
		This.Caption    = laFont[1]
		This.FontName   = laFont[1]
		This.FontBold   = [B] $ laFont[3]
		This.FontItalic = [I] $ laFont[3]
		This.FontSize   = Val(laFont[2])
		If Alen(laFont,1) > 3
			This.FontCharSet = Val(laFont[4])
		Endif
		
		Local loLegend As Label
		loLegend = This.Parent.SelObject
		
		If Vartype(loLegend) = "O"
			loLegend.FontName   = laFont[1]
			loLegend.FontBold   = [B] $ laFont[3]
			loLegend.FontItalic = [I] $ laFont[3]
			loLegend.FontSize   = Val(laFont[2])
			If Alen(laFont,1) > 3 And Pemstatus(loLegend,[FontCharSet],5)
				loLegend.FontCharSet = Val(laFont[4])
			Endif
			Thisform.FoxCharts1.DrawChart()
		Endif
		
		This.Parent.Parent.UpdateLegends()
		
		Return
		
	ENDPROC

