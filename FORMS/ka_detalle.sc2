	PROCEDURE Init
		Lparameters cndoc,ct
		Thisform.grimodelos.RecordSource=""
		If ct="V"
			TEXT TO lc noshow
		     SELECT a.descri,a.unid,b.cant,b.prec,ROUND(b.cant*b.prec,2) as importe,
		     d.fech,c.nomb as nomv,d.idusua as codv,e.nomb as usua,d.fusua,d.mone,d.tdoc,deta,form FROM fe_kar as b
		     inner join fe_art as a ON(a.idart=b.idart) inner join fe_rcom as d on(d.idauto=b.idauto)
		     inner join fe_usua as e on(e.idusua=d.idusua) WHERE d.idauto=?cndoc and d.acti<>'I' and b.acti<>'I'
			ENDTEXT
		Else
			TEXT TO lc noshow
		     SELECT a.descri,a.unid,b.cant,b.prec,ROUND(b.cant*if(b.incl="I",b.prec,b.prec*v.igv),2) as importe,d.fech,e.nomb as usua,d.fusua,d.mone,d.tdoc FROM fe_kar as b
		     join fe_art as a ON(a.idart=b.idart) join fe_rcom as d ON(d.idauto=b.idauto) join fe_usua as e ON(e.idusua=d.idusua), fe_gene as v WHERE d.idauto=?cndoc and d.acti<>'I' and b.acti<>'I'
			ENDTEXT
		ENDIF
			IF verificaconexion()=0 then
		RETURN 0
		endif
		If SQLExec(goapp.bdconn,lc,"dvtas")<1
			errorbd(lc)
			Return
		Endif
		cdc=""
		With Thisform
			Do Case
			Case dvtas.tdoc='01'
				cdc="Factura"
			Case dvtas.tdoc='03'
				cdc="Boleta"
			Case dvtas.tdoc='09'
				cdc="Guia de  Remisión"
			Case dvtas.tdoc='20'
				cdc="Pedido"
			Otherwise
				cdc=''
			Endcase
			.txtdcto.Value=cdc
			.txtfecha.Value=dvtas.fech
			.txtusuario.Value=dvtas.usua
			.txtfechahora.Value=dvtas.fusua
			.txtmoneda.Value=Iif(dvtas.mone="S","Soles","Dólares")
		 	Do Case
			Case kardex.Form="E"
				.cmbforma.ListIndex=1
			Case kardex.Form="C"
				.cmbforma.ListIndex=2
			Case kardex.Form="D"
				.cmbforma.ListIndex=3
			Endcase
		
		
			This.txtdetalle.Value=dvtas.Deta
			This.codigo=cndoc
			If ct="V"
				.txtvendedor.Value=dvtas.nomv
			Endif
		*.txtguia.Value=dvtas.ndo2
			Select Sum(importe) As ti From dvtas Into Cursor xt
			.txttotal.Value=xt.ti
			Select dvtas
			Go Top
			.grimodelos.RecordSource="dvtas"
			.grimodelos.SetFocus
		Endwith
		
	ENDPROC

