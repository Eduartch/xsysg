	PROCEDURE ingresapagos
		Local sw As Integer
		sw=1
		nidp=0
		Select  tmp
		Scan For tmp.pagos>0
			nidp=CancelaCreditos(This.txtserie.Value+This.txtnumero.Value,tmp.pagos,'P',Left(This.cmbmoneda.Value,1),;
				'NOTA DE CREDITO',This.txtfecha.Value,;
				tmp.fevto,tmp.tipo,tmp.ncontrol,'',tmp.rcre_idrc,Id(),goapp.nidusua)
			If nidp<=0 Then
				sw=0
				Exit
			Endif
		Endscan
		If sw=0
			Return 0
		Else
			Return nidp
		Endif
		********
		
	ENDPROC

