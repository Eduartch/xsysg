DEFINE CLASS calcularasaldos AS custom 
 	*< CLASSDATA: Baseclass="custom" Timestamp="" Scale="Pixels" Uniqueid="" />

	#INCLUDE "..\include\sisfotos.h"
	*<DefinedPropArrayMethod>
		*m: ejecutar
	*</DefinedPropArrayMethod>

	Height = 21
	Name = "calcularasaldos"
	Width = 81
	
	PROCEDURE ejecutar
		Lparameters ccodc,ctipo,Sw
		Local Sw,ccodigo
		ccodigo=Alltrim(Str(ccodc))
		If ctipo="C"
			If SQLExec(goapp.bdconn,"CALL PROCALCULARSALDOSCLIENTE(?ccodc)","mvtos")<1
				errorbd(ERRORPROC)
				Return
			Endif
		Else
			If SQLExec(goapp.bdconn,"CALL PROCALCULASALDOSPROVEEDOR(?ccodc)","mvtos")<1
				errorbd(ERRORPROC)
				Return
			Endif
		Endif
		Select Sum(impsoles) As tsoles,Sum(impdolares) As tdolares From mvtos Into Cursor saldos
		*  ELSE
		*   SELECT sum(impsoles) as tsoles,sum(impdolares) as tdolares FROM mvtos INTO CURSOR saldos GROUP BY codc
		*ENDIF
		
	ENDPROC

ENDDEFINE

