	PROCEDURE Generaventas1.grabarventascombustibles
		With This
			.swcreditos=1
			.nauto=0
			If .tdoc='01' Or .tdoc='03' Then
				nidcta1=fe_gene.idctav
				nidcta2=fe_gene.idctai
				nidcta3=fe_gene.idctat
			Else
				nidcta1=0
				nidcta2=0
				nidcta3=0
			Endif
			Select tmpv
			Set Filter To coda<>0
			Go Top
			calma=tmpv.isla
			Set Procedure To capadatos,rngrifo,ple5 Additive
			obj=Createobject("Contometro")
			oturnos=Createobject("turno")
			oturnos.ListarTurnos("lturnos")
			 ogrifero.Asigna(goapp.nidusua,lturnos.turn_inic,lturnos.turn_fina,fe_gene.alma_Sepa,lturnos.turn_turn,goapp.usuario)
			If IniciaTransaccion()=0 Then
				DeshacerCambios()
				Return 0
			Endif
			.nauto=IngresaResumenDcto(.tdoc,.Forma,;
				.ndoc,.fecha,.fecha,.detalle,.valor,;
				.igv,.Impo,.guia,.moneda,.dolar,fe_gene.igv,'k',.codigo,'V',goapp.nidusua,1,goapp.tienda,nidcta1,nidcta2,nidcta3,tmpv.isla,0)
			If .nauto=0 Then
				DeshacerCambios()
				Return 0
			Endif
			If IngresaDatosLCajaEFectivoCturnos(.fecha,"",.razon,nidcta3,.Impo,0,;
					'S',fe_gene.dola,goapp.nidusua,.codigo,.nauto,Left(.Forma,1),.ndoc,.tdoc,goapp.tienda,ogrifero.idturno)=0 Then
				DeshacerCambios()
				Return
			Endif
			If .Forma="C" Or .Forma="D" Then
				.grabacreditos()
			Endif
			If .swcreditos=0 Then
				DeshacerCambios()
				Return 0
			Endif
			na=.nauto
			sw=1
			Do While !Eof()
				cdesc=tmpv.Desc
				calma=tmpv.isla
				If IngresaKardexGrifo(na,tmpv.coda,'V',tmpv.Prec,tmpv.cant,'I','K',.codv,tmpv.isla,tmpv.nidcontometro,0,tmpv.pre1)<1
					sw=0
				Endif
				Select tmpv
				Skip
			Enddo
			If sw=0 Then
				DeshacerCambios()
				Messagebox("El Item:"+Alltrim(cdesc)+" NO Tiene Stock Disponible Para Venta O no se ha fijado El valor del Contometro",16,'Sisven')
				Return 0
			Endif
			If sw=0 Or .swcreditos=0 Then
				DeshacerCambios()
				Return 0
			Endif
			If sw=1 And  .swcreditos=1 Then
				GrabarCambios()
				This.limpiar()
				Return This.nauto
			Endif
		Endwith
		
	ENDPROC

