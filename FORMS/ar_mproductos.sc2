	PROCEDURE limpiar
		With Thisform.frame
			.txtcodigo.Value=0
			.txtdescripcion.Value=""
			.txtprecio.Value=0
		*	.txtunidad.Value=""
		this.frame.txtcodigo1.Value=""
			.txtpeso.Value=0
			.txtcosto.Value=0
			.txtpreciomenor.Value=0
			.txtprecioespecial.Value=0
			.txtpreciomayor.Value=0
			thisform.encontrado=""
			.cmbgrupo.ShowWhatsThis 
		Endwith
		
	ENDPROC

	PROCEDURE modificar
		With Thisform
			cdesc=Rtrim(.frame.txtdescripcion.Value)+' '+Alltrim(.frame.cmbmarca.Value)
			cunid=.frame.txtunidad.Value
			np1=.frame.txtpreciomayor.Value
			np2=.frame.txtprecioespecial.Value
			np3=.frame.txtpreciomenor.Value
			npeso=.frame.txtpeso.Value
			ccat=.frame.txtcodc.Value
			cmar=.frame.txtcodm.Value
			ctipro=Left(.frame.cmbtipo.Value,1)
			nflete=.frame.txtcodigof.Value
			ncoda=.frame.txtcodigo.Value
			cm=Left(.frame.cmbmoneda.Value ,1)
			ce=Left(.frame.cmbestado.Value,1)
			nprecio=.frame.txtprecio.Value
			nidgrupo=.frame.txtcodg.Value
			ncosto=.frame.txtcosto.Value
			nutil1=Iif(.frame.txtutilidad1.Value>0,(.frame.txtutilidad1.Value/100)+1,0)
			nutil2=Iif(.frame.txtutilidad2.Value>0,(.frame.txtutilidad2.Value/100)+1,0)
			nutil3=Iif(.frame.txtutilidad3.Value>0,(.frame.txtutilidad3.Value/100)+1,0)
			ncome=.frame.txtcomisione.Value/100
			ncomc=.frame.txtcomisionc.Value/100
			nidus=goapp.nidusua
			nsmax=.frame.txtstmaximo.Value
			nsmin=.frame.txtstminimo.Value
			nidcosto=.frame.txtidcosto.Value
			ndolar=.frame.txtdolar.Value
		ENDWITH
		IF verificaconexion()=0 then
		RETURN 0
		endif
		If SQLExec(goapp.bdConn,"CALL PROACTUALIZAPRODUCTOS(?cdesc,?cunid,?ncosto,?np1,?np2,?np3,?npeso,?ccat,?cmar,?ctipro,?nflete,?cm,?nprecio,?nidgrupo,?nutil1,?nutil2,?nutil3,?ncome,?ncomc,?nidus,?ncoda,?nsmax,?nsmin,?nidcosto,?ndolar)")  < 1 Then
			errorbd(ERRORPROC)
			Return
		Else
			If ActualizaCostos1(Thisform.frame.txtidcosto.Value,;
					thisform.frame.txtcosto.Value,Thisform.frame.txtflete.Value,Thisform.frame.txtcoston.Value,;
					Left(Thisform.frame.cmbmoneda.Value,1),Thisform.frame.txtdolar.Value)=0 Then
				errorbd(ERRORPROC)
				Return
			Endif
		Endif
		Messagebox("Editado Correctamente",64,MSGTITULO)
		
	ENDPROC

	PROCEDURE preciosvta
		Local nutilidad,nprecio,nd,nt,nflete
		With Thisform.frame
			nt=.cmbmoneda.ListIndex
			nprecio=.txtprecio.Value
			nflete=.txtflete.Value
			.txtcosto.Value=Round(((Iif(nt=2,nprecio*fe_gene.igv*fe_gene.dola,nprecio*fe_gene.igv))+nflete),2)
			.txtcoston.Value=Round(nprecio*fe_gene.igv,4)
			If .txtutilidad3.Value>0 Then
				nutilidad=(.txtutilidad3.Value/100)+1
				.txtpreciomenor.Value=Round(.txtcosto.Value*nutilidad,2)
			Endif
			If .txtutilidad2.Value>0 Then
				nutilidad=(.txtutilidad2.Value/100)+1
				.txtprecioespecial.Value=Round(.txtcosto.Value*nutilidad,2)
			Endif
			If .txtutilidad1.Value>0 Then
				nutilidad=(.txtutilidad1.Value/100)+1
				.txtpreciomayor.Value=Round(.txtcosto.Value*nutilidad,2)
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE refrescar
		With This.frame
			Thisform.vaciarcontroles()
			.txtprecioRef.Value=0
		*	Thisform.jgrupos()
		*	Thisform.jlineas()
		*	Thisform.jmarcas()
		*	Thisform.jfletes()
			.txtcodigo.Value=lpro.idart
			.txtunidad.Value=lpro.unid
			.cmbmoneda.ListIndex=Iif(lpro.tmon="S",1,2)
			ntipo=.cmbmoneda.ListIndex
			nflete=.txtflete.Value
			.txtprecio.Value=lpro.Prec
			.txtcosto.Value=lpro.costo
			.txtcoston.Value=lpro.Prec*fe_gene.igv
			.txtpreciomayor.Value=lpro.pre1
			.txtprecioespecial.Value=lpro.pre2
			.txtpreciomenor.Value=lpro.pre3
			.txtidcosto.Value=Val(lpro.cost_idco)
			.txtpeso.Value=lpro.peso
			.cmbtipo.Listindex=IIF(lpro.tipro='C',1,IIF(lpro.tipro='O',2,3))
			.txtdolar1.Value=lpro.prod_dola
			.txtutilidad1.Value=Iif(lpro.prod_uti1>1,(lpro.prod_uti1*100)-100,0)
			.txtutilidad2.Value=Iif(lpro.prod_uti2>1,(lpro.prod_uti2*100)-100,0)
			.txtutilidad3.Value=Iif(lpro.prod_uti3>1,(lpro.prod_uti3*100)-100,0)
			.txtcomisionc.Value=Iif(lpro.prod_comc>=0,(lpro.prod_comc*100),0)
			.txtcomisione.Value=Iif(lpro.prod_come>=0,(lpro.prod_come*100),0)
			.txtstminimo.Value=lpro.prod_smin
			.txtstmaximo.Value=lpro.prod_smax
			*npos=At(",",lpro.Descri)
			.cmbalmacen.ListIndex=lpro.prod_stoc
			*.txtdescripcion.Value=Substr(lpro.Descri,1,npos-1)
			.txtdescripcion.Value=lpro.Descri
		*cmarca=Alltrim(.cmbmarca.Value)
		*.txtdescripcion.Value=Substr(Alltrim(lpro.Descri),1,Len(Alltrim(lpro.Descri))-Len(cmarca))
			.cmbestado.ListIndex=1
			cpr=lpro.ulpc
			nidusc=lpro.prod_idus
			nidusa=lpro.prod_uact
			.txtstockuno.Value=lpro.uno
			.txtstockdos.Value=lpro.Dos
			.txtpentregar.Value=lpro.tre
			.txtcontable.Value=lpro.cua
			.txtcodigo1.Value=lpro.coda1
			If MUESTRAPROVEEDOResx('',2,cpr,"pprov")<1 Then
				Return
			ENDIF
			IF verificaconexion()=0 then
		RETURN 0
		endif
			If SQLExec(goapp.bdconn,"CALL PROMUESTRAUSUARIOS('',1,?nidusc)","usucr")<1 Then
				errorbd(ERRORPROC)
				Return
			Endif
			If SQLExec(goapp.bdconn,"CALL PROMUESTRAUSUARIOS('',1,?nidusA)","usuac")<1 Then
				errorbd(ERRORPROC)
				Return
			Endif
			nidart=.txtcodigo.Value
			TEXT TO lc NOSHOW
		     	SELECT ifnull(a.prec*z.igv,0) as prec,b.fech,b.mone FROM fe_kar as a INNER JOIN
		        fe_rcom as b ON b.idauto=a.idauto inner join fe_prov as x on x.idprov=b.idprov, fe_gene as z WHERE a.idart=?nidart
		        AND a.acti='A' AND b.acti='A' and a.tipo='C' order by b.fech desc
			ENDTEXT
			IF verificaconexion()=0 then
		RETURN 0
		endif
			If SQLExec(goapp.bdconn,lc,'upr')<0 Then
				errorbd(ERRORPROC)
				Return
			Endif
			Go Top In upr
			.txtMoneda.Value=upr.mone
			.txtprecioRef.Value=upr.Prec
			.txtusuario.Value=usucr.nomb
			.txtusuario1.Value=usuac.nomb
			.txtfechac.Value=lpro.fechc
			.txtfecham.Value=lpro.prod_fact
			.txtproveedor.Value=pprov.razo
			.txtfechacompra.Value=lpro.ulfc
		Endwith
		
	ENDPROC

