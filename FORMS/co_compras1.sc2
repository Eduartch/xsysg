	PROCEDURE registrarcuentasporpagar
		Do Case
			Case Thisform.cmbforma.ListIndex=2 And Thisform.tdoc<>'07'
				If Thisform.ingresadeudas()=0
					Return 0
				Else
					Return 1
				Endif
			Case Thisform.cmbforma.ListIndex=2 And Thisform.tdoc='07'
				swx=1
				Select tmp
				Scan For tmp.pagos>0
					nidpx=CancelaDeudasConNotasCredito(Thisform.txtfecha.Value,.txtfecha.Value,tmp.pagos,;
					Thisform.TXTSErie.Value+Thisform.TXTNUmero.Value,'P',Left(Thisform.cmbmoneda.Value,1),;
						tmp.xdeta,tmp.tipo,tmp.idrd,goapp.nidusua,tmp.ncontrol,'',Id(),Thisform.txtdolar.Value,Thisform.nauto)
					If nidpx=0 Then
						swx=0
						Exit
					Endif
				Endscan
				If swx=0 Then
					Return 0
				Endif
				swx=1
				Select idauto From tmp Where pagos>0 Into Cursor xt Group By idauto
				Select xt
				Scan All
					If IngresarNotasCreditoCompras(Thisform.nauto,xt.idauto,nidpx)=0 Then
						swx=0
						Exit
					Endif
				Endscan
				If swx=0 Then
					Return 0
				Else
					Return 1
				Endif
		Endcase
		
		
	ENDPROC

