		*
	ENDPROC

	PROCEDURE txtfecha.LostFocus
		If !this.ReadOnly 
		   *this.ReadOnly=.t.
		EndIf 
	ENDPROC

	PROCEDURE txtnumero.LostFocus
		If !Empty(This.Value) Then
			If xt.tipo<>'L' Then
				If Left(This.Value,1)<>'.'  Then
					If siestaregistradodctopago(This.Value)=0 Then
						Messagebox("Nº de Documento Ya Registrado "+This.Value,16,MSGTITULO)
						This.Value=""
					Endif
				Endif
			Endif
		Endif
		
	ENDPROC

	PROCEDURE txtpago.LostFocus
		if this.value>0 and !empty(thisform.txtnumero.value) 
		   thisform.cmdaceptar.enabled=.t.
		endif   
	ENDPROC

