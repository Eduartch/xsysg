						TEXT TO lc NOSHOW
		               SELECT b.nomv,b.idven,b.razo,b.fono,b.idclie,b.idauto,b.mone,b.tipo,b.banc,b.importe,b.ncontrol,b.ndoc,b.fech,b.fevto,b.codt,b.impoo,nlet,idcred,nruc,ndni
